	shorttitle = {Un mod{\`e}le associatif de r{\'e}gulation du travail ?},
	url = {http://journals.openedition.org/pmp/1860},
	doi = {10.4000/pmp.1860},
	abstract = {Dans les services {\`a} domicile, dont les salari{\'e}s sont presque uniquement des femmes, la question de la sant{\'e} au travail n{\textquoteright}a gu{\`e}re {\'e}t{\'e} pos{\'e}e jusqu'{\`a} ces derni{\`e}res ann{\'e}es. Apparemment semblables {\`a} celles que la plupart des femmes effectuent chaque jour dans leur propre maison, les t{\^a}ches effectu{\'e}es sont largement invisibles et leur banalit{\'e} les fait consid{\'e}rer comme inoffensives. Toutefois, plusieurs {\'e}tudes {\'e}pid{\'e}miologiques r{\'e}centes soulignent l{\textquoteright}importance des risques psychosociaux encourus et l{\textquoteright}incidence {\'e}lev{\'e}e des troubles musculo- squelettiques et psychologiques. Cet article pr{\'e}sente les r{\'e}sultats d{\textquoteright}une enqu{\^e}te de terrain men{\'e}e aupr{\`e}s de deux grandes associations de services {\`a} domicile de l'Ouest de la France montrant les liens entre les modes d'organisation du travail et la sant{\'e} des salari{\'e}es. L{\textquoteright}ensemble des dispositifs permettant une n{\'e}gociation de la d{\'e}finition du service offre {\'e}galement des espaces de discussion sur le travail, susceptibles de fournir un cadre protecteur de la sant{\'e} des employ{\'e}s. Ce mode de gouvernance du service, d{\textquoteright}origine associative, peut ainsi {\^e}tre consid{\'e}r{\'e} comme un mod{\`e}le sp{\'e}cifique de r{\'e}gulation du travail dans l{\textquoteright}espace priv{\'e}, favorable {\`a} la sant{\'e} des salari{\'e}es.},
	language = {fr},
	number = {Vol. 27/1},
	urldate = {2020-06-26},
	journal = {Politiques et Management Public},
	author = {Dussuet, Annie},
	month = jan,
	year = {2010},
	pages = {79--96},
	annote = {Organisation du travail, sant{\'e} au travail, services {\`a} domicile, {\'e}conomie sociale, gouvernance m{\'e}diatrice.},
}

@article{gilbert_levaluation_2010,
	title = {L{\textquoteright}{\'e}valuation de la performance, analyseur de la gouvernance associative},
	issn = {0758-1726, 2119-4831},
