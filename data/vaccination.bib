	shorttitle = {L'espace comme instrument de l'innovation publique},
	doi = {10.3166/pmp.34.2017.0013},
	abstract = {La mise en place d{\textquoteright}open labs dans des institutions publiques marque une rencontre para- doxale entre, d{\textquoteright}un c{\^o}t{\'e}, un espace hybride et souple, et de l{\textquoteright}autre, une bureaucratie orga- nisationnelle. Cette rencontre tend {\`a} questionner une s{\'e}rie de fronti{\`e}res entre : i) la sph{\`e}re publique et marchande, ii) l{\textquoteright}usager et le concepteur du service, iii) la prestation d{\textquoteright}un service public et sa cr{\'e}ation. La construction de tels espaces physiques et virtuels interroge les ca- pacit{\'e}s d{\textquoteright}innovation des organisations publiques au prisme des niveaux d{\textquoteright}implication des usagers (co-conception/contribution) et du degr{\'e} d'ouverture de l'espace (ouvert/ferm{\'e}). {\`A} partir d{\textquoteright}une {\'e}tude de cas multiples dans le secteur de la sant{\'e} et de la culture, nous avons construit une typologie d{\textquoteright}open labs mettant en {\'e}vidence diff{\'e}rentes sources de transforma- tion des organisations publiques o{\`u} sont bouscul{\'e}s les pratiques et r{\^o}les de ses membres, en permettant l{\textquoteright}hybridation des projets et en ouvrant les fronti{\`e}res des organisations en interne et en externe.},
	journal = {Politiques et management public},
	author = {Aubouin, Nicolas and Le Chaffotec, Alexandra},
	month = dec,
	year = {2017},
	pages = {207--229},
	annote = {espaces, usagers, open labs, secteurs de la sant{\'e} et de la culture, instrument d'innovation publique.},
}

