@article{gibert_peut-discipliner_2016,
	title = {Peut-on discipliner le pouvoir ? {\'E}tude sur le contr{\^o}le de la rationalit{\'e} manag{\'e}riale par le juge dans trois innovations l{\'e}gislatives fran{\c c}aises},
	volume = {33},
	issn = {0758-1726},
	shorttitle = {Peut-on discipliner le pouvoir ?},
	url = {https://pmp.revuesonline.com/article.jsp?articleId=37746},
	doi = {10.3166/pmp.33.3-4.165-196},
	abstract = {Il y a un paradoxe {\`a} constater que des normes se proposant d{\textquoteright}introduire une forme de ra- tionalit{\'e} manag{\'e}riale dans la gestion publique sont port{\'e}es par des textes juridiques de niveau tr{\`e}s {\'e}lev{\'e} (en France Loi et loi organique), alors que les analystes mettent fr{\'e}quem- ment en relief les contradictions entre cette rationalit{\'e} manag{\'e}riale et la logique politique, corollaire de la lutte pour le pouvoir et de sa conservation. L{\textquoteright}{\'e}mergence de la rationalit{\'e} manag{\'e}riale affaiblirait-elle la domination de la logique poli- tique, lui disputant m{\^e}me sa primaut{\'e} ? Peut-on r{\'e}ellement discipliner le politique en don- nant force obligatoire aux bonnes pratiques de management contenues dans la Loi ? Cet article {\'e}value la question de la discipline du politique en {\'e}tudiant la mise en {\oe}uvre de trois textes aux ambitions manag{\'e}riales claires : la loi organique relative aux lois de finances (LOLF) de 2001 instituant l{\textquoteright}obligation, pour les pouvoirs publics, de pr{\'e}senter des projets et des rapports annuels de performance {\`a} l{\textquoteright}appui de chacun de leurs programmes, la loi organique du 15 avril 2009 rendant obligatoires les {\'e}tudes d{\textquoteright}impact pour les projets de Loi d{\'e}pos{\'e}s par le Gouvernement, et enfin l{\textquoteright}ordonnance du 23 juillet 2015 imposant {\`a} l{\textquoteright}{\'E}tat et aux collectivit{\'e}s territoriales l{\textquoteright}{\'e}valuation du mode de r{\'e}alisation choisi et les {\'e}tudes pr{\'e}a- lables associ{\'e}es aux march{\'e}s de partenariat. Les jurisprudences constitutionnelle et administrative, relatives aux trois textes {\'e}tudi{\'e}s, r{\'e}v{\`e}lent une prudence extr{\^e}me du juge pour faire respecter l{\textquoteright}ambition du l{\'e}gislateur de discipliner le politique du point de vue manag{\'e}rial. L{\textquoteright}article sugg{\`e}re plusieurs explica- tions {\`a} ce comportement qui, pour n{\textquoteright}{\^e}tre pas nouveau, confirme une jurisprudence tra- ditionnelle visant plus {\`a} censurer l{\textquoteright}irrationalit{\'e} manifeste qu{\textquoteright}{\`a} imposer une forme pr{\'e}cise de rationalit{\'e}.},
	number = {3-4},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Gibert, Patrick and Verrier, Pierre-{\'E}ric},
	month = dec,
	year = {2016},
	pages = {165--196},
	annote = {gouvernance, LOLF, {\'e}tudes d{\textquoteright}impact, march{\'e}s de partenariat, discipline manag{\'e}riale, contr{\^o}le, rationalit{\'e} manag{\'e}riale, analyse d{\textquoteright}impact de la r{\'e}glementation.},
}

