	abstract = {Cette communication examine comment se pose le probl{\`e}me de la responsabilit{\'e} des cadres dans les collectivit{\'e}s territoriales. Dans cette perspective, un examen du lien entre autorit{\'e}, responsabilit{\'e} et responsabilitation permet de positionner la dimension manag{\'e}hale de la responsabilit{\'e} publique. Au terme de l'analyse, il appara{\^i}t que le syst{\`e}me d'autorit{\'e} et de responsabilit{\'e} de l'encadrement dans les collectivit{\'e}s locales fonctionne {\`a} double d{\'e}tente. A une responsabilit{\'e} croissante, parall{\`e}le au renforcement du pouvoir de d{\'e}cision des cadres sup{\'e}rieurs, se conjugue une volont{\'e} de responsabiliser l'encadrement interm{\'e}diaire par des pratiques plus manag{\'e}hales et n'effa{\c c}ant pas la responsabilit{\'e} individuelle derri{\`e}re les r{\`e}gles.},
	language = {fr},
	number = {3},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Desmarais, C{\'e}line and Jameux, Claude},
	year = {2001},
	pages = {101--119},
}

