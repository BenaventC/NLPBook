	doi = {10.3406/pomap.1995.3392},
	abstract = {Le d{\'e}but des ann{\'e}es 80 est marqu{\'e} par une surench{\`e}re des Etats f{\'e}d{\'e}r{\'e}s am{\'e}ricains en mati{\`e}re de politiques de recrutement industriel (marketing territorial) et la multiplication d'instruments d'attraction de nouvelles entreprises (abattements fiscaux, aides directes). Plus r{\'e}cemment, on constate le d{\'e}veloppement d'une cat{\'e}gorie tr{\`e}s h{\'e}t{\'e}rog{\`e}ne d'instruments de politique industrielle, qui vont des aides aux PME, au d{\'e}veloppement technologique et {\`a} l'exportation, {\`a} des mesures plus informelles et immat{\'e}rielles. Cependant, la tr{\`e}s grande majorit{\'e} des ressources est encore consacr{\'e}e aux mesures traditionnelles. Le d{\'e}veloppement de nouveaux instruments correspond davantage {\`a} une s{\'e}dimentation de politiques industrielles : La diversification des instruments ne fait qu'ajouter de nouvelles politiques, sans pour autant {\'e}vincer ou remplacer les politiques traditionnelles.},
	language = {fr},
	number = {2},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Burmeister, Antje},
	year = {1995},
	pages = {27--45},
}

@article{girard-zdanowska_evaluation_1995,
