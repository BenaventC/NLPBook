	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1994_num_12_2_3157},
	doi = {10.3406/pomap.1994.3157},
	abstract = {Manager recherche et innovation appara{\^i}t impossible dans les termes habituels du management quantitatif et du contr{\^o}le de gestion. Et pourtant, il est indispensable de bien manager des investissements d{\'e}sormais consid{\'e}rables et dont la r{\'e}ussite est d{\'e}cisive pour le succ{\`e}s des entreprises. Il faut comprendre qu'avec la contribution la plus immat{\'e}rielle de l'esprit humain ce sont les hommes et leurs relations qui sont en question et non plus les choses. C'est par une meilleure compr{\'e}hension qualitative des ph{\'e}nom{\`e}nes humains, de la gestion des interfaces, du d{\'e}veloppement des personnes plus que de la gestion des carri{\`e}res que l'on peut cr{\'e}er les conditions du succ{\`e}s de l'invention et de la diffusion des innovations.},
	language = {fr},
	number = {2},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Crozier, Michel},
	year = {1994},
	pages = {11--16},
}

@article{luchaire_levaluation_1994,
