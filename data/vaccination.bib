@article{bayle_gouvernance_2010,
	title = {La gouvernance des f{\'e}d{\'e}rations d{\textquoteright}associations charg{\'e}es d{\textquoteright}une mission de service public : le cas des f{\'e}d{\'e}rations sportives fran{\c c}aises},
