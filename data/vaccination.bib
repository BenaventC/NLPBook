	volume = {28},
	shorttitle = {Politiques familiales municipales au {Qu{\'e}bec}},
	doi = {10.3166/pmp.28.467-507},
	abstract = {Les politiques familiales municipales (PFM) sont relativement r{\'e}centes au Qu{\'e}bec et connaissent, en d{\'e}pit des nombreux d{\'e}fis restant {\`a} surmonter, un succ{\`e}s croissant au sein des municipalit{\'e}s et MRC (Municipalit{\'e}s r{\'e}gionales de comt{\'e}). Con{\c c}ues par le gou- vernement comme des politiques transversales adapt{\'e}es {\`a} l{\textquoteright}{\'e}chelon local et r{\'e}gional, les PFM contribuent {\`a} {\'e}largir le champ d{\textquoteright}action des municipalit{\'e}s, les amenant souvent {\`a} un {\guillemotleft} re {\guillemotright}-positionnement par rapport {\`a} leurs responsabilit{\'e}s (envers leurs citoyens). Cet article pr{\'e}sente une analyse de ces politiques en lien avec le d{\'e}veloppement lo- cal et les strat{\'e}gies municipales. Politique transversale, {\guillemotleft} catalyseur {\guillemotright}, tremplin pour de nombreuses actions, nous verrons cependant que les PFM ne sauraient constituer une solution d{\'e}finitive face aux enjeux de transfert des comp{\'e}tences de l{\textquoteright}{\'E}tat et de d{\'e}veloppement. Par contre, elles constituent une initiative fort originale, qui n{\textquoteright}existe pas comme telle ailleurs et peut inspirer d{\textquoteright}autres municipalit{\'e}s, d{\textquoteright}autres pays, car cela permet de compl{\'e}ter l{\textquoteright}action nationale en mati{\`e}re familiale et {\'e}largit la gouvernance de la question familiale.},
	journal = {Politiques et management public},
	author = {Rochman, Juliette and Tremblay, Diane-Gabrielle},
	month = dec,
	year = {2011},
	pages = {467--507},
	annote = {Politiques familiales, comp{\'e}tences municipales, d{\'e}veloppement local, municipalit{\'e}s},
}

@article{marina_territorialisation_2011,
