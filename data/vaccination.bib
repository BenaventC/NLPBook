	issn = {0758-1726, 2119-4831},
	shorttitle = {Les obstacles {\`a} l{\textquoteright}apprentissage organisationnel au sein de la s{\'e}curit{\'e} civile},
	url = {http://journals.openedition.org/pmp/1438},
	doi = {10.4000/pmp.1438},
	abstract = {L{\textquoteright}objet de cette contribution est d{\textquoteright}identifier un certain nombre de facteurs {\guillemotleft} humains {\guillemotright} qui font obstacle {\`a} l{\textquoteright}apprentissage organisationnel au sein de la s{\'e}curit{\'e} civile. Notre int{\'e}r{\^e}t se porte sur la premi{\`e}re {\'e}tape du processus d{\textquoteright}apprentissage {\`a} savoir la perception d{\textquoteright}une erreur par au moins un acteur en situation (Argyris, Sch{\"o}n, Senge). A partir de l{\textquoteright}observation in situ d{\textquoteright}une op{\'e}ration d{\textquoteright}un Service Secours Incendie suite {\`a} un d{\'e}part de feu dans le Sud de la France, nous avons pu identifier quatre registres qui peuvent constituer autant d{\textquoteright}obstacles {\`a} l{\textquoteright}apprentissage organisationnel. Le premier est relatif au d{\'e}calage entre l{\textquoteright}activit{\'e} exerc{\'e}e par un acteur et son projet de vie, le second s{\textquoteright}int{\'e}resse au d{\'e}calage entre la nature du projet et le style de l{\textquoteright}acteur, le troisi{\`e}me se situe en rapport avec les {\'e}carts en terme de savoir- faire entre ce que n{\'e}cessite la situation et ce que poss{\`e}de l{\textquoteright}acteur en situation et le quatri{\`e}me est relatif au contexte collectif non propre {\`a} permettre l{\textquoteright}{\'e}change et le dialogue vis-{\`a}-vis de la situation. En conclusion nous proposerons quelques pistes en mati{\`e}re de gestion des ressources humaines pour tenter de pallier ces obstacles.},
	language = {fr},
	number = {Vol 26/2},
	urldate = {2020-06-26},
	journal = {Politiques et Management Public},
	author = {Gautier, Ana{\"i}s and Li{\`e}vre, Pascal and Rix-Li{\`e}vre, G{\'e}raldine},
	month = jun,
	year = {2008},
	pages = {137--168},
	annote = {Gestion des risques - Perception de l'erreur - Apprentissage organisationnel - Organisation apprenante - Services d'incendie et de secours},
}

@article{chemla-lafay_professionnaliser_2008,
	title = {Professionnaliser la {GRH} dans la fonction publique de l{\textquoteright}{\'E}tat en {France}, une incantation sans effets r{\'e}els},
	issn = {0758-1726, 2119-4831},
	url = {http://journals.openedition.org/pmp/1432},
