	shorttitle = {L'analyse des co{\^u}ts dans les organisations publiques},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1984_num_2_1_1766},
	doi = {10.3406/pomap.1984.1766},
	abstract = {Si l'on part d'une d{\'e}finition des co{\^u}ts {\`a} la fois technique et t{\'e}l{\'e}ologique, on constate que les exigences techniques du calcul des co{\^u}ts sont mal satisfaites dans les organisations publiques et que l'on peut m{\^e}me avoir des doutes sur les finalit{\'e}s suppos{\'e}e de l'analyse. Lorsqu'on passe en revue les objectifs traditionnellement assign{\'e}s {\`a} cette analyse, leur peu de pertinence pratique pour nombre d'organisations publiques frappe et force est de constater que l'analyse des co{\^u}ts remplit plut{\^o}t dans celles-ci des fonctions socio-organisationnelles qu'elle ne sert {\`a} des objectifs pr{\'e}cis de rationalit{\'e} {\'e}conomique. Revenant au probl{\`e}me technique, on s'aper{\c c}oit qu'il ne pr{\'e}sente de difficult{\'e}s insurmontables ni au niveau de la saisie de l'information, ni {\`a} celui de son traitement. Au total, les interrogations soulev{\'e}es par l'analyse des co{\^u}ts dans les organisations publiques sont de nature {\`a} am{\'e}liorer la probl{\'e}matique de celle-ci dans tout type d'organisation.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Burlaud, Alain and Gibert, Patrick},
	year = {1984},
	pages = {93--117},
}

