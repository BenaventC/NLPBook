@article{gibert_controler_1984,
	title = {Contr{\^o}ler la gestion ou {\'e}valuer les politiques ?},
	volume = {2},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1984_num_2_2_1782},
	doi = {10.3406/pomap.1984.1782},
	abstract = {Le contr{\^o}le de gestion des organisations publiques est une discipline qui se rattache incontestablement {\`a} l'approche organisationnelle du management public. Les d{\'e}finitions que l'on peut en donner : {\guillemotleft}processus par lequel la direction s'assure que l'organisation met en oeuvre ses strat{\'e}gies de fa{\c c}on efficace et efficiente{\guillemotright} [2, p. 3], {\guillemotleft}ensemble des proc{\'e}dures et des {\'e}tats qui permettent aux dirigeants d'une organisation de ma{\^i}triser dans la mesure du possible le fonctionnement et le d{\'e}veloppement d'une organisation{\guillemotright} [2, p. 36], l'insistance mise sur le contr{\^o}le du syst{\`e}me de contr{\^o}le comme instrument de pilotage de chacun des services de relation entre ces services ou entre niveaux hi{\'e}rarchiques, la nature des probl{\`e}mes favoris des contr{\^o}leurs de gestion comme celui de leur rattachement dans l'organigramme en t{\'e}moignent. Davantage encore, la simple constatation que les syst{\`e}mes de contr{\^o}le de gestion mis en place dans les organisations publiques l'ont {\'e}t{\'e} par imitation puis transposition de ceux existant dans les entreprises, c'est-{\`a}-dire par emprunt {\`a} un des {\'e}l{\'e}ments du management priv{\'e}, est de nature {\`a} nous convaincre de l'appartenance du contr{\^o}le {\`a} l'approche organisationnelle.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Gibert, Patrick and Andrault, Marianne},
	year = {1984},
	pages = {123--133},
}

@article{caille-talon_bilan_1984,
