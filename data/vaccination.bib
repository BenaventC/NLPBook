	abstract = {Le secteur de la sant{\'e} mentale conna{\^i}t un certain nombre de difficult{\'e}s et dysfonction- nements (Laforcade, 2016) qui paraissent souvent li{\'e}s {\`a} des facteurs entrem{\^e}l{\'e}s d{\textquoteright}ordre organisationnel, institutionnel et culturel. Diverses analyses scientifiques et op{\'e}ration- nelles, au plan international, incitent {\`a} explorer de nouveaux mod{\`e}les de prise en charge, permettant une approche plus globale et mieux coordonn{\'e}e, s{\textquoteright}appuyant notamment sur l{\textquoteright}imbrication des soins et des services sociaux et m{\'e}dico-sociaux. Cet article examine, {\`a} partir d{\textquoteright}un cadre th{\'e}orique interdisciplinaire (sant{\'e} publique, psychiatrie, management et organisation) et d{\textquoteright}une {\'e}tude de cas approfondie, en quoi la mise en {\oe}uvre d{\textquoteright}un dispo- sitif structurel et manag{\'e}rial innovant et s{\textquoteright}effor{\c c}ant de r{\'e}pondre {\`a} des besoins identifi{\'e}s, peut se heurter {\`a} des obstacles tels que des formes de conservatisme soci{\'e}tal et diverses manifestations de r{\'e}sistance au changement. Ainsi, cette recherche exploratoire met en avant d{\textquoteright}une part la faisabilit{\'e} et l{\textquoteright}opportunit{\'e} de nouveaux mod{\`e}les de prise en charge du patient en sant{\'e} mentale, et {\'e}galement les d{\'e}fis {\`a} surmonter et les conditions de r{\'e}ussite de ce type d{\textquoteright}{\'e}volution.},
	language = {fr},
	urldate = {2020-06-29},
	author = {Bartoli, Annie and Gozlan, Guy and Sebai, Jihane},
	annote = {Sant{\'e} mentale, Prise en charge, Cas complexes, Empowerment, Case Management, Coordination, Int{\'e}gration, Changement organisationnel, Pilotage du changement.},
}

@article{tiberghien_resilience_2019,
	title = {La r{\'e}silience des forces de l{\textquoteright}ordre en contexte post-{\'e}tat d{\textquoteright}urgence : entre logiques de fiabilit{\'e} organisationnelle et de coproduction de service},
	volume = {36},
	shorttitle = {La r{\'e}silience des forces de l{\textquoteright}ordre en contexte post-{\'e}tat d{\textquoteright}urgence},
	doi = {10.3166/pmp.36.2019.00014},
	abstract = {Suite aux vagues d{\textquoteright}attentats de 2015 en France et depuis, dans un contexte post-{\'e}tat d{\textquoteright}urgence, nous avons proc{\'e}d{\'e} {\`a} des entretiens aupr{\`e}s d{\textquoteright}acteurs de la BRI (Brigade de Re- cherche et d{\textquoteright}Intervention, commun{\'e}ment appel{\'e}e brigade antigang), du RAID (Recherche, Assistance, Intervention, Dissuasion) et de la PM (Police Municipale) sur la mani{\`e}re dont ils conduisent leurs activit{\'e}s quotidiennes. Plus pr{\'e}cis{\'e}ment, nous les avons interrog{\'e}s sur la perception qu{\textquoteright}ils avaient des sources de r{\'e}silience qu{\textquoteright}ils mobilisaient en interne, dans le cadre de logiques de fiabilit{\'e} organisationnelle. Reprenant {\`a} titre illustratif les principaux {\'e}l{\'e}ments de r{\'e}ponse fournis par ces forces de l{\textquoteright}ordre, nous questionnons plus fondamen- talement dans cet article le sch{\'e}ma de politique publique original qui se d{\'e}gage pour ces organisations en prise avec leurs logiques internes de fiabilit{\'e} organisationnelle et la n{\'e}ces- sit{\'e} de d{\'e}velopper des processus de coproduction de service. Nous interrogeons en cela la port{\'e}e d{\textquoteright}une association de ces deux corpus th{\'e}oriques et concluons par la proposition d{\textquoteright}un cadre int{\'e}grateur de ces deux logiques.},
	number = {2},
	journal = {Politiques et Management Public},
	author = {Tiberghien, Bruno and Bout Vallot, Ludivine},
	year = {2019},
	pages = {223--243},
	annote = {R{\'e}silience, terrorisme, organisations {\`a} haute fiabilit{\'e}, forces de l{\textquoteright}ordre, coproduction.},
}

@book{broussolle_quel_nodate,
	title = {Quel syst{\`e}me incitatif r{\'e}aliste pour la politique de r{\'e}duction des d{\'e}chets m{\'e}nagers ? {Enseignements} tir{\'e}s de la litt{\'e}rature {\'e}conomique et du cas fran{\c c}ais - {ProQuest}},
	shorttitle = {Quel syst{\`e}me incitatif r{\'e}aliste pour la politique de r{\'e}duction des d{\'e}chets m{\'e}nagers ?},
	url = {https://search.proquest.com/openview/919efc580c99482daca791100153580b/1?pq-origsite=gscholar&cbl=2069451},
	abstract = {La r{\'e}duction du volume des d{\'e}chets m{\'e}nagers est un objectif majeur des politiques municipales dans le monde. La tarification du service de collecte {\'e}tant g{\'e}n{\'e}ralement inefficace, la litt{\'e}rature {\'e}conomique promeut un syst{\`e}me incitatif qui, le plus souvent, consiste {\`a} faire payer le service {\`a} la quantit{\'e} mise en poubelle. En se basant sur des {\'e}tudes empiriques, sur l{\textquoteright}avanc{\'e}e de la r{\'e}flexion {\'e}conomique et sur l{\textquoteright}exp{\'e}rience fran- {\c c}aise, l{\textquoteright}article explique pourquoi cette voie ne peut conna{\^i}tre qu{\textquoteright}une extension limi- t{\'e}e. D{\textquoteright}un c{\^o}t{\'e}, sa mise en {\oe}uvre rencontre des difficult{\'e}s pratiques et convient essen- tiellement {\`a} l{\textquoteright}habitat individuel. De l{\textquoteright}autre, l{\textquoteright}analyse des comportements des agents, que leur motivation soit utilitariste ou orient{\'e}e par des valeurs, montre {\'e}galement les inconv{\'e}nients qu{\textquoteright}une extension trop forte du syst{\`e}me peut produire. Au total, il est souhaitable que la part incitative du tarif reste modeste. Cela peut sembler insuffisant et contradictoire avec le principe m{\^e}me de la tarification incitative, l{\textquoteright}article souligne que ce n{\textquoteright}est pas le cas. Pour {\^e}tre plus efficace, une strat{\'e}gie incitative concernant les m{\'e}nages devrait donc aussi passer par d{\textquoteright}autres m{\'e}canismes mobilisateurs. {\`A} ce niveau, le syst{\`e}me de la consigne para{\^i}t un bon candidat, car il vise les m{\^e}mes objectifs et {\'e}vite les faiblesses de la tarification incitative.},
	language = {fr},
	urldate = {2020-06-29},
	author = {Broussolle, Damien},
	annote = {tarification des d{\'e}chets m{\'e}nagers, tarification incitative, syst{\`e}me de la consigne.},
}

