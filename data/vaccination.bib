	month = mar,
	year = {2014},
	pages = {51--68},
	annote = {{\'e}valuation, causalit{\'e}, {\'e}pist{\'e}mologie r{\'e}aliste, th{\'e}orie du changement.},
}

@article{okbani_rolevaluation_2014,
