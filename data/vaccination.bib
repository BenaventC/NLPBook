}

@article{bourgault_problemes_1985,
	title = {Probl{\`e}mes li{\'e}s {\`a} la nomination et {\`a} l'{\'e}valuation des pr{\'e}sidents de soci{\'e}t{\'e}s d'{Etat} au {Qu{\'e}bec}},
