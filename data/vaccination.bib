@article{drucker-godard_etre_2013,
	title = {{\^E}tre enseignant-chercheur aujourd'hui : discussion autour du plafonnement},
	volume = {30},
	issn = {0758-1726},
	shorttitle = {{\^E}tre enseignant-chercheur aujourd'hui},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=18276},
	doi = {10.3166/pmp.30.5-27},
	abstract = {Cet article s{\textquoteright}int{\'e}resse aux universit{\'e}s fran{\c c}aises et aborde le m{\'e}tier d{\textquoteright}enseignant- chercheur dans l{\textquoteright}enseignement sup{\'e}rieur, {\`a} travers le concept de plafonnement de carri{\`e}re. Ainsi, il montre {\`a} l{\textquoteright}appui d{\textquoteright}une {\'e}tude empirique quantitative aupr{\`e}s de 401 enseignants-chercheurs issus des sections CNU 01 {\`a} 06 que ces derniers s{\textquoteright}estiment majoritairement plafonn{\'e}s en termes structurels, bien qu{\textquoteright}ils soient peu plafonn{\'e}s en termes de contenu. Dit autrement, ils aiment leur m{\'e}tier et ne s{\textquoteright}ennuient pas, mais ont peu d{\textquoteright}esp{\'e}rance de progression de carri{\`e}re. Cela a des cons{\'e}quences sur leur impli- cation organisationnelle et professionnelle, ainsi que sur leur satisfaction au travail et leur intention de quitter l{\textquoteright}organisation. Notamment, cet article permet de mettre en lumi{\`e}re l{\textquoteright}existence de diff{\'e}rents profils d{\textquoteright}enseignants-chercheurs, en fonction de leur degr{\'e} de plafonnement. Il montre {\'e}galement que plus les enseignants-chercheurs sont plafonn{\'e}s, moins ils sont satisfaits de leur travail et plus ils ont l{\textquoteright}intention de quit- ter l{\textquoteright}institution {\`a} laquelle ils appartiennent. Les liens entre plafonnement et implica- tion organisationnelle et professionnelle sont quant {\`a} eux plus complexes {\`a} analyser dans le cadre des sections CNU {\'e}tudi{\'e}es.},
	number = {1},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Drucker-Godard, Carole and Fouque, Thierry and Gallety, Mathilde and Le Flanchec, Alice},
	month = mar,
	year = {2013},
	pages = {5--27},
	annote = {plafonnement de carri{\`e}re, implication, satisfaction, enseignants-chercheurs.},
}

