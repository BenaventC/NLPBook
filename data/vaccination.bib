}

@article{saussois_theories_2008,
	title = {Th{\'e}ories des organisations et management public : quel rapport ?},
	issn = {0758-1726, 2119-4831},
	shorttitle = {Th{\'e}ories des organisations et management public},
	url = {http://journals.openedition.org/pmp/1523},
	doi = {10.4000/pmp.1523},
	abstract = {L{\textquoteright}article se propose d{\textquoteright}{\'e}valuer la contribution des th{\'e}ories des organisations dans la constitution du management public comme champ disciplinaire sp{\'e}cifique. Quatre apports sont discut{\'e}s ; tout d{\textquoteright}abord un retour aux source rappelle le travail pionnier de Simon qui a utilis{\'e} son exp{\'e}rience comme ing{\'e}nieur en organisation dans l{\textquoteright}agence am{\'e}ricaine charg{\'e} de mettre en place le discours du G{\'e}n{\'e}ral Marshall pour {\'e}laborer le premier cadre th{\'e}orique, ensuite le deuxi{\`e}me apport sera la fa{\c c}on dont le concept de contingence a {\'e}t{\'e} utilis{\'e} ensuite le troisi{\`e}me apport vient des travaux portant sur l{\textquoteright}apprentissage organisationnel enfin le quatri{\`e}me apport porte sur l{\textquoteright}industrialisation des proc{\'e}dures administratives. Le but de cet article est d{\textquoteright}{\'e}valuer l{\textquoteright}apport des th{\'e}ories des organisations dans ce qu{\textquoteright}il est convenu d{\textquoteright}appeler, avec la pr{\'e}caution dont les diff{\'e}rents articles de ce num{\'e}ro t{\'e}moignent, le management public, objet furtif s{\textquoteright}il en est. L{\textquoteright}espace limit{\'e} r{\'e}serv{\'e} {\`a} cette {\'e}valuation oblige {\`a} forcer le trait. Des traits au fusain plut{\^o}t que des traits au crayon h{\'e}sitant et effa{\c c}able. Quatre remarques pour tenter d{\textquoteright}{\'e}valuer cet apport.},
	language = {fr},
	number = {Vol. 26/3},
	urldate = {2020-06-26},
	journal = {Politiques et Management Public},
	author = {Saussois, Jean-Michel},
	month = oct,
	year = {2008},
	pages = {101--109},
}

@article{chevallier_management_2008,
	title = {Management public et {Droit}},
	issn = {0758-1726, 2119-4831},
	url = {http://journals.openedition.org/pmp/1520},
	doi = {10.4000/pmp.1520},
	abstract = {Le management public ne s{\textquoteright}est pas d{\'e}velopp{\'e} en France en marge du droit : il a agi sur l{\textquoteright}ordre juridique lui-m{\^e}me, d{\textquoteright}une part en contribuant {\`a} faire {\'e}voluer le contenu des r{\`e}gles applicables {\`a} la gestion publique, d{\textquoteright}autre part en soumettant le droit {\`a} un imp{\'e}ratif nouveau d{\textquoteright}efficacit{\'e} ; la {\guillemotleft} juridicisation des pr{\'e}ceptes manag{\'e}riaux {\guillemotright} a ainsi {\'e}t{\'e} doubl{\'e}e d{\textquoteright}un processus plus g{\'e}n{\'e}ral de {\guillemotleft} manag{\'e}risation du droit {\guillemotright}, visant {\`a} renforcer la port{\'e}e normative de celui-ci. En se juridicisant, le management public tend cependant {\`a} perdre une part de sa substance et {\`a} {\^e}tre phagocyt{\'e} par la rationalit{\'e} juridique.},
