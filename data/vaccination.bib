	doi = {10.3406/pomap.1986.1883},
	abstract = {Cet article se propose d'utiliser et de tester un cadre th{\'e}orique {\'e}labor{\'e} par HAFSI concernant les relations entre l'Etat et une entreprise publique, pr{\'e}sent{\'e} dans le num{\'e}ro 2/1983 de cette Revue et dans une publication ult{\'e}rieure. Le cas d'{\'e}tude se r{\'e}f{\`e}re aux t{\'e}l{\'e}communications dans les quatre grands pays europ{\'e}ens. Le mod{\`e}le HAFSI, d{\^u}ment {\'e}tendu, fournit une tr{\`e}s bonne classification et un sch{\'e}ma d'interpr{\'e}tation int{\'e}ressant des diff{\'e}rentes phases de l'{\'e}volution des PTT en Europe sur les vingt derni{\`e}res ann{\'e}es. Il permet d'expliquer pourquoi, partant d'un syst{\`e}me commun mis en place durant les cinquante ann{\'e}es pr{\'e}c{\'e}dentes, les PTT europ{\'e}ens ont connu des voies de d{\'e}veloppement aussi diverses sous la pression de la technologie et de la demande. Il conduit {\`a} l'identification des variables cruciales propres {\`a} chaque pays : la recherche du consensus en RFA, la dialectique {\guillemotleft}technocratique{\guillemotright} en France, le poids {\'e}crasant de la bataille politique en Italie et l'inad{\'e}quation d'une entreprise publique avec son statut en Grande Bretagne. L'article ne v{\'e}rifie pas l'assertion {\'e}volutionniste du mod{\`e}le HAFSI, {\guillemotleft}le cycle de vie des entreprises publiques{\guillemotright}. Notre conclusion est que, bien que leur autonomie soit n{\'e}cessaire pour tenir le pas avec le changement {\'e}conomique et technique, elle n'est en aucune fa{\c c}on in{\'e}luctable.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Dang-N'Guyen, Godefroy},
	year = {1986},
	pages = {119--152},
}

@article{verrier_naissance_1986,
	title = {Naissance du statut g{\'e}n{\'e}ral de la {Fonction} publique territoriale},
	volume = {4},
