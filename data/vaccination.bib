}

@article{meyssonnier_gestion_1995,
	title = {Gestion de la communication et communication sur la gestion dans les villes},
	volume = {13},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_3_2070},
	doi = {10.3406/pomap.1995.2070},
	abstract = {Une enqu{\^e}te aupr{\`e}s des villes fran{\c c}aises de plus de 20 000 habitants permet de r{\'e}aliser la premi{\`e}re {\'e}tude syst{\'e}matique des journaux communaux, qu'ils soient externes (destin{\'e}s {\`a} la population) ou internes (destin{\'e}s au personnel). L'{\'e}volution historique de la presse municipale est pr{\'e}sent{\'e}e. On montre que les journaux externes se d{\'e}veloppent depuis une vingtaine d'ann{\'e}es, au point d'{\^e}tre maintenant pr{\'e}sents dans quasiment toutes les villes {\'e}tudi{\'e}es, mais que l'usage de journaux internes est beaucoup plus r{\'e}cent et ne touche qu'une commune sur deux. Les caract{\'e}ristiques et le contenu des journaux sont analys{\'e}s. Les journaux externes apparaissent assez homog{\`e}nes dans leur forme et leur r{\^o}le. Ils semblent {\^e}tre plus un levier de construction d'une image de la mairie que le lieu de d{\'e}bats et d'{\'e}changes entre les acteurs de la cit{\'e}. Une d{\'e}marche de type marketing semble pr{\'e}dominer. Les journaux internes sont plus divers. Une Analyse des Correspondances Multiples montre qu'ils se diff{\'e}rencient selon la taille de la ville et la date de cr{\'e}ation. L'implication du personnel appara{\^i}t dans tous les cas assez faible.},
	language = {fr},
	number = {3},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Meyssonnier, Fran{\c c}ois and Appel, Violaine},
	year = {1995},
	pages = {249--266},
}

@article{marsaud_recul_1995,
	title = {Recul, red{\'e}ploiement ou d{\'e}ploiement du management dans les communes ? {Questions} et tentatives de r{\'e}ponses {\`a} partir de l'exp{\'e}rience de {Saint}-{Denis}},
	volume = {13},
	issn = {0758-1726},
	shorttitle = {Recul, red{\'e}ploiement ou d{\'e}ploiement du management dans les communes ?},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_3_2069},
	doi = {10.3406/pomap.1995.2069},
	abstract = {Depuis quelques ann{\'e}es, on assiste dans les collectivit{\'e}s territoriales {\`a} une certaine {\'e}volution de la gestion dont les caract{\'e}ristiques et la tendance par rapport {\`a} la p{\'e}riode pass{\'e}e peuvent laisser penser {\`a} une r{\'e}orientation. Le discours et les principes manag{\'e}riaux classiques des ann{\'e}es 1980 semblent c{\'e}der du terrain {\`a} une gestion apparemment moins rigoureuse, plus souple, plus pragmatique, plus "politique". Le souci du co{\^u}t des activit{\'e}s ou des {\'e}quipements compose plus qu'il y a quelques ann{\'e}es avec une volont{\'e} de mieux r{\'e}pondre aux attentes et aux pr{\'e}occupations des usagers. La gestion du personnel est redevenue plus "sociale", certains diront plus laxiste. Fruit de la d{\'e}centralisation, de la crise de la politique et signe des temps, les "pratiques d{\'e}mocratiques" des {\'e}lus locaux se sont d{\'e}velopp{\'e}es, tant avec les administr{\'e}s qu'avec les personnels. L'association plus {\'e}troite de ces partenaires {\`a} la d{\'e}finition des objectifs et {\`a} leur mise en {\oe}uvre g{\'e}n{\`e}re au niveau des politiques et de l'administration de nouvelles m{\'e}thodes de travail, de nouvelles relations. Les cadres de direction, principalement mobilis{\'e}s jusqu'alors sur le management interne, sont de plus en plus interpell{\'e}s par l'externe, que cela s'appelle d{\'e}veloppement local ou politique de la ville. Dans un environnement {\'e}conomique difficile (mais est-ce seulement pour cela ?) le service public commence {\`a} r{\'e}afficher fi{\`e}rement ses valeurs. Est-ce l{\`a} le r{\'e}sultat d'un simple effet de balancier, d'une r{\'e}action {\`a} certains exc{\`e}s du management en milieu public, d'une r{\'e}gression dans un contexte d{\'e}pressif, d'une adaptation {\`a} la crise, ou bien les signes d'une phase nouvelle, d'une maturation du management public, d'une {\'e}mergence de ses {\'e}l{\'e}ments sp{\'e}cifiques, de l'affirmation du citoyen fonctionnaire ou de l'usager citoyen comme acteur du management ? Mais cette dimension humaniste, d{\'e}mocratique, politique du management, pour r{\'e}elle qu'elle soit, ne vient-elle pas servir d'alibi, de couverture {\`a} un essoufflement des pratiques manag{\'e}riales, et justifier le retour {\`a} une certaine facilit{\'e}, dans un contexte o{\`u} le secteur priv{\'e} a un genou {\`a} terre ? Comment mieux prendre en compte les avanc{\'e}es du management public local sans remettre en cause les acquis des p{\'e}riodes pass{\'e}es ?.},
	language = {fr},
	number = {3},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Marsaud, Jacques},
	year = {1995},
	pages = {225--247},
}

