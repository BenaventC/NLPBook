	url = {http://pmp.revuesonline.com/article.jsp?articleId=19757},
	doi = {10.3166/pmp.31.301-317},
	abstract = {La r{\'e}gulation des {\'e}tablissements scolaires s{\textquoteright}est {\'e}loign{\'e}e, en France comme ailleurs, de sa forme bureaucratique et m{\^e}le aujourd{\textquoteright}hui r{\'e}gulation par les normes, les projets et les r{\'e}- sultats. Une enqu{\^e}te aupr{\`e}s des chefs d{\textquoteright}{\'e}tablissements secondaires nous permet d{\textquoteright}appr{\'e}- cier comment, en France aujourd{\textquoteright}hui, se combinent ces trois {\'e}l{\'e}ments dans la r{\'e}gulation des {\'e}tablissements. L{\textquoteright}article montre que r{\'e}gulation par projets et par les r{\'e}sultats sont peu pr{\'e}gnantes dans les relations entre les {\'e}tablissements et leur instance de tutelle, mais le sont davantage dans la r{\'e}gulation interne aux {\'e}tablissements. Le mode de r{\'e}gulation qui en r{\'e}sulte, plut{\^o}t informel comme le montre la grande dispersion des r{\'e}ponses, ne para{\^i}t pas susciter de rejet de la part des acteurs mais son adaptation {\`a} la r{\'e}gulation des {\'e}tablis- sements {\guillemotleft} difficiles {\guillemotright} peut {\^e}tre questionn{\'e}e, ce qui conduit {\`a} s{\textquoteright}interroger sur ses effets en termes d{\textquoteright}{\'e}quit{\'e} de l{\textquoteright}enseignement.},
	number = {3},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Meuret, Denis and Morlaix, Sophie and Dirani, Agathe},
	month = sep,
	year = {2014},
	pages = {301--317},
	annote = {{\'e}tablissement scolaire, r{\'e}gulation, bureaucratie, {\'e}ducation prioritaire, {\'e}quit{\'e}, pilotage.},
}

@article{basle_lacculturation_2014,
	title = {L{\textquoteright}acculturation silencieuse {\`a} l{\textquoteright}{\'e}valuation des politiques publiques et programmes dans les collectivit{\'e}s territoriales en {France} : le d{\'e}veloppement de capacit{\'e}s internes orient{\'e}es vers la performance et la nouvelle gouvernance publique},
	volume = {31},
	issn = {0758-1726},
	shorttitle = {L{\textquoteright}acculturation silencieuse {\`a} l{\textquoteright}{\'e}valuation des politiques publiques et programmes dans les collectivit{\'e}s territoriales en {France}},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=19755},
	doi = {10.3166/pmp.31.267-282},
	abstract = {L{\textquoteright}{\'e}valuation des politiques publiques dans les collectivit{\'e}s territoriales n{\textquoteright}est plus une activit{\'e} nouvelle. Cette activit{\'e}, d{\'e}velopp{\'e}e silencieusement, a suivi une trajectoire {\`a} double origine. En premier lieu, la participation {\`a} des activit{\'e}s et pratiques de l{\textquoteright}{\'E}tat cen- tral et l{\textquoteright}imitation avec l{\textquoteright}apprentissage par la pratique d{\textquoteright}{\'e}valuation sur des programmes publics cofinanc{\'e}s. Et, en second lieu, une origine propre : les pratiques {\'e}valuatives en- dog{\`e}nes dans les collectivit{\'e}s ont {\'e}t{\'e} aussi d{\'e}pendantes des dimensions organisation- nelles des collectivit{\'e}s territoriales (taille, existence d{\textquoteright}un service d{\'e}di{\'e}, fonctionnement en r{\'e}seau...) et politiques (proximit{\'e}s des {\'e}lus et agents vis-{\`a}-vis des b{\'e}n{\'e}ficiaires et citoyens). Quelques {\'e}tudes de cas illustrent ici cette hypoth{\`e}se d{\'e}fendue de la double origine. Plus analytiquement, il appara{\^i}t, malgr{\'e} une assez grande h{\'e}t{\'e}rog{\'e}n{\'e}it{\'e} entre collectivit{\'e}s territoriales, une double figure de l{\textquoteright}innovation en {\'e}valuation dans les col- lectivit{\'e}s territoriales : l{\textquoteright}innovation a d{\textquoteright}abord eu lieu {\`a} cause d{\textquoteright}une implication manag{\'e}- riale forte dans les nouvelles pratiques {\'e}valuatives des collectivit{\'e}s territoriales (la mon- t{\'e}e en puissance du contr{\^o}le de gestion, du management strat{\'e}gique et r{\'e}cemment du management de la performance). Ensuite, l{\textquoteright}innovation va de pair avec la volont{\'e} souvent observable d{\textquoteright}animation du d{\'e}bat public de proximit{\'e} (avec le sociogramme des parties prenantes d{\textquoteright}un programme). Cette double figure d{\textquoteright}innovation territoriale constitue une forme de mix tr{\`e}s contemporain et d{\textquoteright}actualit{\'e} entre la banni{\`e}re de la {\guillemotleft} Nouvelle performance publique {\guillemotright} (NPP) et celle de la {\guillemotleft} Nouvelle gouvernance publique {\guillemotright} (NGP). C{\textquoteright}est cette seconde banni{\`e}re qui risque de prendre de l{\textquoteright}importance avec le nou- veau mode de gouvernance territoriale qui interviendra avec les lois de d{\'e}centralisa- tion et les restrictions dans les d{\'e}penses, nouveau mode o{\`u} les collectivit{\'e}s davantage plac{\'e}es sur un pied d{\textquoteright}{\'e}galit{\'e} avec l{\textquoteright}{\'E}tat pourront, selon des modalit{\'e}s propres, assurer leur responsabilit{\'e} dans la conception, le suivi et l{\textquoteright}{\'e}valuation des politiques publiques et des programmes territorialis{\'e}s.},
	number = {3},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Basl{\'e}, Maurice Alexandre},
	month = sep,
	year = {2014},
	pages = {267--282},
	annote = {{\'e}valuations - {\'e}valuations partenariales obligatoires - capacit{\'e}s {\'e}valuatives des villes, capacit{\'e}s {\'e}valuatives des d{\'e}partements, capacit{\'e}s {\'e}valuatives des r{\'e}gions - pilotage de la performance et ouverture citoyenne en collectivit{\'e}s territoriales.},
}

@article{honta_renovation_2014,
	title = {La r{\'e}novation de la gestion des carri{\`e}res dans le secteur public {\`a} l{\textquoteright}{\'e}preuve des relations entre {Etat} et groupes d{\textquoteright}int{\'e}r{\^e}t. {Le} cas des {Conseillers} {Techniques} {Sportifs} du {Minist{\`e}re} des {Sports}},
