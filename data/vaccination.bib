	number = {2},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Harnay, Sophie and Vigouroux, Isabelle},
	year = {2000},
	pages = {69--92},
}

@article{clergeau_les_2000,
	title = {Les politiques de d{\'e}veloppement technologique : contenu, organisation et {\'e}valuation de la fonction d'interm{\'e}diation technologique},
	volume = {18},
	issn = {0758-1726},
	shorttitle = {Les politiques de d{\'e}veloppement technologique},
