	issn = {0758-1726, 2119-4831},
	shorttitle = {Lolf},
	url = {http://journals.openedition.org/pmp/1362},
	doi = {10.4000/pmp.1362},
	abstract = {Notre article est issu d{\textquoteright}une recherche action de trois ann{\'e}es (2004-2007) sur la modernisation de la Gestion des Ressources Humaines (GRH) et de la mise en place d{\textquoteright}une Gestion Pr{\'e}visionnelle des Emplois et des Comp{\'e}tences (GPEC) dans neuf pr{\'e}fectures d{\textquoteright}Indre et Loire, d{\textquoteright}Eure et Loir, du Cher, du Loir et Cher, de l{\textquoteright}Indre, du Doubs, de Haute Vienne, du Loiret, de la Corr{\`e}ze. De cette exp{\'e}rience, nous voulons retenir que la Lolf ne se limite pas {\`a} poser des contraintes financi{\`e}res, mais qu{\textquoteright}elle vise un changement de logique, suscite un {\'e}lan manag{\'e}rial, incite {\`a} des pratiques innovantes et invite {\`a} l{\textquoteright}attribution de nouvelles responsabilit{\'e}s dans des marges de man{\oe}uvre {\'e}largies. Nous l{\textquoteright}illustrons en montrant, en premier lieu, que la Lolf propose un nouveau mode de gouvernance obligeant {\`a} repenser les responsabilit{\'e}s de chacun; en deuxi{\`e}me lieu, qu{\textquoteright}elle invite {\`a} rapprocher les intentions strat{\'e}giques telles qu{\textquoteright}elles sont v{\'e}hicul{\'e}es, de la GRH telle qu{\textquoteright}elle se pratique (les m{\'e}thodes, les outils...) ; en troisi{\`e}me lieu, que la Lolf est catalyseur de changement, notamment culturel ; et en quatri{\`e}me lieu, qu{\textquoteright}elle est cr{\'e}atrice de valeur ajout{\'e}e pour l{\textquoteright}usager et l{\textquoteright}agent. Dans le cinqui{\`e}me et dernier point de l{\textquoteright}article, nous examinons les cons{\'e}quences de ces changements sur les pratiques de rationalisation des ressources humaines et la gestion des comp{\'e}tences.},
	language = {fr},
	number = {Vol 26/2},
	urldate = {2020-06-26},
	journal = {Politiques et Management Public},
	author = {Abraham, Jocelyne and Brillet, Franck},
	month = jun,
	year = {2008},
	pages = {15--30},
	annote = {Changement ; Comp{\'e}tences ; Cr{\'e}ation de valeur ; Culture ; Employabilit{\'e} ; Gestion des Ressources Humaines (GRH) ; Gestion Pr{\'e}visionnelle des Emplois et des Comp{\'e}tences (GPEC) ; Gouvernance ; LOLF ; Performance ; Strat{\'e}gie},
}

