	number = {2},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Renault, Sophie and Boutigny, Erwan},
	month = jun,
	year = {2014},
	pages = {215--237},
	annote = {crowdsourcing, crowdsourcing citoyen, ville, financement participatif, TIC, {\'e}tudes de cas.},
}

