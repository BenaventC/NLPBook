	volume = {31},
	issn = {0758-1726},
	shorttitle = {Une analyse comparative des {\'e}valuations de politiques de clusters en {Europe}},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=19323},
	doi = {10.3166/pmp.31.87-111},
	abstract = {Dans ce volet de nos recherches sur l{\textquoteright}{\'e}valuation des clusters (cf. Galli{\'e}, Glaser, Lefebvre et Pallez, 2012, qui analyse le cas de l{\textquoteright}{\'e}valuation de la politique fran{\c c}aise des p{\^o}les de comp{\'e}titivit{\'e}), nous comparons quatre cas europ{\'e}ens d{\textquoteright}{\'e}valuation de clusters : Wal- lonie, Allemagne, Basse-Autriche et France. Nous d{\'e}veloppons d{\textquoteright}abord une grille de lecture avec laquelle nous analysons ensuite ces diff{\'e}rentes {\'e}valuations. Cette grille de lecture nous aide surtout {\`a} souligner les diff{\'e}rences entre ces {\'e}valuations sur trois dimensions : les objets de l{\textquoteright}{\'e}valuation et l{\textquoteright}imputabilit{\'e} des r{\'e}sultats, la d{\'e}marche et la m{\'e}thodologie des {\'e}valuations, et les effets et usages des {\'e}valuations. En conclusion, {\`a} partir de ces cas empiriques, nous proposons de distinguer deux mod{\`e}les id{\'e}al- typiques d{\textquoteright}{\'e}valuation, qui combinent ces diff{\'e}rentes caract{\'e}ristiques de mani{\`e}re coh{\'e}- rente et renvoient {\`a} des usages contrast{\'e}s des {\'e}valuations : un mod{\`e}le {\guillemotleft} {\'e}conomiste {\guillemotright} et un mod{\`e}le {\guillemotleft} gestionnaire {\guillemotright}.},
	number = {1},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Galli{\'e}, Emilie-Pauline and Glaser, Anna and Pallez, Fr{\'e}d{\'e}rique},
	month = mar,
	year = {2014},
	pages = {87--111},
	annote = {politique publique, {\'e}valuation, p{\^o}le de comp{\'e}titivit{\'e}, cluster, politique de cluster.},
}

@article{tourmen_contributions_2014,
