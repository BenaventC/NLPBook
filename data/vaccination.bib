	volume = {35},
	shorttitle = {L{\textquoteright}analyse d{\textquoteright}impact de la r{\'e}glementation},
	doi = {10.3166/pmp.35.2018.0008},
	abstract = {Dans les derni{\`e}res d{\'e}cennies, les programmes de r{\'e}forme de la r{\'e}glementation ont {\'e}t{\'e} largement adopt{\'e}s, et ont plac{\'e} l{\textquoteright}analyse d{\textquoteright}impact de la r{\'e}glementation dans la position centrale d{\textquoteright} {\guillemotleft} outil cl{\'e} de l{\textquoteright}am{\'e}lioration de la r{\'e}glementation {\guillemotright}. D{\textquoteright}un point de vue juridique la question se pose de savoir s{\textquoteright}il existe un devoir l{\'e}gal de mener {\`a} bien des analyses d{\textquoteright}impact et si ce devoir peut {\^e}tre sanctionn{\'e} par les tribunaux. Dans le monde acad{\'e}mique, les arguments pour ou contre une judiciarisation des instruments de la r{\'e}forme r{\'e}glementaire ont {\'e}t{\'e} d{\'e}velopp{\'e}s. Dans cet article, ces arguments sont compar{\'e}s, en particulier, {\`a} la pratique des tribunaux des pays europ{\'e}ens et de la cour europ{\'e}enne de justice. L{\textquoteright}article conclut que l{\textquoteright}impact de l{\textquoteright}examen judiciaire ne doit pas {\^e}tre surestim{\'e} compte tenu de la d{\'e}f{\'e}rence manifest{\'e}e par les tribunaux. Un strict exa- men formel de processus en particulier ne produit aucun effet. Contrastant avec cela l{\textquoteright}examen de rationalit{\'e} proc{\'e}durale a davantage de potentiel comme il ouvre le chemin {\`a} une perspective des droits fondamentaux et pr{\'e}sente des sauvegardes contre l{\textquoteright}inter- f{\'e}rence arbitraire.},
	journal = {Politiques et management public},
	author = {Popelier, Patricia},
	month = dec,
	year = {2018},
	pages = {107--129},
	annote = {analyse d'impact, devoir l{\'e}gal, contr{\^o}le judiciaire, rationalit{\'e} proc{\'e}durale.},
}

