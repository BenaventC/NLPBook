@article{le_pottier_efficacite_1989,
	title = {Efficacit{\'e} et justice : crit{\`e}res pour le choix d'un mode de financement d'un service d'enl{\`e}vement des ordures},
	volume = {7},
	issn = {0758-1726},
	shorttitle = {Efficacit{\'e} et justice},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1989_num_7_3_2902},
	doi = {10.3406/pomap.1989.2902},
	abstract = {Le financement d'un service communal de ramassage des ordures peut {\^e}tre assur{\'e} par le budget g{\'e}n{\'e}ral, par une taxe ou par une redevance fonction du service rendu. Les crit{\`e}res de choix du mode de financement sont ceux de l'efficacit{\'e} et de la justice. Cet article, qui s'appuie sur une {\'e}tude r{\'e}alis{\'e}e par les auteurs au sein d'une commune fran{\c c}aise de 15 000 habitants, montre que : 1{\textdegree}) en mati{\`e}re d'efficacit{\'e} et contrairement {\`a} ce qu'une analyse superficielle laisse {\`a} penser, la taxe est plut{\^o}t pr{\'e}f{\'e}rable {\`a} la redevance ; 2{\textdegree}) en mati{\`e}re de justice, d'une part la substitution d'une redevance {\`a} une taxe entra me des transferts de charge tr{\`e}s importants, ce qui montre que le syst{\`e}me de la taxe, laquelle n'est pas fonction comme la redevance du service rendu, est tr{\`e}s injuste (vis-{\`a}-vis de la justice commutative) ; d'autre part, si on s'int{\'e}resse {\`a} la justice distributive, on constate que ces transferts ne sont pas syst{\'e}matiquement r{\'e}gressifs, l{\`a} aussi contrairement {\`a} ce qu'une vue rapide des choses peut laisser croire.},
	language = {fr},
	number = {3},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Le Pottier, Jacques and Van-Hien, Thai},
	year = {1989},
	pages = {25--53},
}

@article{guglielmi_discours_1989,
	title = {Discours de la bureaucratie},
	volume = {7},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1989_num_7_3_2901},
	doi = {10.3406/pomap.1989.2901},
	abstract = {A travers toute technique de management, les organisations bureaucratiques mettent en {\oe}uvre le langage. En partant de l'hypoth{\`e}se que l'{\'e}tude des discours bureaucratiques permettrait d'am{\'e}liorer la connaissance du mode de fonctionnement de ces organisations, il est possible d'aborder ces discours sous l'angle, rituel, magique ou po{\'e}tique. En effet, parce qu'il vise {\`a} la repr{\'e}sentation du pouvoir, le discours de la bureaucratie comporte un aspect rituel ; parce qu'il tend {\`a} agir directement sur les choses, il participe de la magie ; enfin, en raison de l'usage qu'il fait de la confusion dans le langage, il est aussi un discours po{\'e}tique. Une telle approche invite {\`a} mener une recherche approfondie tendant {\`a} situer les discours bureaucratiques dans un syst{\`e}me de production et de contr{\^o}le des significations. Elle r{\'e}v{\`e}le aussi que le langage de la bureaucratie, au-del{\`a} de la simple transmission de l'information, peut remplir des fonctions d'occultation ou de suggestion pour {\'e}viter le recours {\`a} l'argumentation.},
	language = {fr},
	number = {3},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Guglielmi, Gilles J.},
	year = {1989},
	pages = {1--23},
}

@article{wildavsky_les_1989,
