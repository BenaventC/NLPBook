@article{boukamel_roreseaux_2017,
	title = {Le r{\^o}le des r{\'e}seaux d{\textquoteright}acteurs dans l{\textquoteright}innovation publique complexe : le cas du vote {\'e}lectronique dans le canton de {Gen{\`e}ve}},
