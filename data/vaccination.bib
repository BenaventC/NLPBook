	doi = {10.3406/pomap.1990.2946},
	abstract = {Cet article constitue la synth{\`e}se de quatre monographies r{\'e}alis{\'e}es pour le compte du Minist{\`e}re de la Fonction Publique, avec le concours financier de l'Institut de Management Public, du Minist{\`e}re de l'Agriculture et de la For{\^e}t et du Minist{\`e}re de l'Equipement. Les analyses pr{\'e}sent{\'e}es reposent sur un {\'e}chantillon d'observation limit{\'e}. Elles s'appuient toutefois sur plus d'une centaine d'interviews r{\'e}alis{\'e}es aupr{\`e}s des diff{\'e}rents acteurs de la modernisation, tant dans les directions centrales que dans les services d{\'e}concentr{\'e}s (personnels de tous niveaux hi{\'e}rarchiques, directeurs d{\'e}partementaux, directeurs du personnel et/ou leurs adjoints, responsables syndicaux...). Par ailleurs, ces analyses ont pu {\^e}tre crois{\'e}es avec les mat{\'e}riaux r{\'e}unis dans le cadre d'actions de formation et de conseil que les auteurs ont pu r{\'e}aliser. Si bien que les d{\'e}veloppements propos{\'e}s peuvent {\^e}tre consid{\'e}r{\'e}s finalement comme significatifs de certaines tendances actuellement observ{\'e}es dans l'administration. Elles n'en constituent pas moins une {\'e}tape dans l'analyse que les auteurs m{\`e}nent actuellement sur la place de la ressource humaine dans la modernisation et qui les conduira {\`a} la r{\'e}daction d'un ouvrage sur ce th{\`e}me. L'article abordera quatre points principaux : . La m{\'e}thodologie et le d{\'e}roulement de l'{\'e}tude . Le constat . Les cons{\'e}quences pour les administrations centrales . La probl{\'e}matique de l'{\'e}valuation : une premi{\`e}re approche.},
	language = {fr},
	number = {2},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Barouch, Gilles and Chavas, Herv{\'e}},
	year = {1990},
	pages = {133--156},
}

@article{ternier_moderniser_1990,
	title = {Moderniser, oui, mais avec les hommes},
	volume = {8},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1990_num_8_2_2945},
	doi = {10.3406/pomap.1990.2945},
	abstract = {Le secteur public, comme les entreprises, agit dans un contexte culturel, concurrentiel et {\'e}conomique en {\'e}volution rapide. Anticiper et s'adapter doivent {\^e}tre aussi ses priorit{\'e}s. D{\`e}s 1 987, l'Office National de la Navigation charg{\'e} de rapprocher l'offre et la demande de transports par voies d'eau rel{\`e}ve le d{\'e}fi dans une d{\'e}marche de projet d'entreprise. Quatre ann{\'e}es plus tard, les r{\'e}sultats obtenus d{\'e}passent les esp{\'e}rances que l'on pouvait fonder : la productivit{\'e} a augment{\'e} de plus de 40 \% en 4 ans et le co{\^u}t pour le contribuable a diminu{\'e} de 14 \% pendant la m{\^e}me p{\'e}riode, en francs constants. Sa dynamique continue de produire des effets. Le succ{\`e}s r{\'e}side sans doute dans la mani{\`e}re de proc{\'e}der : si un dirigeant veut mobiliser les hommes sur leur avenir, il doit {\^e}tre {\`a} leur {\'e}coute et s'engager totalement.},
	language = {fr},
	number = {2},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Ternier, Michel},
	year = {1990},
	pages = {115--131},
}

@article{pfeiffer_perequation_1990,
