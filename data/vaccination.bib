@article{maury_les_1997,
	title = {Les contradictions du n{\'e}o-lib{\'e}ralisme gestionnaire : l'exemple du syst{\`e}me municipal n{\^i}mois (1983-1995)},
	volume = {15},
	issn = {0758-1726},
	shorttitle = {Les contradictions du n{\'e}o-lib{\'e}ralisme gestionnaire},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1997_num_15_4_2168},
	doi = {10.3406/pomap.1997.2168},
	abstract = {Avec Jean Bousquet, D{\'e}put{\'e}-Maire de 1983 jusqu'en 1995, N{\^i}mes entendait redevenir une "ville-strat{\`e}ge", quitter le champ des politiques assistancielles pr{\'e}c{\'e}dentes pour entrer, au sein d'un r{\'e}seau de villes du Midi politique et au-del{\`a}, dans le jeu concurrentiel national et international. Cette reformulation des r{\`e}gles du jeu communal allait, en outre, se saisir du nouvel espace ouvert par les lois de d{\'e}centralisation et annoncer la mise en {\oe}uvre d'un mode de gestion n{\'e}o-lib{\'e}ral structur{\'e} autour de trois axes principaux : - la gestion financi{\`e}re du budget communal sur la base d'outils et d'indicateurs de gestion performants, - la r{\'e}alisation d'investissements structur{\'e}s autour de grands {\'e}quipements urbains, dont la finalit{\'e} serait de dynamiser le tissu {\'e}conomique local, - la gestion dynamique du personnel municipal, {\`a} la fois recentr{\'e} sur ses m{\'e}tiers de "base" et dont l'adh{\'e}sion au concept de ville-entreprise serait recherch{\'e}e au moyen de nouvelles techniques de gestion. A travers cet article, l'auteur se propose de v{\'e}rifier ces hypoth{\`e}ses initiales, d'analyser la m{\'e}thode et les fondements d'un n{\'e}o- lib{\'e}ralisme gestionnaire instrumentalis{\'e} {\`a} l'{\'e}chelon local par un Maire-entrepreneur (I) "La ville comme entreprise", puis d'examiner les contradictions et les limites de cette approche entrepreneuriale des affaires publiques locales (II) "L'{\'e}chec du municipalisme entrepreneurial".},
	language = {fr},
	number = {4},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Maury, Yann},
	year = {1997},
	pages = {145--170},
}

@article{de_lattre-gasquet_projet_1997,
