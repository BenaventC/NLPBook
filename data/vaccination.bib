@article{de_francesco_evaluer_2018,
	title = {{\'E}valuer {\'e}quitablement les r{\'e}gulateurs : aligner les analyses d{\textquoteright}impact sur le savoir scientifique},
	volume = {35},
	shorttitle = {{\'E}valuer {\'e}quitablement les r{\'e}gulateurs},
	doi = {10.3166/pmp.35.2018.0007},
	abstract = {Des analyses r{\'e}trospectives, des mesures et des tableaux de bord de la performance r{\'e}glementaire ont {\'e}t{\'e} utilis{\'e}s pour {\'e}valuer la qualit{\'e} des analyses d{\textquoteright}impact. Toutefois, ces m{\'e}thodes n{\'e}gligent le savoir scientifique r{\'e}el disponible au moment de la r{\'e}alisation de l{\textquoteright}appr{\'e}ciation d{\textquoteright}une politique d{\'e}termin{\'e}e et ne permettent pas d{\textquoteright}appr{\'e}hender l{\textquoteright}ampleur de l{\textquoteright}apprentissage diachronique, li{\'e}e {\`a} une r{\'e}forme de la r{\'e}glementation.L{\textquoteright}objectif du pr{\'e}sent article qui se concentre sur la lib{\'e}ralisation du rail dans l{\textquoteright}Union Europ{\'e}enne, est d{\textquoteright}{\'e}valuer la qualit{\'e} des analyses {\'e}conomiques incluses dans les analy-ses d{\textquoteright}impact au regard de l{\textquoteright}{\'e}tat des connaissances scientifiques. La conception de notre phase de recherche est simple. Un examen de la litt{\'e}rature {\'e}conomique retrace les progr{\`e}s dans les m{\'e}thodes scientifiques qui permettent d{\textquoteright}{\'e}valuer l{\textquoteright}impact {\'e}conomique de la lib{\'e}ralisation du rail. En comparant ces connaissances scientifiques avec le savoir utilis{\'e} dans la pratique de l{\textquoteright}analyse d{\textquoteright}impact, il convient principalement de v{\'e}rifier si les mod{\`e}les {\'e}conomiques sont pleinement exploit{\'e}s par les entit{\'e}s responsables de l{\textquoteright}{\'e}valuation des politiques. La contribution du pr{\'e}sent article est double : d{\textquoteright}une part, il am{\'e}liore la m{\'e}thodologie de l{\textquoteright}{\'e}valuation des politiques de r{\'e}gulation et, d{\textquoteright}autre part, il contribue {\`a} la documentation sur l{\textquoteright}utilisation des connaissances scientifiques dans l{\textquoteright}{\'e}laboration des politiques.},
	journal = {Politiques et management public},
	author = {De Francesco, Fabrizio},
	month = dec,
	year = {2018},
	pages = {131--151},
	annote = {Commission europ{\'e}enne, lib{\'e}ralisation du rail, qualit{\'e} des analyses d{\textquoteright}impact, analyses r{\'e}trospectives.},
}

@article{popelier_lanalyse_2018,
