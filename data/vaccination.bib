	journal = {Politiques et management public},
	author = {Attar{\c c}a, Mourad and Chomienne, Herv{\'e} and Corbel, Pascal},
	month = mar,
	year = {2012},
	pages = {473--491},
	annote = {politique publique, propri{\'e}t{\'e} intellectuelle, brevet, innovation, recherche publique.},
}

