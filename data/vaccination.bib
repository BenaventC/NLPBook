	number = {2},
	urldate = {2020-06-29},
	journal = {Politiques et Management public},
	author = {Bensadon, Didier},
	year = {2016},
	keywords = {accounting harmonization, accounting history, accounting regulation, consolidation des comptes, group accounting, histoire de la comptabilit{\'e}, IFRS, normalisation comptable, normes IFRS},
	annote = {harmonisation comptable europ{\'e}enne, normes IFRS, normalisation comptable, consolidation des comptes, Septi{\`e}me directive europ{\'e}enne, histoire de la comptabilit{\'e}.},
}

