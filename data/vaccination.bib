	abstract = {L{\textquoteright}article a pour propos d{\textquoteright}examiner les cons{\'e}quences sociales de la loi de Baumol sur la hausse continue des prix relatifs des activit{\'e}s {\`a} gains de productivit{\'e} faible, largement repr{\'e}sent{\'e}es par les biens collectifs. Cette {\guillemotleft} croissance d{\'e}s{\'e}quilibr{\'e}e {\guillemotright} contribue {\`a} aggraver les {\'e}carts de richesse et de bien-{\^e}tre au sein des populations, surtout si l{\textquoteright}action publique continue {\`a} favoriser la recherche d{\textquoteright}un d{\'e}veloppement {\'e}conomique fond{\'e} sur le progr{\`e}s technique. Pourtant, des voix s{\textquoteright}{\'e}l{\`e}vent pour convaincre les puissants de ce monde d{\textquoteright}agir pour d{\textquoteright}autres objectifs que la croissance du PIB, comme la mise en {\oe}uvre effective des droits fondamentaux, de la pr{\'e}servation des {\'e}cosyst{\`e}mes, du d{\'e}veloppement humain... qui n{\'e}cessitent la construction de nouvelles informations. La loi de Baumol, fond{\'e}e sur des informations en provenance des march{\'e}s (prix et quantit{\'e}s), d{\'e}montre une double impossibilit{\'e}, {\'e}conomique et sociale : d{\textquoteright}une part, la croissance du {\guillemotleft} secteur dynamique {\guillemotright} - celui des biens industriels - ne peut {\^e}tre infinie et, d{\textquoteright}autre part, l{\textquoteright}action publique ne pourra pas compenser financi{\`e}rement la hausse sans fin des prix relatifs des activit{\'e}s du {\guillemotleft} secteur stagnant {\guillemotright} - celui des services marchands et non marchands qui nourrissent les liens sociaux. D{\textquoteright}o{\`u}, l{\textquoteright}interrogation l{\'e}gitime d{\textquoteright}un changement d{\textquoteright}orientation de la vie {\'e}conomique.},
	number = {3},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Le Duff, Robert and Orange, G{\'e}rald},
	month = mar,
	year = {2012},
	pages = {451--472},
	annote = {Action publique, biens collectifs, d{\'e}veloppement humain, information, loi de Baumol.},
}

