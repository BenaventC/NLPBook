	issn = {0758-1726},
	shorttitle = {Public {Management} in the {United} {Kingdom}},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_2001_num_19_3_2683},
	doi = {10.3406/pomap.2001.2683},
	abstract = {Ce document examine certains changements {\`a} grande {\'e}chelle qui ont eu lieu dans la gestion des organisations de service public au Royaume-Uni au cours des vingt derni{\`e}res ann{\'e}es environ. Ces changements peuvent {\^e}tre d{\'e}crits comme un exemple de "nouvelle gestion publique" (NGP). Le Royaume-Uni se distingue par la mesure dans laquelle ces id{\'e}es ont {\'e}t{\'e} adopt{\'e}es au sein des services publics. Certaines raisons de ce mod{\`e}le de "forte incidence de la NGP" seront expos{\'e}es et les principales caract{\'e}ristiques du mod{\`e}le britannique de NGP seront d{\'e}crites. La question de savoir si les gouvernements du New Labour sont susceptibles de perturber ce mod{\`e}le est examin{\'e}e, mais il est conclu que les ajustements seront probablement marginaux. Dans la seconde moiti{\'e} du document, la question de la responsabilit{\'e} {\`a} l'{\'e}gard du NPM est abord{\'e}e plus en d{\'e}tail. Diff{\'e}rents modes de responsabilit{\'e} sont d{\'e}crits. Il est conclu que le mode de responsabilit{\'e} dominant reste la "responsabilit{\'e} ascendante", avec des formes beaucoup plus faibles de "responsabilit{\'e} descendante". Si le m{\'e}canisme national de pr{\'e}vention a pu apporter des avantages en termes d'efficacit{\'e} et de gestion, les dangers de ce d{\'e}ficit d{\'e}mocratique persistant sont {\'e}galement {\'e}vidents.},
	language = {fr},
	number = {3},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Ferlie, Ewan},
	year = {2001},
	pages = {121--138},
}

