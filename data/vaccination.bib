	abstract = {Le concept de motivation de service public (MSP) conna{\^i}t un engouement croissant dans la litt{\'e}rature mondiale en management public. La MSP a {\'e}t{\'e} d{\'e}finie dans l{\textquoteright}article fonda- teur de Perry et Wise (1990) comme {\guillemotleft} une pr{\'e}disposition individuelle {\`a} agir en r{\'e}ponse {\`a} des mobiles caract{\'e}ristiques des institutions et organisations publiques {\guillemotright}. En mettant l{\textquoteright}accent sur les motivations altruistes des agents publics, la MSP ouvre de nouvelles perspectives concernant le management des ressources humaines dans les organisations publiques. Malgr{\'e} l{\textquoteright}int{\'e}r{\^e}t que suscite la notion au niveau international, il existe aujourd{\textquoteright}hui peu de travaux fran{\c c}ais sur ce th{\`e}me, notamment de nature empirique. C{\textquoteright}est pourquoi nous analysons la mani{\`e}re dont la MSP peut {\^e}tre appr{\'e}hend{\'e}e dans le contexte institutionnel fran{\c c}ais. En effet, dans ce contexte, la notion de service public est riche de repr{\'e}sentations diversifi{\'e}es (notamment politique, juridique, sociale) et poss{\`e}de parfois une dimension passionnelle. Apr{\`e}s une analyse th{\'e}orique de la mani{\`e}re dont les quatre dimensions de la MSP reconnues par la litt{\'e}rature (dimension politique, compassion, sacrifice et valeurs) peuvent {\^e}tre per{\c c}ues dans le contexte fran{\c c}ais, nous pr{\'e}sentons les r{\'e}sultats d{\textquoteright}une {\'e}tude internationale men{\'e}e aupr{\`e}s d{\textquoteright}agents de villes dans 12 pays, en mettant l{\textquoteright}accent sur les sp{\'e}cificit{\'e}s de la MSP en France.},
	number = {3},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Desmarais, C{\'e}line and Edey-Gamassou, Claire},
	month = mar,
	year = {2012},
	pages = {393--411},
	annote = {motivation, culture, service public, GRH, comparaison internationale.},
}

