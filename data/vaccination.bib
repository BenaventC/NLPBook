	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_sup_13_2_3391},
	doi = {10.3406/pomap.1995.3391},
	abstract = {La France, o{\`u} la vaccination anti-coquelucheuse est recommand{\'e}e mais non obligatoire, fut consid{\'e}r{\'e}e pendant une tr{\`e}s longue p{\'e}riode comme l'exemple du pays qui, gr{\^a}ce au recours {\`a} une couverture vaccinale tr{\`e}s {\'e}lev{\'e}e, a r{\'e}ussi {\`a} ma{\^i}triser la morbidit{\'e} et la mortalit{\'e} dues {\`a} la coqueluche. La r{\'e}apparition r{\'e}cente des cas de cette maladie en France pose de nouveau le probl{\`e}me du r{\^o}le de la vaccination et du maintien d'une large couverture vaccinale contre les maladies dites "en voie de disparition". Sur la base de la relation de d{\'e}pendance temporelle entre le taux d'acceptation de la vaccination et le nombre de cas enregistr{\'e}s de coqueluche, nous effectuons une {\'e}valuation compar{\'e}e des co{\^u}ts de la maladie et de la vaccination. Les r{\'e}sultats de l'{\'e}valuation co{\^u}t-b{\'e}n{\'e}fice de diff{\'e}rents programmes de vaccination confirment du point vue {\'e}conomique le bien- fond{\'e} de l'introduction d'une couverture vaccinale {\'e}lev{\'e}e et la n{\'e}cessit{\'e} de la stabilit{\'e} de cette politique dans le temps. Ainsi ils permettent de d{\'e}noncer, d'une part, le danger que repr{\'e}sente actuellement la baisse enregistr{\'e}e en France de la couverture vaccinale et, d'autre part la carence de toute surveillance {\'e}pid{\'e}miologique de la coqueluche dans notre pays.},
	language = {fr},
	number = {2},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Girard-Zdanowska, Dorota},
	year = {1995},
	pages = {1--25},
}

@article{hurstel_interet_1995,
	title = {Int{\'e}r{\^e}t g{\'e}n{\'e}ral et gestion priv{\'e}e : partenaires incompatibles ?},
	volume = {13},
	issn = {0758-1726},
	shorttitle = {Int{\'e}r{\^e}t g{\'e}n{\'e}ral et gestion priv{\'e}e},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_2_2051},
	doi = {10.3406/pomap.1995.2051},
	abstract = {Alors que les ann{\'e}es 80 et 90 sont marqu{\'e}es par le d{\'e}sengagement de l'Etat dans la vie {\'e}conomique, on peut se demander si le mouvement n'a pas {\'e}t{\'e} trop fort et s'il a toujours {\'e}t{\'e} suffisamment r{\'e}fl{\'e}chi. Les Etats concern{\'e}s ont-ils toujours accord{\'e} aux missions de service public qui leur incombent et au r{\^o}le primordial qu'ils doivent jouer dans la d{\'e}fense de l'int{\'e}r{\^e}t g{\'e}n{\'e}ral l'attention n{\'e}cessaire ? La question qui se poserait serait alors celle des moyens. Ceux propos{\'e}s par le droit des soci{\'e}t{\'e}s, {\`a} savoir la participation de l'Etat au capital de soci{\'e}t{\'e}s et la d{\'e}tention de l'action sp{\'e}cifique, montrent leurs limites. Dans ces conditions, peut-{\^e}tre convient-il de rappeler les avantages offerts par les solutions du droit administratif, qui par le biais de la d{\'e}l{\'e}gation de service public, permettent {\`a} l'Etat de confier la gestion des services publics {\`a} des op{\'e}rateurs priv{\'e}s tout en conservant un {\'e}troit contr{\^o}le sur cette gestion.},
	language = {fr},
	number = {2},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Hurstel, Daniel},
	year = {1995},
	pages = {129--146},
}

@article{basle_problemes_1995,
	title = {Probl{\`e}mes g{\'e}n{\'e}raux de transposition de la m{\'e}thode d'{\'e}valuation contingente au cas de services collectifs publics et sociaux},
	volume = {13},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_2_2050},
	doi = {10.3406/pomap.1995.2050},
	abstract = {La m{\'e}thode d'{\'e}valuation contingente commence {\`a} {\^e}tre import{\'e}e des Etats-Unis vers la France et on voit se d{\'e}velopper les premi{\`e}res applications dans le domaine des actifs naturels et des politiques environnementales. La m{\'e}thode est appuy{\'e}e sur l'individualisme m{\'e}thodologique et la nouvelle {\'e}conomie du bien-{\^e}tre. Elle cherche d'une mani{\`e}re g{\'e}n{\'e}rale {\`a} estimer des comportements de demande de biens collectifs et {\`a} justifier un certain niveau de fourniture de ces biens collectifs. Les d{\'e}bats sur la port{\'e}e et les limites ou les contradictions de la m{\'e}thode ont d{\'e}j{\`a} {\'e}t{\'e} nombreux {\`a} propos des valeurs de non-usage ou d'existence des actifs naturels. On essaie ici de montrer que ces d{\'e}bats seront encore plus lourds d'enjeux dans le cas de la valeur d'existence de la justice sociale et de l'{\'e}valuation des services sociaux ou des politiques sociales. Le transfert d'un outil au traitement de la question de la justice sociale pose des probl{\`e}mes qui sont {\'e}lucid{\'e}s et pour lesquels on ne trouve de solution que dans une utilisation non unilat{\'e}rale de la m{\'e}thode d'{\'e}valuation contingente.},
	language = {fr},
	number = {2},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Basl{\'e}, Maurice A.},
	year = {1995},
	pages = {109--127},
}

@article{bellon_evolutions_1995,
	title = {Evolutions institutionnelles des politiques industrielles en syst{\`e}me f{\'e}d{\'e}ral},
	volume = {13},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_2_2049},
	doi = {10.3406/pomap.1995.2049},
	abstract = {Nous analysons ici les conditions institutionnelles dans lesquelles peuvent se d{\'e}velopper les politiques industrielles {\`a} l'int{\'e}rieur d'un contexte f{\'e}d{\'e}ral. Nous associons l'efficacit{\'e} des politiques industrielles, moins au montant des cr{\'e}dits qui sont affect{\'e}s, qu'aux dynamiques institutionnelles qui en constituent le cadre. Cette {\'e}tude est fond{\'e}e sur les Etats-Unis, pays de plus en plus ouvert sur l'{\'e}conomie mondiale, mais o{\`u} le territoire continue de pr{\'e}valoir sur la nation et la proximit{\'e} strat{\'e}gique et politique sur la coh{\'e}rence nationale. La diss{\'e}mination des relations de pouvoirs et des initiatives ainsi que leur articulation aux niveaux r{\'e}gionaux et locaux sont une source permanente de cr{\'e}ation, d'adaptation et d'association du tissu productif. Cette dynamique conduit {\`a} une multiplication d'initiatives publiques ou collectives dans tous les domaines, mais simultan{\'e}ment {\`a} une duplication fr{\'e}quente des efforts et pas n{\'e}cessairement {\`a} une bonne ad{\'e}quation des politiques par rapport aux probl{\`e}mes pos{\'e}s. Notre hypoth{\`e}se est que cet atout constitue aujourd'hui, dans une {\'e}conomie ouverte, un avantage aussi d{\'e}cisif pour les Etats-Unis que ceux sur lesquels se focalise usuellement l'attention comme leur avance technologique, leur capacit{\'e} d'innovation, le poids des commandes militaires, le niveau de la concentration industrielle ou la dimension de leur march{\'e} int{\'e}rieur. On se situera ici {\`a} l'intersection de deux pr{\'e}occupations majeures : d'une part, l'{\'e}valuation de la capacit{\'e} des institutions publiques {\`a} construire un environnement favorable {\`a} la comp{\'e}titivit{\'e} productive dans le cadre de comportements enti{\`e}rement d{\'e}centralis{\'e}s, et le plus souvent atomis{\'e}s ; d'autre part , la mise en question de ces m{\^e}mes institutions, moins par le fait des mutations de l'environnement international que par leurs contradictions internes, parmi lesquelles on privil{\'e}gie la fragmentation institutionnelle et le manque de capacit{\'e} {\`a} gouverner.},
	language = {fr},
	number = {2},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Bellon, Bertrand},
	year = {1995},
	pages = {83--107},
}

@article{gusmao_politique_1995,
	title = {La politique fran{\c c}aise de la recherche communautaire : les le{\c c}ons de la mise en {\oe}uvre},
	volume = {13},
	issn = {0758-1726},
	shorttitle = {La politique fran{\c c}aise de la recherche communautaire},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_2_2048},
	doi = {10.3406/pomap.1995.2048},
	abstract = {Dans le cadre du programme-cadre de recherche-d{\'e}veloppement de l'Union Europ{\'e}enne, la mise en {\oe}uvre de la politique de la France est r{\'e}alis{\'e}e par l'action de ses repr{\'e}sentants dans les comit{\'e}s de gestion et de coordination des programmes communautaires. L'article rel{\`e}ve la grande variabilit{\'e}, selon le domaine scientifique consid{\'e}r{\'e}, des conceptions et des pratiques de ces repr{\'e}sentants. Cet {\'e}clatement s'explique par le double positionnement de la mise en {\oe}uvre : en aval dans le d{\'e}roulement de la politique publique, elle r{\'e}alise la m{\'e}diation entre les normes g{\'e}n{\'e}rales d'action fix{\'e}es par le gouvernement fran{\c c}ais et les sp{\'e}cificit{\'e}s du travail fourni par les {\'e}quipes de recherche impliqu{\'e}es dans les contrats europ{\'e}ens ; elle s'inscrit, en outre, dans une organisation segment{\'e}e des op{\'e}rateurs publics et priv{\'e}s de la recherche-d{\'e}veloppement fran{\c c}aise. A partir de l'exemple de la recherche communautaire, on propose de consid{\'e}rer le caract{\`e}re sectoriel d'une politique publique comme une propri{\'e}t{\'e} de sa mise en {\oe}uvre, variant en fonction de la nature des interm{\'e}diaires intervenant entre l'administration et le domaine cible, plut{\^o}t que comme l'attribut d'un type particulier de politique.},
	language = {fr},
	number = {2},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Gusmao, Regina and Charvolin, Florian},
	year = {1995},
	pages = {59--82},
}

@article{balme_europe_1995,
	title = {L'{Europe} en r{\'e}gion : les fonds structurels et la r{\'e}gionalisation de l'action publique en {France} m{\'e}tropolitaine},
	volume = {13},
	issn = {0758-1726},
	shorttitle = {L'{Europe} en r{\'e}gion},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_2_2047},
	doi = {10.3406/pomap.1995.2047},
	abstract = {Ces derni{\`e}res ann{\'e}es, l'int{\'e}gration europ{\'e}enne s'est notamment traduite par une volont{\'e} de la Commission des Communaut{\'e}s Europ{\'e}ennes d'associer plus {\'e}troitement les collectivit{\'e}s locales, et principalement les r{\'e}gions, aux politiques communautaires. En 1989, la r{\'e}forme des fonds structurels, sur lesquels prend appui la politique r{\'e}gionale communautaire, r{\'e}pondait en partie {\`a} cet objectif par le biais de la mise en {\oe}uvre d'un ensemble de r{\`e}gles et de proc{\'e}dures de n{\'e}gociation (subsidiarit{\'e}, additionnante, partenariat, programmation, ...). Cet article tente d'appr{\'e}hender l'impact institutionnel de cette r{\'e}forme sur les modes d'action de la puissance publique dans les r{\'e}gions m{\'e}tropolitaines fran{\c c}aises. Il relativise fortement la place et la fonction de l'institution r{\'e}gionale dans les phases de programmation et de mise en {\oe}uvre des interventions de l'Union Europ{\'e}enne dans le cadre de sa politique r{\'e}gionale. A l'inverse, il insiste, par le biais de la notion de r{\'e}seau politique, sur une recomposition des modes d'action publique b{\'e}n{\'e}ficiant en premier lieu {\`a} l'Etat par le biais de ses administrations d{\'e}concentr{\'e}es au niveau r{\'e}gional et d{\'e}partemental.},
	language = {fr},
	number = {2},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Balme, Richard and Jouve, Bernard},
	year = {1995},
	pages = {35--58},
}

@article{solle_calculs_1995,
	title = {Calculs de co{\^u}ts et management des {\'e}tablissements universitaires},
	volume = {13},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_2_2046},
	doi = {10.3406/pomap.1995.2046},
	abstract = {Les services publics d'enseignement sup{\'e}rieur, en raison de l'autonomie qui leur est conf{\'e}r{\'e}e, doivent pr{\'e}senter leur plan de d{\'e}veloppement strat{\'e}gique qui se traduit de mani{\`e}re concr{\`e}te en objectifs puis en actions de r{\'e}alisation. Ces organismes doivent se doter d'un syst{\`e}me d'information de gestion autant dans une optique pr{\'e}visionnelle que dans une perspective de suivi des r{\'e}alisations, de contr{\^o}le et d'{\'e}valuation. L'introduction de m{\'e}thodes de gestion inspir{\'e}es du secteur commercial et industriel est envisag{\'e}e. Pourtant, elle ne peut {\^e}tre r{\'e}alis{\'e}e sans des adaptations importantes, en raison des sp{\'e}cificit{\'e}s organisationnelles des {\'e}tablissements universitaires et des difficult{\'e}s li{\'e}es {\`a} l'application des techniques de comptabilit{\'e} analytique dans le secteur des services non-marchands. Nous nous interrogeons sur la pertinence d'une lecture de l'organisation en terme de processus d'activit{\'e}s par opposition {\`a} une structure en services ou en divisions. Mais, dans cette hypoth{\`e}se, ne faut-il pas inverser les {\'e}tapes de la r{\'e}novation en centrant la r{\'e}flexion sur de nouveaux modes manag{\'e}riaux au sein des universit{\'e}s ? L'adoption de modalit{\'e}s de gestion pr{\'e}visionnelle et de techniques "simples" de calcul de co{\^u}ts s'impose. Une budg{\'e}tisation {\`a} base d'activit{\'e}s et une comptabilit{\'e} {\`a} base d'activit{\'e}s peuvent alors constituer des {\'e}l{\'e}ments de r{\'e}ponse.},
	language = {fr},
	number = {2},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Solle, Guy},
	year = {1995},
	pages = {1--33},
}

@article{clark_les_1995,
	title = {Les strat{\'e}gies de l'innovation dans les collectivit{\'e}s territoriales : le{\c c}ons internationales},
	volume = {13},
	issn = {0758-1726},
	shorttitle = {Les strat{\'e}gies de l'innovation dans les collectivit{\'e}s territoriales},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_3_2058},
	doi = {10.3406/pomap.1995.2058},
	abstract = {Partant d'une synth{\`e}se du "Projet Aust{\'e}rit{\'e} Fiscale et Innovation Urbaine" ({\'e}tude de 7 000 villes dans 35 pays), ce papier offre des r{\'e}sultats et propose des interpr{\'e}tations toujours sujettes {\`a} controverse. Par exemple : . La pression fiscale peut introduire de l'incertitude, mais ne change pas n{\'e}cessairement les r{\`e}gles du jeu politique. . Quelles sont les strat{\'e}gies adapt{\'e}es pour r{\'e}pondre {\`a} la pression fiscale ? Variantes nationales. . La privatisation r{\'e}alise des {\'e}conomies, uniquement dans le cas des organisations sans but lucratif. . Les changements de politique fondamentale proviennent de "leaders" d'une nouvelle culture politique, conservateurs en ce qui concerne la fiscalit{\'e}, mais progressistes sur les questions sociales. . Le r{\^o}le de la population s'accro{\^i}t, mais varie consid{\'e}rablement selon les villes et les pays. . Les regroupements d'administrations locales ont souvent cr{\'e}{\'e} des co{\^u}ts consid{\'e}rables.},
	language = {fr},
