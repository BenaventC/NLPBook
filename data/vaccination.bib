@article{valette_fabrication_2015,
	title = {La fabrication du {\guillemotleft} m{\'e}dico-{\'e}onomique {\guillemotright} {\`a} l'h{\^o}pital fran{\c c}ais : une analyse de la litt{\'e}rature},
