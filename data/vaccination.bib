	shorttitle = {L'expertise locale en transports urbains},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1988_num_6_1_1990},
	doi = {10.3406/pomap.1988.1990},
	abstract = {La d{\'e}cennie soixante-dix marque sans conteste le renouveau des services urbains de transports en commun, qui deviennent une pr{\'e}occupation majeure de l'administration centrale comme des {\'e}lus. Dans ce ph{\'e}nom{\`e}ne de r{\'e}habilitation politique, {\'e}conomique et id{\'e}ologique d'un mode de d{\'e}placement vou{\'e} quelques ann{\'e}es plus t{\^o}t {\`a} la disparition, l'expertise a jou{\'e} un r{\^o}le consid{\'e}rable. Outil d'innovation organisationnelle, son d{\'e}veloppement s'est explicitement effectu{\'e} comme r{\'e}ponse {\`a} la crise des transports urbains des ann{\'e}es soixante. Ainsi s'est op{\'e}r{\'e}e l'apparition d'un nouveau milieu professionnel, s'opposant point {\`a} point (formation, r{\'e}f{\'e}rences disciplinaires, insertion locale, rapport au politique...) aux expertises traditionnel/es centra/es (Corps des Ing{\'e}nieurs des Ponts et Chauss{\'e}es) et territoriales (Ing{\'e}nieurs des Services Techniques des Villes). Il participe en outre, par les fonctions de m{\'e}diation qu'il remplit, {\`a} la reformulation de la notion de service public. Mais son avenir appara{\^i}t bien incertain, au regard de son absence d'institutionnalisation comme des forces centrifuges qui r{\'e}duisent tout {\`a} la fois son homog{\'e}n{\'e}it{\'e} et sa l{\'e}gitimit{\'e}.},
	language = {fr},
	number = {1},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Offner, Jean-Marc},
	year = {1988},
	pages = {81--102},
}

@article{becquart-leclercq_culture_1988,
