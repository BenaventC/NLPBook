	abstract = {Les r{\'e}formes de modernisation de l{\textquoteright}{\'E}tat font de la r{\'e}novation de la gestion des res- sources humaines l{\textquoteright}une des composantes majeures de la {\guillemotleft} performance {\guillemotright} d{\'e}sormais attendue de la puissance publique. {\`A} partir du cas des Conseillers Techniques Spor- tifs (CTS), agents du Minist{\`e}re des Sports, nous montrons que les dispositions qu{\textquoteright}elles pr{\'e}voient restent limit{\'e}es tant les principes structurant les rapports entre l{\textquoteright}{\'E}tat et les f{\'e}d{\'e}rations sportives heurtent les axes des r{\'e}formes adopt{\'e}es. La position administra- tive des CTS et leur contexte relationnel de travail exacerbent en effet les tensions et paradoxes inh{\'e}rents {\`a} la mise en {\oe}uvre d{\textquoteright}une d{\'e}marche d{\textquoteright}appr{\'e}ciation des profession- nels. La diversit{\'e} des acteurs en pr{\'e}sence, aux int{\'e}r{\^e}ts pouvant {\^e}tre divergents, oriente actuellement la d{\'e}finition de leur parcours professionnel et suscite par l{\`a} m{\^e}me des sentiments d{\textquoteright}injustice et d{\textquoteright}iniquit{\'e}.},
	number = {2},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Honta, Marina and Juhle, Samuel},
	month = jun,
	year = {2014},
	pages = {239--257},
	annote = {gestion des ressources humaines, Conseillers Techniques Sportifs, r{\'e}forme de l{\textquoteright}{\'E}tat, carri{\`e}re.},
}

@article{renault_crowdsourcing_2014,
