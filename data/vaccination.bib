	url = {http://pmp.revuesonline.com/article.jsp?articleId=18106},
	doi = {10.3166/pmp.29.591-606},
	abstract = {La lib{\'e}ralisation des activit{\'e}s ferroviaires a favoris{\'e} autant l{\textquoteright}{\'e}mergence de nouveaux op{\'e}rateurs priv{\'e}s que celle de nouvelles structures publiques de r{\'e}gulation et de contr{\^o}le. L{\textquoteright}une des difficult{\'e}s majeures de ces structures r{\'e}side dans leur ind{\'e}pendance, {\`a} la fois, par rapport aux acteurs du secteur, mais aussi par rapport {\`a} l{\textquoteright}{\'E}tat, historique- ment impliqu{\'e} dans le secteur ferroviaire. Les fonctions essentiellement techniques des structures de r{\'e}gulation peuvent-elles s{\textquoteright}exercer ind{\'e}pendamment des acteurs du secteur, et notamment des acteurs historiques ? L{\textquoteright}{\'e}volution des modalit{\'e}s de r{\'e}gulation du secteur ferroviaire en France, depuis la Mission de Contr{\^o}le des Activit{\'e}s Ferroviaires jusqu{\textquoteright}{\`a} la r{\'e}cente Autorit{\'e} de R{\'e}gulation des Activit{\'e}s Ferroviaires, traduit cette diffi- cult{\'e}. Pour rendre davantage effective l{\textquoteright}ind{\'e}pendance de cette Autorit{\'e}, la France a choisi de lui conf{\'e}rer des pouvoirs plus cons{\'e}quents, au prix cependant de contre-pou- voirs insuffisants.},
	number = {4},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Blanquart, Corinne and Hyard, Alexandra},
	month = mar,
	year = {2012},
	pages = {591--606},
	annote = {Activit{\'e}s ferroviaires, ind{\'e}pendance, France, lib{\'e}ralisation, Union europ{\'e}enne, r{\'e}gulation.},
}

@article{gallie_evaluation_2012,
	title = {{\'E}valuation moderne, {\'e}valuation modeste ? {Le} cas des p{\^o}les de comp{\'e}titivit{\'e} fran{\c c}ais},
	volume = {29},
	issn = {0758-1726},
	shorttitle = {{\'E}valuation moderne, {\'e}valuation modeste ?},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=18105},
	doi = {10.3166/pmp.29.573-590},
	abstract = {L{\textquoteright}{\'e}valuation des politiques publiques est un th{\`e}me r{\'e}current pour les praticiens et cher- cheurs travaillant sur l{\textquoteright}action publique. Mais au-del{\`a} de l{\textquoteright}accord sur le principe, il n{\textquoteright}y a pas stabilisation de la doctrine et de ses pratiques. Diff{\'e}rents mod{\`e}les coexistent ou se suc- c{\`e}dent, selon les pays ou les {\'e}poques. En nous appuyant sur le cas de l{\textquoteright}{\'e}valuation de la politique fran{\c c}aise des p{\^o}les de comp{\'e}titivit{\'e} en 2008, nous montrons les principales difficult{\'e}s de la d{\'e}marche, li{\'e}es au caract{\`e}re syst{\'e}mique, multi-acteurs et h{\'e}t{\'e}rog{\`e}ne de l{\textquoteright}objet {\guillemotleft} cluster {\guillemotright}. {\`A} travers l{\textquoteright}analyse des utilisations de l{\textquoteright}{\'e}valuation, nous mettons ensuite en {\'e}vidence que cette {\'e}valuation participe {\`a} la constitution progressive et simultan{\'e}e d{\textquoteright}une doctrine des clusters et de leur pilotage. En conclusion, nous tra{\c c}ons les bases d{\textquoteright}un mod{\`e}le de d{\'e}marche {\'e}valuative, qui la d{\'e}sacralise et l{\textquoteright}int{\`e}gre {\`a} un dispositif plus large de pilotage des politiques publiques.},
	number = {4},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Galli{\'e}, {\'E}milie-Pauline and Glaser, Anna and Lefevre, Philippe and Pallez, Fr{\'e}d{\'e}rique},
	month = mar,
	year = {2012},
	pages = {573--590},
	annote = {cluster, {\'e}valuation, politiques publiques, p{\^o}le de comp{\'e}titivit{\'e}.},
}

@article{brami_reformes_2012,
