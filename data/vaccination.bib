	issn = {0758-1726, 2119-4831},
	shorttitle = {Gestion publique},
	url = {http://journals.openedition.org/pmp/1534},
	doi = {10.4000/pmp.1534},
	abstract = {Le paysage institutionnel fran{\c c}ais a subi depuis 25 ans, et surtout ces derni{\`e}res ann{\'e}es, de profondes inflexions dans trois domaines. Le premier est le passage progressif d{\textquoteright}un {\'E}tat unitaire {\`a} une forme hybride mi-d{\'e}centralis{\'e}e mi-d{\'e}concentr{\'e}e, qui t{\'e}moigne de la permanence d{\textquoteright}une exception fran{\c c}aise au sein de l{\textquoteright}Europe. A c{\^o}t{\'e} d{\textquoteright}un {\'E}tat central qui entend contr{\^o}ler la plus grande part des fonds publics, une kyrielle de lieux de pouvoirs locaux s{\textquoteright}{\'e}chinent {\`a} tirer le maximum de la manne financi{\`e}re en provenance de Paris. Le second changement porte sur les r{\`e}gles de gestion avec comme fait majeur l{\textquoteright}instauration de la Lolf. Mais les transformations les moins visibles, il faut les chercher dans le quotidien de l{\textquoteright}action publique dont les m{\'e}thodes ont {\'e}t{\'e} progressivement bouscul{\'e}es par l{\textquoteright}introduction de multiples outils de gestion associ{\'e}e {\`a} la recherche de la performance publique. A quels rythmes ces r{\'e}formes ont-elles progress{\'e} ? Quelle est la port{\'e}e des changements institu{\'e}s ? Quels en sont les freins vers une gestion publique efficace au service des citoyens ? L{\textquoteright}article brosse de ce large tableau un bilan contrast{\'e}...},
	language = {fr},
	number = {Vol. 26/3},
	urldate = {2020-06-26},
	journal = {Politiques et Management Public},
	author = {Demeest{\`e}re, Ren{\'e} and Orange, G{\'e}rald},
	month = oct,
	year = {2008},
	pages = {127--147},
	annote = {comp{\'e}tences, contr{\^o}le de gestion, d{\'e}centralisation, d{\'e}concentration, gestion publique, indicateurs, Lolf, management, performance},
}

@article{biondi_quantification_2008,
	title = {De la quantification comptable et financi{\`e}re dans le secteur public : promesses et usages de la gestion par les r{\'e}sultats},
	issn = {0758-1726, 2119-4831},
	shorttitle = {De la quantification comptable et financi{\`e}re dans le secteur public},
	url = {http://journals.openedition.org/pmp/1532},
	doi = {10.4000/pmp.1532},
	abstract = {Regroup{\'e}es sous le label de {\guillemotleft} nouvelle gestion publique {\guillemotright}, de nombreuses r{\'e}formes des administrations publiques sont exp{\'e}riment{\'e}es depuis une trentaine d{\textquoteright}ann{\'e}es dans les pays de l{\textquoteright}OCDE et ailleurs. Ces r{\'e}formes se traduisent par la mise en {\oe}uvre d{\textquoteright}instruments de gestion et sont porteuses de promesses de rationalisation, d{\'e}mocratisation et performance accrue. Pourtant, comme le souligne l{\textquoteright}OCDE dans une tentative de bilan partiel en 2005, {\guillemotleft} la r{\'e}alit{\'e} de la r{\'e}forme n{\textquoteright}a pas {\'e}t{\'e} {\`a} la hauteur du discours {\guillemotright}. Les promesses non tenues sugg{\`e}rent donc de questionner ces dispositifs afin d{\textquoteright}en percevoir les r{\^o}les sp{\'e}cifiques dans les organisations et la soci{\'e}t{\'e}. Cette compr{\'e}hension avertie des dispositifs de gestion en situation {\'e}claire leur impact sur les choix et les comportements et leur capacit{\'e} {\`a} rendre possibles, voire n{\'e}cessaires, des choix politiques qu{\textquoteright}ils ne sauront remplacer.},
	language = {fr},
	number = {Vol. 26/3},
	urldate = {2020-06-26},
	journal = {Politiques et Management Public},
	author = {Biondi, Yuri and Chatelain-Ponroy, St{\'e}phanie and Sponem, Samuel},
	month = oct,
