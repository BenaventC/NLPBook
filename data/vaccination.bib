	number = {3},
	urldate = {2020-06-29},
	journal = {Politiques et management public},
	author = {Michel, Herv{\'e} and Jourdain, Alain},
	month = sep,
	year = {2011},
	pages = {279--309},
	annote = {h{\^o}pital, planification, r{\'e}gionalisation, SROS, ARH, ARS, politiques de sant{\'e}, territoire de sant{\'e}, {\'E}tat r{\'e}gulateur, r{\'e}gulation, syst{\`e}me hospitalier.},
