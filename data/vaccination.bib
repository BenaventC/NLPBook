	abstract = {Dans le champ de l{\textquoteright}action sociale, s{\textquoteright}opposent deux logiques institutionnelles, celle de l{\textquoteright}innovation et celle de la rationalisation budg{\'e}taire. Elles semblent de prime abord inconciliables. La premi{\`e}re, porteuse de diversit{\'e} et d{\textquoteright}idiosyncrasie, serait l{\textquoteright}apanage des associations. La seconde donnerait la part belle aux financeurs publics en visant l{\textquoteright}homog{\'e}n{\'e}isation et la transparence de l{\textquoteright}action associative. Le caract{\`e}re irr{\'e}ductible de cette opposition dualiste est-il av{\'e}r{\'e}? L{\textquoteright}article {\'e}tudie les interactions entre les deux logiques {\`a} travers l{\textquoteright}{\'e}tude du cas d{\textquoteright}une association et des modalit{\'e}s de son financement. Il s{\textquoteright}attache, tout d{\textquoteright}abord, {\`a} d{\'e}crire les formes concr{\`e}tes des relations association-financeurs dans le cas {\'e}tudi{\'e}. Puis, il analyse les processus temporels et multi-acteurs, montrant le d{\'e}passement de l{\textquoteright}apparente incompatibilit{\'e} entre les deux logiques.},
	number = {4},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Gilbert, Patrick and Raulet-Croset, Nathalie and Chemin-Bouzir, Carine},
	month = mar,
	year = {2012},
	pages = {607--627},
	annote = {logique institutionnelle, innovation sociale, financement, dispositif de gestion, action sociale.},
}

@article{blanquart_lindependance_2012,
