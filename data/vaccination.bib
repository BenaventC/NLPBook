@article{mattijs_crise_2019,
	title = {Crise de l{\'e}gitimit{\'e} et changement de l{\textquoteright}action publique : des discours aux pratiques},
	volume = {36},
	shorttitle = {Crise de l{\'e}gitimit{\'e} et changement de l{\textquoteright}action publique},
	doi = {10.3166/pmp.36.2019.0009},
	abstract = {En ao{\^u}t 2017, {\`a} Milan, s{\textquoteright}est d{\'e}roul{\'e}e la conf{\'e}rence annuelle du Groupe Europ{\'e}en d{\textquoteright}Admi- nistration Publique (GEAP, ou EGPA en anglais), organis{\'e}e pour la 39e fois par l{\textquoteright}Institut International des Science Administratives (IISA). Ce gros {\'e}v{\'e}nement rassemble pr{\`e}s de 500 participants acad{\'e}miques mais aussi praticiens, avec une bonne repr{\'e}sentation de toute l{\textquoteright}Europe, venus suivre les travaux de pr{\`e}s de 30 groupes de travail permanents. L{\textquoteright}IISA a maintenu {\'e}galement des activit{\'e}s scientifiques en fran{\c c}ais, {\`a} travers un {\guillemotleft} S{\'e}minaire francophone {\guillemotright} qui accueille chaque ann{\'e}e les communications de chercheurs venus pr{\'e}senter leurs travaux dans leur langue, mais aussi avec des perspectives qui peuvent diff{\'e}rer des travaux pr{\'e}sent{\'e}s dans les ateliers th{\'e}matiques. Au s{\'e}minaire s{\textquoteright}ajoute une table ronde organis{\'e}e avec le soutien de l{\textquoteright}IGPDE (Institut de la Gestion Publique et du D{\'e}veloppement {\'E}conomique, op{\'e}rateur de formation permanente du minist{\`e}re de l{\textquoteright}{\'E}conomie et des Finances fran{\c c}ais) qui confronte les points de vue de praticiens et des chercheurs sur une th{\'e}matique apparent{\'e}e au s{\'e}minaire. N{\textquoteright}{\'e}tant pas tenus par une th{\'e}matique permanente, le s{\'e}minaire francophone comme la table ronde peuvent orienter leurs travaux en fonction des tendances de l{\textquoteright}actualit{\'e}. C{\textquoteright}est ainsi qu{\textquoteright}en 2017, l{\textquoteright}appel {\`a} communications s{\textquoteright}intitulait {\guillemotleft} Retournements de ten- dances : la gestion publique {\`a} l{\textquoteright}{\'e}preuve des tourments de l{\textquoteright}opinion {\guillemotright}, r{\'e}agissant au vote du Brexit et {\`a} l{\textquoteright}{\'e}lection de Donald Trump. Des th{\'e}matiques voisines ont {\'e}t{\'e} explor{\'e}es en 2018 (instabilit{\'e} et innovation) et le seront encore en 2019 ({\guillemotleft} d{\'e}connexions {\guillemotright} politiques et administratives), tant il est vrai que les r{\'e}gimes politiques lib{\'e}raux ou social-d{\'e}mo- crates semblent maintenant les sujets d{\textquoteright}une remise en cause profonde de leur l{\'e}gitimit{\'e}. Les articles du pr{\'e}sent dossier sp{\'e}cial de Politiques et Management Public sont le reflet de ces travaux : la r{\'e}flexion de Robin Degron pr{\'e}parait les {\'e}changes de la table ronde, tandis que les quatre autres contributions, issues de travaux scientifiques de terrain, ont {\'e}t{\'e} discut{\'e}es en atelier lors du s{\'e}minaire de Milan.},
	journal = {Politiques et management public},
	author = {Mattijs, Jan and Turc, Emil},
	month = jun,
	year = {2019},
	pages = {121--131},
}

