	url = {http://pmp.revuesonline.com/article.jsp?articleId=19320},
	doi = {10.3166/pmp.31.31-50},
	abstract = {Cet article questionne la place de l{\textquoteright}{\'e}valuation de politique publique dans un contexte d{\textquoteright}exp{\'e}rimentations sociales par l{\textquoteright}{\'e}tude de cas des exp{\'e}rimentations du RSA. On analyse comment le lancement de l{\textquoteright}exp{\'e}rimentation et de son {\'e}valuation, sa mise en {\oe}uvre et ses usages sont marqu{\'e}s par des int{\'e}r{\^e}ts d{\textquoteright}acteurs, des dynamiques institutionnelles et des syst{\`e}mes de croyances. La d{\'e}marche se trouve n{\'e}goci{\'e}e et investie par les acteurs, et l{\textquoteright}{\'e}valuation contrainte par des enjeux politiques, orientant ainsi la connaissance qu{\textquoteright}elle produit. Celle-ci vient alors apporter des preuves de l{\textquoteright}acceptabilit{\'e} sociale du dispositif, de sa faisabilit{\'e}, de son op{\'e}rationnalit{\'e} et de son efficacit{\'e} aupr{\`e}s des d{\'e}cideurs. Ses r{\'e}sul- tats constituent un argument politique fort l{\'e}gitimant le mode de production de l{\textquoteright}action publique, le dispositif exp{\'e}riment{\'e} et les acteurs qui le mettent en {\oe}uvre. Cela am{\`e}ne {\`a} questionner comment l{\textquoteright}instrumentation (voire l{\textquoteright}instrumentalisation) de l{\textquoteright}exp{\'e}rimentation et de l{\textquoteright}{\'e}valuation, contribue {\`a} modifier la mani{\`e}re d{\textquoteright}{\'e}laborer, de l{\'e}gitimer et de conduire l{\textquoteright}action publique.},
	number = {1},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Okbani, Nadia},
	month = mar,
	year = {2014},
	pages = {31--50},
	annote = {{\'e}valuation, exp{\'e}rimentation sociale, m{\'e}thode d{\textquoteright}{\'e}valuation exp{\'e}rimentale, RSA, instrument d{\textquoteright}action public.},
}

@article{barbier_les_2014,
	title = {Les sources intellectuelles de l{\textquoteright}{\'e}valuation des politiques publiques},
	volume = {31},
	issn = {0758-1726},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=19319},
	doi = {10.3166/pmp.31.13-30},
	abstract = {L{\textquoteright}{\'e}valuation des politiques publiques est une {\guillemotleft} discipline {\guillemotright} d{\'e}licate {\`a} situer dans le paysage du management public. Routinis{\'e}e dans certains pays o{\`u} elle dispose de r{\'e}f{\'e}rences profes- sionnelles, elle est moins fermement identifi{\'e}e dans d{\textquoteright}autres. Dans les deux cas cependant, elle s{\textquoteright}ins{\`e}re dans une famille d{\textquoteright}activit{\'e}s qui traitent d{\textquoteright}information {\'e}valuative. Le contr{\^o}le de gestion est une discipline voisine. On comprend mieux la coh{\'e}rence de l{\textquoteright}{\'e}valuation de poli- tiques publiques en remontant {\`a} ses sources intellectuelles. Historiquement, la premi{\`e}re est la forme d{\textquoteright}exp{\'e}rimentalisme d{\'e}velopp{\'e}e par Donald Campbell, dont il reste dans l{\textquoteright}{\'e}va- luation pratiqu{\'e}e aujourd{\textquoteright}hui comme une nostalgie scientiste. Mais le management public, et en particulier sa version dite {\guillemotleft} nouvelle gestion publique {\guillemotright} constituent le milieu d{\textquoteright}accueil de la discipline. Comme le management, l{\textquoteright}{\'e}valuation allie un {\guillemotleft} {\'e}tat-de-l{\textquoteright}art {\guillemotright} transnational {\`a} des pratiques nationales diversifi{\'e}es.},
	number = {1},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Barbier, Jean-Claude},
	month = mar,
	year = {2014},
	pages = {13--30},
	annote = {exp{\'e}rimentalisme, {\'e}valuation, politiques publiques, management public.},
}

