	volume = {1},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1983_num_1_2_1724},
	doi = {10.3406/pomap.1983.1724},
	abstract = {Le d{\'e}veloppement de la vie associative est souvent consid{\'e}r{\'e} comme un enrichissement de la d{\'e}mocratie locale. Ne constitue-t-il pas pourtant l'un des instruments privil{\'e}gi{\'e}s du nouveau {\guillemotleft}pouvoir local{\guillemotright} ? L'analyse compar{\'e}e des strat{\'e}gies municipales {\`a} l'{\'e}gard des associations dans des communes de banlieue en France et aux Etats-Unis permet de montrer comment ces organisations volontaires participent surtout {\`a} la l{\'e}gitimation fonctionnelle du pouvoir. Le recours par les {\'e}lus {\`a} des formes de consultation et de concertation avec les associations r{\'e}pond le plus souvent au souci d'organiser un contr{\^o}le et une segmentation de la demande locale et d'assurer la l{\'e}gitimit{\'e} des d{\'e}cisions municipales. Mais ces strat{\'e}gies du {\guillemotleft}partnership{\guillemotright} ou de l'{\'e}coute s{\'e}lective sont assorties, en France surtout, de nombreuses {\guillemotleft}d{\'e}viations{\guillemotright} : l'appartenance de conseillers punicipaux aux bureaux de certaines associations, le carcan des proc{\'e}dures de participation et la {\guillemotleft}satellisation{\guillemotright} municipale de la plupart des organisations de services et d'animation transforment les groupes associatifs en v{\'e}ritables relais de la municipalit{\'e}. Pour l'auteur, ce n'est que par effet d'illusion que certains ont pu voir dans cette {\guillemotleft}d{\'e}multiplication{\guillemotright} le d{\'e}veloppement d'une pluralit{\'e} des lieux du pouvoir.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Dressayre, Philippe},
	year = {1983},
	pages = {27--50},
}

