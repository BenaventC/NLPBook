	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1988_num_6_4_2025},
	doi = {10.3406/pomap.1988.2025},
	abstract = {Ayant {\'e}largi significativement le secteur public {\`a} des groupes compl{\`e}tement int{\'e}gr{\'e}s dans la concurrence internationale par la loi de nationalisation de 1982, les Socialistes fran{\c c}ais affichent leur souci de distinguer nationalisation et {\'e}tatisation. Ils affirment par ailleurs vouloir concilier autonomie de gestion et respect des grandes orientations {\'e}conomiques et sociales fix{\'e}es dans le Plan national. La contractualisation des rapports permettra de hi{\'e}rarchiser les objectifs de la Puissance Publique et ceux de l'entreprise et d'accorder {\`a} cette derni{\`e}re des contreparties {\`a} sa soumission {\`a} l'int{\'e}r{\^e}t g{\'e}n{\'e}ral. La proc{\'e}dure {\textbackslash}textlessr contrat de plan {\guillemotright} sera ouverte par J.P. CHEVENEMENT en juillet 1982 et sera prise en charge par une structure administrative ad hoc, le Service des Entreprises Nationales. Le contenu des contrats de plan est tr{\`e}s pauvre. Cependant si leur {\'e}laboration n'a pas donn{\'e} aux Pouvoirs Publics la possibilit{\'e} d'infl{\'e}chir les strat{\'e}gies industrielles men{\'e}es par les groupes nationalis{\'e}s, elle leur a permis d'en avoir une connaissance pr{\'e}cise et de n{\'e}gocier en d'autres lieux (r{\'e}unions restreintes) des amendements de strat{\'e}gies et leur contrepartie financi{\`e}re (sous forme de dotations en capital ou de pr{\^e}ts participatifs).},
	language = {fr},
	number = {4},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Barreau, Jocelyne and Hardy, Jacques},
	year = {1988},
	pages = {25--47},
}

@article{bonaiti_politiques_1988,
	title = {Politiques nationales de ma{\^i}trise de l'{\'e}nergie dans l'habitat : une innovation {\`a} l'{\'e}preuve du terrain},
	volume = {6},
	issn = {0758-1726},
