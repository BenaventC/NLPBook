	author = {Dessertines, Philippe and Gibert, Patrick},
	year = {2007},
	pages = {57--82},
}

@article{segas_proximite_2007,
	title = {La proximit{\'e} : nouveau p{\'e}rim{\`e}tre du public ? {L}'exemple des politiques d'am{\'e}nagement territorialis{\'e}es},
	volume = {25},
