@article{chatelain-ponroy_lopposition_2013,
	title = {De l{\textquoteright}opposition {\guillemotleft} politiques/administratifs {\guillemotright} au clivage {\guillemotleft} centre/p{\'e}riph{\'e}rie {\guillemotright} : les divergences de perception des outils de mesure dans les universit{\'e}s},
	volume = {30},
	issn = {0758-1726},
	shorttitle = {De l{\textquoteright}opposition {\guillemotleft} politiques/administratifs {\guillemotright} au clivage {\guillemotleft} centre/p{\'e}riph{\'e}rie {\guillemotright}},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=18981},
	doi = {10.3166/pmp.30.495-518},
	abstract = {Les outils de mesure sont au c{\oe}ur de la gestion et du pilotage des universit{\'e}s. On sait pour- tant peu de chose sur la mani{\`e}re dont ils sont utilis{\'e}s et per{\c c}us par les acteurs qui constituent la communaut{\'e} universitaire. Cet article a ainsi pour objectif de pr{\'e}senter un {\'e}tat des lieux de ces perceptions et de ces usages et de mettre en {\'e}vidence les diff{\'e}rences qui peuvent exister entre diff{\'e}rentes cat{\'e}gories d{\textquoteright}acteurs, en travaillant en particulier sur la distinction qu{\textquoteright}ils {\'e}ta- blissent eux-m{\^e}mes entre {\guillemotleft} politique {\guillemotright} et {\guillemotleft} administratif {\guillemotright}. Pour cela nous chercherons {\`a} quali- fier les usages des outils de mesure en distinguant notamment les usages {\`a} vis{\'e}e de contr{\^o}le diagnostique, contr{\^o}le interactif, les usages {\`a} vis{\'e}e d{\textquoteright}aide {\`a} la d{\'e}cision et de rationalisation (Simons, 1995 ; Burchell et al., 1980). Nous {\'e}tudierons aussi la perception des acteurs concernant la diffusion de ces dispositifs. Cet {\'e}tat des lieux s{\textquoteright}appuie sur une enqu{\^e}te par questionnaires, r{\'e}alis{\'e}e aupr{\`e}s de toutes les universit{\'e}s fran{\c c}aises, {\`a} laquelle ont r{\'e}pondu 2 598 personnes exer{\c c}ant des fonctions {\`a} responsabilit{\'e}s ou de gouvernance, qu{\textquoteright}elles soient enseignants et/ou chercheurs ou administratifs. Nos r{\'e}sultats montrent que si la fonction de l{\'e}gitimation semble pr{\'e}valoir aux yeux de nos r{\'e}pondants sur celles de contr{\^o}le diagnostique, de prise de d{\'e}cision et de contr{\^o}le interactif, des variations notables sont observ{\'e}es suivant le type de donn{\'e}es pro- duites mais aussi les cat{\'e}gories d{\textquoteright}acteurs. Nos r{\'e}sultats sugg{\`e}rent que plus que la distinction entre les {\guillemotleft} administratifs {\guillemotright} et les {\guillemotleft} politiques {\guillemotright}, la perception des mesures d{\'e}pend du fait d{\textquoteright}{\^e}tre un acteur {\guillemotleft} central {\guillemotright} ou {\guillemotleft} p{\'e}riph{\'e}rique {\guillemotright}.},
	number = {4},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Chatelain-Ponroy, St{\'e}phanie and Mignot-G{\'e}rard, St{\'e}phanie and Musselin, Christine and Sponem, Samuel},
	month = dec,
	year = {2013},
	pages = {495--518},
	annote = {universit{\'e}s, outils de mesure, politiques, administratifs.},
}

