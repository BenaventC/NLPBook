@article{le_pen_evaluation_1983-1,
	title = {Une {\'e}valuation du co{\^u}t des politiques de cr{\'e}ation d'emplois publics},
	volume = {1},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1983_num_1_1_1706},
	doi = {10.3406/pomap.1983.1706},
	abstract = {Cet article est une tentative d'{\'e}valuer, th{\'e}oriquement et empiriquement, l'effet {\'e}conomique d'une politique publique de stabilisation par l'emploi public. En utilisant le cadre th{\'e}orique de Bacon et Eltis qui renvoient le probl{\`e}me {\'e}conomique du monde occidental aux d{\'e}penses excessives des administrations publiques et suite {\`a} leur division de l'{\'e}conomie en un secteur productif et un secteur improductif, nous montrons qu'un d{\'e}placement de l'emploi public implique un double co{\^u}t pour le secteur productif : un "co{\^u}t financier" {\'e}gal aux ressources absorb{\'e}es de fa{\c c}on improductive et un co{\^u}t "r{\'e}el" correspondant au co{\^u}t d'opportunit{\'e} de la valeur non g{\'e}n{\'e}r{\'e}e dans les secteurs productifs par les travailleurs transf{\'e}r{\'e}s. N{\'e}anmoins, la mesure empirique de ces deux co{\^u}ts pour les ann{\'e}es 1969-1978 en France, montre que cette charge n'est pas susceptible d'{\^e}tre responsable du ralentissement de la croissance {\'e}conomique, au moins {\`a} court terme : le co{\^u}t financier est en effet tr{\`e}s inf{\'e}rieur au "co{\^u}t apparent" (salaires) de l'emploi public puisque les agents publics assument une part de leur propre financement. De plus, dans une situation d'offre exc{\'e}dentaire sur le march{\'e} du travail, le co{\^u}t d'opportunit{\'e} tend {\`a} diminuer {\`a} un niveau proche de z{\'e}ro.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Le Pen, Claude},
	year = {1983},
	pages = {25--47},
}

@article{anastassopoulos_entreprises_1983-1,
	title = {Entreprises publiques et d{\'e}veloppement},
	volume = {1},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1983_num_1_1_1707},
	doi = {10.3406/pomap.1983.1707},
	abstract = {Dans la plupart des PMA, en particulier dans ceux qui s'industrialisent r{\'e}ellement, le secteur public est tr{\`e}s important et joue un r{\^o}le cl{\'e} dans le processus de d{\'e}veloppement. L'article se concentre sur les grandes entreprises publiques, et met particuli{\`e}rement l'accent sur la relation entre les strat{\'e}gies d'entreprise et les politiques nationales. Il est issu d'une enqu{\^e}te sur quatre grands secteurs du Br{\'e}sil et du Mexique : l'{\'e}lectricit{\'e}, le p{\'e}trole, l'acier et les t{\'e}l{\'e}communications. La premi{\`e}re partie montre qu'au-del{\`a} de leurs diff{\'e}rences id{\'e}ologiques et historiques, le Br{\'e}sil et le Mexique suivent des mod{\`e}les {\'e}conomiques de d{\'e}veloppement tr{\`e}s similaires, qui sont centr{\'e}s sur trois acteurs : les entreprises nationales priv{\'e}es, les multinationales ({\'e}trang{\`e}res) et les entreprises publiques. Les premiers sont trop faibles, les seconds sont confront{\'e}s {\`a} un nationalisme fort, et les derniers repr{\'e}sentent donc in{\'e}vitablement une tr{\`e}s grande partie de l'activit{\'e} {\'e}conomique globale. La deuxi{\`e}me partie analyse la strat{\'e}gie des entreprises publiques et montre comment les politiques nationales interagissent avec elles dans un processus dialectique. Le r{\'e}sultat de ce processus est consid{\'e}r{\'e} comme largement d{\'e}termin{\'e} par trois logiques diff{\'e}rentes : le stade de d{\'e}veloppement du pays est plus important dans la d{\'e}termination des objectifs strat{\'e}giques g{\'e}n{\'e}raux et du financement des investissements, tandis que les caract{\'e}ristiques particuli{\`e}res d'un secteur d'activit{\'e} donn{\'e} influencent davantage le march{\'e} des produits et les strat{\'e}gies technologiques et industrielles, les relations sociales et la l{\'e}gitimit{\'e} sociale {\'e}tant d{\'e}termin{\'e}es, enfin et surtout, par la culture nationale sp{\'e}cifique de chaque pays. En conclusion, l'article affirme que si les entreprises publiques savent mieux que quiconque quels doivent {\^e}tre les bons choix technico-{\'e}conomiques pour elles, et si les gouvernements doivent {\^e}tre responsables des grands choix politiques, l'interface entre les deux est mal organis{\'e}e, en raison d'un manque de comp{\'e}tences manag{\'e}riales dans les deux pays. Elle sugg{\`e}re donc que de nouveaux outils, tels que les contrats de plan fran{\c c}ais et d'autres, soient exp{\'e}riment{\'e}s dans ces pays.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Anastassopoulos, Jean-Pierre and Blanc, Georges},
	year = {1983},
	pages = {49--84},
}

