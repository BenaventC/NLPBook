	pages = {109--129},
}

@article{zagrodzki_douze_2007,
	title = {Douze ans d'une r{\'e}forme du {Los} {Angeles} {Police} {Department}},
	volume = {25},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_2007_num_25_1_2359},
	doi = {10.3406/pomap.2007.2359},
	abstract = {Le passage {\`a} tabac de Rodney King par quatre policiers, puis les {\'e}meutes de 1992 ont fait de la r{\'e}forme du Los Angeles Police Department (LAPD) un probl{\`e}me politique prioritaire. Le maire de la ville, Thomas Bradley, nomme une commission ind{\'e}pendante pour formuler des mesures concr{\`e}tes afin d'am{\'e}liorer les m{\'e}thodes et l'image paramilitaires du LAPD. La commission d{\'e}cide de faire appliquer le community policing, m{\'e}thode polici{\`e}re bas{\'e}e sur la proximit{\'e} et la pr{\'e}vention, {\`a} Los Angeles. Si les {\'e}v{\'e}nements cit{\'e}s plus haut ont permis d'inscrire la r{\'e}forme du LAPD sur l'agenda politique, son application a {\'e}t{\'e} rendue plus difficile du fait de diff{\'e}rentes contraintes organisationnelles et de la perception du community policing par les policiers comme un artifice politique destin{\'e} {\`a} calmer les attentes de l'{\'e}lectorat. Par ailleurs, la succession de chefs {\`a} la t{\^e}te du LAPD depuis une douzaine d'ann{\'e}es a emp{\^e}ch{\'e} une v{\'e}ritable continuit{\'e} de la r{\'e}forme, le community policing devenant plus un affichage politique invoqu{\'e} par les {\'e}lus et la hi{\'e}rarchie polici{\`e}re qu'une v{\'e}ritable priorit{\'e}.},
	language = {fr},
	number = {1},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Zagrodzki, Mathieu},
	year = {2007},
	pages = {85--108},
}

@article{trivin_comment_2007,
	title = {Comment mesurer les activit{\'e}s pharmaceutiques hospitali{\`e}res ? {Validation} via une d{\'e}marche processus d'indicateurs et de tableaux de bord},
	volume = {25},
	issn = {0758-1726},
	shorttitle = {Comment mesurer les activit{\'e}s pharmaceutiques hospitali{\`e}res ?},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_2007_num_25_1_2358},
	doi = {10.3406/pomap.2007.2358},
	abstract = {La politique d'{\'e}valuation des {\'e}tablissements de sant{\'e} est aujourd'hui renforc{\'e}e par la constitution des p{\^o}les d'activit{\'e} m{\'e}dicale et la contractualisation interne et externe. Les activit{\'e}s pharmaceutiques ne sont pas int{\'e}gr{\'e}es dans le Programme de M{\'e}dicalisation du Syst{\`e}me d'Information. Or elles ne sont pas exemptes de ce bouleversement des modes d'organisation et de management hospitaliers, dans la mesure o{\`u} elles repr{\'e}sentent une part toujours croissante des d{\'e}penses budg{\'e}taires des {\'e}tablissements. Dans ce contexte, il est indispensable pour les pharmaciens de cr{\'e}er un outil de comparaison externe ({\guillemotleft} Benchmarking {\guillemotright}) et interne des activit{\'e}s r{\'e}alis{\'e}es au sein d'une Pharmacie {\`a} Usage Int{\'e}rieur. Apr{\`e}s analyse des diverses exp{\'e}riences existantes en France, nous avons appliqu{\'e} une m{\'e}thodologie de gestion par processus des activit{\'e}s pharmaceutiques hospitali{\`e}res. Des indicateurs d'activit{\'e} pertinents ont {\'e}t{\'e} s{\'e}lectionn{\'e}s au moyen d'un {\guillemotleft} Delphi modifi{\'e} {\guillemotright} et rassembl{\'e}s dans des tableaux de bord relatifs {\`a} neuf processus pharmaceutiques fondamentaux. Ces tableaux de bord, outils de pilotage permettant une mesure quantitative et qualitative des activit{\'e}s exerc{\'e}es, sont d{\'e}sormais utilis{\'e}s en pratique courante au sein du D{\'e}partement Pharmacie du Centre Hospitalier Universitaire de Grenoble.},
	language = {fr},
	number = {1},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Trivin, Caroline and Allenet, Beno{\^i}t and Vinot, Didier and Marie, Fr{\'e}d{\'e}rick and Calop, Jean},
	year = {2007},
	pages = {65--83},
}

@article{pflieger_gestion_2007,
	title = {Gestion urbaine de l'eau : de l'usager au consommateur vers une primaut{\'e} des normes sur le politique},
	volume = {25},
	issn = {0758-1726},
	shorttitle = {Gestion urbaine de l'eau},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_2007_num_25_1_2357},
	doi = {10.3406/pomap.2007.2357},
	abstract = {Dans quelle mesure la transformation du r{\^o}le des usagers des services urbains t{\'e}moigne d'une mutation plus profonde de la place du politique dans la gestion des services ? Quel est le degr{\'e} d'int{\'e}gration entre les activit{\'e}s de gestion des services urbains et les politiques locales ? Quelle est la place du d{\'e}bat et de la responsabilit{\'e} politique locale, dans une gestion urbaine de plus en plus encadr{\'e}e par les normes et les revendications standardis{\'e}es des usagers-consommateurs ? Au plan national, le processus concomitant de renforcement de la figure du consommateur et de normalisation de la gestion des services locaux contribue {\`a} disqualifier le pouvoir local et {\`a} limiter les marges de manoeuvre des collectivit{\'e}s. En parall{\`e}le, au plan local, l'{\'e}tude des comit{\'e}s d'usagers de l'eau montre que la r{\'e}duction de la place du politique par les r{\`e}gles et les normes s'accompagne d'un evitement d'une participation citoyenne par les collectivit{\'e}s elles-m{\^e}mes. Or, en situation de crise, la consolidation du cadre r{\'e}glementaire national d'une part, et les d{\'e}faillances de la concertation entre collectivit{\'e}s et usagers, d'autre part, placent les d{\'e}cideurs locaux devant de vives controverses qui r{\'e}v{\`e}lent une prise en {\'e}tau du politique local, entre normes et redevabilit{\'e}.},
	language = {fr},
	number = {1},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Pflieger, G{\'e}raldine},
	year = {2007},
	pages = {43--63},
}

@article{poupeau_agence_2007,
	title = {L'agence locale comme outil d'une politique d'efficacit{\'e} {\'e}nerg{\'e}tique ? {Deux} {\'e}tudes de cas en {Grande}-{Bretagne}},
	volume = {25},
	issn = {0758-1726},
	shorttitle = {L'agence locale comme outil d'une politique d'efficacit{\'e} {\'e}nerg{\'e}tique ?},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_2007_num_25_1_2356},
	doi = {10.3406/pomap.2007.2356},
	abstract = {Parmi les moyens mis en {\oe}uvre par les villes europ{\'e}ennes pour favoriser l'efficacit{\'e} {\'e}nerg{\'e}tique, les agences de l'{\'e}nergie ont connu un r{\'e}el succ{\`e}s. A partir de deux {\'e}tudes de cas, cet article rend compte de la mani{\`e}re dont ces structures se sont ins{\'e}r{\'e}es dans leur tissu local. Il constate que leur greffe repose sur une logique de d{\'e}veloppement {\guillemotleft} extensive {\guillemotright} et {\guillemotleft} pacifi{\'e}e {\guillemotright}, qui se caract{\'e}rise par la mise en {\oe}uvre d'actions certes innovantes mais qui se juxtaposent plus qu'elles ne s'articulent aux initiatives des acteurs existants, et dont la contribution {\`a} l'efficacit{\'e} {\'e}nerg{\'e}tique est pourtant d{\'e}cisive. L'article met alors en {\'e}vidence que ce cloisonnement n'est pas le seul fait de la r{\'e}ticence - traditionnelle - de ces acteurs. Il tient {\'e}galement aux effets des strat{\'e}gies d{\'e}ploy{\'e}es par les agences pour assurer leur survie. La mise {\`a} jour de ces dynamiques {\guillemotleft} endog{\`e}nes {\guillemotright} permet d'{\'e}valuer d'une autre mani{\`e}re l'impact de ces structures et de montrer les limites inh{\'e}rentes {\`a} ce mod{\`e}le d'action publique dans sa pr{\'e}tention {\`a} diffuser plus largement les questions d'efficacit{\'e} {\'e}nerg{\'e}tique.},
	language = {fr},
	number = {1},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Poupeau, Fran{\c c}ois-Mathieu},
	year = {2007},
	pages = {19--41},
}

@article{mattina_transformation_2007,
	title = {La transformation des politiques client{\'e}laires de redistribution. {Nouvelle} gestion urbaine et pratiques politiques {\`a} {Marseille} et {\`a} {Naples}},
	volume = {25},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_2007_num_25_1_2355},
	doi = {10.3406/pomap.2007.2355},
	abstract = {Dans un contexte de renouvellement des politiques urbaines et du leadership, deux villes de l'Europe du sud, comme Naples et Marseille, ont connu un changement profond des modes de r{\'e}gulation, de gouvernement urbain et de gestion des institutions. Des nouveaux leaderships politiques incarn{\'e}s dans les ann{\'e}es 1990 par les nouveaux maires Antonio Bassolino et Robert Vigouroux ont remis en cause les traditionnelles politiques de redistribution client{\'e}laires. S'ouvre ainsi un espace de r{\'e}flexion sur les transformations des pratiques politiques et du m{\'e}tier d'{\'e}lu local en relation avec le r{\'e}tr{\'e}cissement, {\`a} partir des ann{\'e}es 1980, des ressources client{\'e}laires {\`a} redistribuer au sein des soci{\'e}t{\'e}s urbaines. Les {\'e}lus locaux se trouvent ainsi pris dans un d{\'e}calage existant entre les imp{\'e}ratifs de la nouvelle gestion publique et la demande sociale en provenance des {\'e}lecteurs traditionnellement orient{\'e}e vers les questions priv{\'e}es et personnelles.},
	language = {fr},
	number = {1},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Mattina, Cesare},
	year = {2007},
	pages = {1--18},
}

@article{jeandet-mengual_dirigeants_2007,
