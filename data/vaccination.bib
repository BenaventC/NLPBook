	abstract = {La pr{\'e}sente recherche de nature exploratoire vise {\`a} analyser le processus en cours de recomposition de la structure hospitali{\`e}re et son impact sur la gestion des ressources humaines. Elle repose sur un dispositif de recherche, original, qui associe au sein du CHU d{\textquoteright}Amiens - un praticien, cadre dirigeant, coordonnateur de p{\^o}le, charg{\'e} de concevoir et mettre en {\oe}uvre la politique ressources humaines, - un observatrice participante, affect{\'e}e {\`a} temps complet au sein de la Direction des Affaires G{\'e}n{\'e}rales, - un enseignant-chercheur, observateur non participant. La nouvelle gouvernance, libert{\'e} impos{\'e}e, prolonge et r{\'e}oriente les r{\'e}formes hospitali{\`e}res ant{\'e}rieures. Elle se traduit par le d{\'e}coupage de l{\textquoteright}organisation en p{\^o}les mais ne s{\textquoteright}y r{\'e}duit pas. La d{\'e}concentration de la gestion des ressources humaines, dans une logique de contractualisation interne, suppose des choix, fait courir des risques et offre des opportunit{\'e}s aux acteurs du management hospitalier.},
	language = {fr},
	number = {Vol 26/2},
	urldate = {2020-06-26},
	journal = {Politiques et Management Public},
	author = {Crozet, Paul and Kaaniche, Asma and Lienard, Jean},
	month = jun,
	year = {2008},
	pages = {31--52},
	annote = {gestion des ressources humaines, h{\^o}pital, nouvelle gouvernance, d{\'e}concentration, instruments de gestion},
}

@article{abraham_lolf_2008,
