}

@article{ruffat_finaliser_1983,
	title = {Finaliser la gestion de l'entreprise publique},
	volume = {1},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1983_num_1_1_1708},
	doi = {10.3406/pomap.1983.1708},
	abstract = {Cet article vise {\`a} clarifier la logique et les processus internes des "entreprises non marchandes". Il pr{\'e}sente une approche qui devrait renforcer et enrichir les relations entre les entreprises publiques et la communaut{\'e} nationale, et contribuer {\`a} construire un cadre de gestion mieux adapt{\'e} {\`a} la sp{\'e}cificit{\'e} des entit{\'e}s publiques. Le cadre de gestion traditionnel des entreprises capitalistes est construit autour de la "maximisation du rendement financier pour les actionnaires". Mais g{\'e}n{\'e}ralement, les entreprises publiques refusent de s'enfermer dans ce mod{\`e}le d{\'e}terministe et mal adapt{\'e}. Pourtant, leurs administrateurs ne parviennent g{\'e}n{\'e}ralement pas {\`a} fixer un objectif et un cadre de gestion diff{\'e}rents et plus pertinents. C'est pourquoi elles deviennent souvent le centre de jeux de pouvoir politique entre leurs parties prenantes, ce qui affaiblit s{\'e}rieusement leur efficacit{\'e}. L'auteur recommande l'application du "cadre de responsabilit{\'e} des parties prenantes" propos{\'e} aux monopoles publics, mais il peut {\'e}galement {\^e}tre utilis{\'e} avec succ{\`e}s par les entreprises publiques qui fournissent des biens et des services sur le march{\'e}. Cet essai vise principalement {\`a} susciter un d{\'e}bat technique plut{\^o}t qu'{\`a} provoquer des discussions sur le diagnostic, qui est bas{\'e} sur une vaste exp{\'e}rience internationale et ne fait pas explicitement r{\'e}f{\'e}rence {\`a} la situation pr{\'e}cise d'une entreprise publique sp{\'e}cifique dans un pays donn{\'e}.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Ruffat, Jean},
	year = {1983},
	pages = {85--127},
}

@article{hafsi_etat_1983,
	title = {L' {\'E}tat en affaires : au-del{\`a} de l'id{\'e}ologie},
	volume = {1},
	issn = {0758-1726},
	shorttitle = {L' {\'E}tat en affaires},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1983_num_1_2_1726},
	doi = {10.3406/pomap.1983.1726},
	abstract = {La complexit{\'e} croissante des activit{\'e}s {\'e}conomiques des grandes nations modernes remet {\`a} l'ordre du jour la r{\'e}flexion sur le r{\^o}le que doit jouer le gouvernement dans la r{\'e}gulation de ces activit{\'e}s. En prenant comme exemple l'intervention directe de l'Etat par le biais des entreprises publiques (EP) cet article met en {\'e}vidence la difficult{\'e} grandissante que l'Etat rencontre dans la gestion de ses relations avec celles- ci. Les relations Etat-EP {\'e}voluent pour des raisons de gestion suivant un cycle ; elles commencent coop{\'e}ratives, deviennent tendues avec des affrontements fr{\'e}quents puis tendent {\`a} dispara{\^i}tre pour laisser plus d'autonomie {\`a} l'EP. En cons{\'e}quence, un gouvernement, qui peut avoir de nombreuses raisons pour cr{\'e}er des EP, doit r{\'e}aliser que celles-ci tendent {\`a} {\'e}chapper {\`a} son contr{\^o}le et il ne peut {\'e}viter cela qu'en consommant une attention et des {\'e}nergies consid{\'e}rables avec souvent des r{\'e}sultats d{\'e}risoires, voire la destruction de la raison d'{\^e}tre de l'entreprise. Pourquoi alors ne pas concevoir l'EP comme une institution transitoire dont la mission est de r{\'e}gler un certain nombre de probl{\^e}mes puis de dispara{\^i}tre (en tant qu'EP) lorsque cette mission est achev{\'e}e ? L'EP serait alors un instrument - ce qu'elle devrait {\^e}tre - et non une fin en soi - ce qu'une conception statique de l'id{\'e}ologie pourrait en faire.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Hafsi, Ta{\"i}eb},
	year = {1983},
	pages = {67--91},
}

