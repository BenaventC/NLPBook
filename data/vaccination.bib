	volume = {34},
	shorttitle = {La conduite du changement en h{\^o}pital psychiatrique},
	url = {https://hal.archives-ouvertes.fr/hal-01792752},
	abstract = {Dans cet article, nous faisons l{\textquoteright}hypoth{\`e}se que la sociologie de la traduction mobilis{\'e}e de mani{\`e}re normative est une m{\'e}thode de conduite du changement qui permettrait de favoriser l{\textquoteright}{\'e}mergence et la mise en place de pratiques de travail innovantes. Une recherche-intervention d{\textquoteright}une dur{\'e}e de 3 ans dans un h{\^o}pital psychiatrique parisien montre qu{\textquoteright}effectivement, cette m{\'e}thode a permis de franchir les fronti{\`e}res politiques en n{\'e}gociant la participation aux groupes de travail d{\textquoteright}acteurs poursuivant des objectifs diff{\'e}rents et en assurant le lien avec les niveaux hi{\'e}rarchiques sup{\'e}rieurs. Les groupes de travail constitu{\'e}s en centres de traduction ont permis de franchir les fronti{\`e}res interpr{\'e}tatives. Ils ont favoris{\'e} l{\textquoteright}apprentissage collectif de la controverse sur le travail r{\'e}el et l{\textquoteright}{\'e}mergence de pratiques innovantes jusqu{\textquoteright}alors clandestines.},
	number = {3},
	urldate = {2020-06-29},
	journal = {Politiques et Management public},
	author = {Baret, Christophe and Boiteau, Karine},
	year = {2017},
	keywords = {Economie, Education, Gestion, Sociologie},
	pages = {231--246},
	annote = {H{\^o}pital, sociologie de la traduction, conduite du changement, recherche intervention.},
}

@article{aubouin_espace_2017,
