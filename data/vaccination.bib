	issn = {0758-1726},
	shorttitle = {Les fonctionnaires et la fabrique des politiques publiques},
	url = {https://pmp.revuesonline.com/article.jsp?articleId=38668},
	doi = {10.3166/pmp.34.2017.0009},
	abstract = {Une conception w{\'e}b{\'e}rienne, id{\'e}alis{\'e}e et monolithique, du fonctionnaire, neutre et apolitique pourrait nous faire n{\'e}gliger d{\textquoteright}{\'e}tudier son r{\^o}le dans le continuum des politiques publiques. En effet, si le r{\^o}le du fonctionnaire se limite {\`a} celui du robot, son action n{\textquoteright}aurait aucun effet sensible sur l{\textquoteright}action de l{\textquoteright}{\'E}tat. La r{\'e}alit{\'e} est pourtant beaucoup plus complexe et souvent en porte-{\`a}-faux avec cette image d{\textquoteright}{\'E}pinal. Le pr{\'e}sent article se penche sur ces acteurs-fonctionnaires et r{\'e}fl{\'e}chis sur l{\textquoteright}impact significatif qu{\textquoteright}ils peuvent avoir sur la mise en {\oe}uvre des politiques publiques. Conditionn{\'e}s par des institutions internes qui ne s{\textquoteright}inscrivent pas toujours dans un mouvement lin{\'e}aire qui serait en symbiose avec la finalit{\'e} de l{\textquoteright}action publique globale, les propres d{\'e}terminants culturels des fonctionnaires d{\'e}terminent une bonne partie de leurs actions. L{\textquoteright}effet de leur rencontre, {\`a} l{\textquoteright}intersection de mondes trop souvent parall{\`e}les, constitue un facteur non n{\'e}gligeable de la fabrique des politiques publiques.},
	number = {1-2},
	urldate = {2020-06-29},
	journal = {Politiques et management public},
	author = {Baily, Dominique and Savard, Jean-Fran{\c c}ois},
	month = jun,
	year = {2017},
	pages = {167--181},
	annote = {acteurs, r{\`e}gles, n{\'e}o institutionnalisme, mise en {\oe}uvre, politiques publiques.},
}

@article{baret_conduite_2017,
