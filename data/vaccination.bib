	year = {1995},
	pages = {1--27},
}

@article{clergeau_de_mascureau_quelles_1995,
	title = {Quelles entraves organisationnelles et institutionnelles {\`a} l'innovation dans les organisations bureaucratiques publiques ?},
	volume = {13},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_sup_13_2_3395},
	doi = {10.3406/pomap.1995.3395},
	abstract = {Les bureaucraties publiques sont soumises {\`a} des contraintes organisationnelles et institutionnelles, d'essence w{\'e}b{\'e}rienne, qui n'emp{\^e}chent pas l'innovation mais en impriment les caract{\'e}ristiques. Hi{\'e}rarchie, contr{\^o}le, centralisation des syst{\`e}mes de coordination et d'incitation, droit public r{\'e}gissent les {\'e}changes entre l'organisation et son environnement et les relations internes. Ils d{\'e}terminent les modes d'int{\'e}gration et d'exploitation des informations nouvelles. Ils impriment les caract{\'e}ristiques des processus d'innovation, telles la multiplicit{\'e} des innovations mineures ou la lenteur des processus d'int{\'e}gration et de diffusion des innovations majeures. La probl{\'e}matique de l'innovation dans les organisations bureaucratiques publiques peut ainsi se penser en terme d'arbitrage entre rigidit{\'e}-contr{\^o}le- innovation progressive et souplesse-perte de contr{\^o}le-innovation rapide.},
	language = {fr},
	number = {2},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Clergeau de Mascureau, C{\'e}cile},
	year = {1995},
	pages = {141--171},
}

@article{deme_les_1995,
	title = {Les privatisations, une solution pour l'{Afrique} ?},
	volume = {13},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_sup_13_2_3394},
	doi = {10.3406/pomap.1995.3394},
	abstract = {Les pays africains ont d{\'e}velopp{\'e} au cours des ann{\'e}es, suivant les ind{\'e}pendances, le secteur de leurs entreprises publiques. Cet {\'e}largissement a renforc{\'e} la pr{\'e}sence des pouvoirs publics aussi bien dans les activit{\'e}s de base que dans celles soumises {\`a} la concurrence parce qu'elles avaient vocation {\`a} participer en premi{\`e}re ligne au d{\'e}veloppement {\'e}conomique et social de ces pays. La d{\'e}sillusion fut totale. Instruments de la politique {\'e}conomique, les entreprises publiques sont devenues, au fil des ann{\'e}es, d{\'e}ficitaires. Aussi, devant leurs dysfonctionnements et leurs difficult{\'e}s, les Etats ont {\'e}t{\'e} men{\'e}s, sous l'impulsion des bailleurs de fonds, {\`a} rompre avec cette formule d'organisation et {\`a} {\'e}pouser des options lib{\'e}rales dont les privatisations sont une composante. Par rapport aux limites observables de la privatisation, cet article s'attache {\`a} d{\'e}finir les domaines respectifs des espaces public et priv{\'e}, {\`a} d{\'e}passer le cadre r{\'e}ducteur des politiques de privatisation en pr{\^o}nant une compl{\'e}mentarit{\'e} entre initiative priv{\'e}e et action publique parce que ces deux modes d'intervention sont compl{\'e}mentaires plut{\^o}t qu'alternatifs.},
	language = {fr},
	number = {2},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Deme, Mouhamadou},
	year = {1995},
	pages = {97--140},
}

@article{cattacin_maitrise_1995,
	title = {La ma{\^i}trise du sida : une analyse comparative des r{\'e}actions organisationnelles au vih/sida en {Europe} de l'{Ouest}},
	volume = {13},
	issn = {0758-1726},
	shorttitle = {La ma{\^i}trise du sida},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_sup_13_2_3393},
	doi = {10.3406/pomap.1995.3393},
	abstract = {La r{\'e}flexion autour des politiques sociales s'inscrit aujourd'hui dans le d{\'e}bat sur la question du r{\^o}le et de l'intervention des pouvoirs publics dans une situation de crise de l'Etat social et souligne l'importance des acteurs non {\'e}tatiques. Nous avons {\'e}labor{\'e} une typologie des politiques de lutte contre le vih/sida en analysant le welfare mix pr{\'e}sent dans ces politiques, c'est-{\`a}- dire l'ensemble des producteurs du bien-{\^e}tre -{\'e}tatiques et non {\'e}tatiques- et la division des t{\^a}ches existant entre eux, le contenu des politiques de lutte contre le vih/sida et les processus politiques en action au sein de ces politiques. Les trois types de r{\'e}ponses identifi{\'e}s vont au-del{\`a} des dichotomies propos{\'e}es jusqu'ici dans le domaine du vhi/sida et pr{\'e}sentent des variantes inattendues par rapport aux expectatives des typologies g{\'e}n{\'e}rales de l'Etat social. Nous nous trouvons ainsi dans une situation o{\`u} les Etats sociaux sont en train de se restructurer pour aboutir {\`a} d'autres constellations internes et externes. Cette situation changeante, o{\`u} les mod{\`e}les traditionnels peuvent {\^e}tre "trahis" par des nouvelles exp{\'e}riences, va dans le sens d'un futur possible qui verrait une gestion diff{\'e}rente de tout le secteur social.},
	language = {fr},
	number = {2},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Cattacin, Sandro and Panchaud, Christine},
	year = {1995},
	pages = {47--95},
}

@article{burmeister_instruments_1995,
