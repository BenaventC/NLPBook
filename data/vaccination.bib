}

@article{dafflon_lequilibre_1995,
	title = {L'{\'e}quilibre des budgets publics locaux : quelle strat{\'e}gie et quels risques de d{\'e}rapage ?},
	volume = {13},
	issn = {0758-1726},
	shorttitle = {L'{\'e}quilibre des budgets publics locaux},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_3_2064},
	doi = {10.3406/pomap.1995.2064},
	abstract = {Cette communication examine l'{\'e}quilibre du budget comme r{\`e}gle de gestion des finances publiques locales. L'{\'e}quilibre du budget est-elle la seule strat{\'e}gie acceptable ? Quels sont les risques de d{\'e}rapage si elle n'est pas retenue ? Une r{\'e}ponse est fournie en comparant l'exp{\'e}rience des communes d'un canton suisse aux propositions normatives du f{\'e}d{\'e}ralisme financier dans la perspective des choix publics. Apr{\`e}s avoir d{\'e}fini les concepts de responsabilit{\'e} et de discipline budg{\'e}taires, et analys{\'e} les arguments ayant justifi{\'e} la r{\`e}gle de l'{\'e}quilibre, la d{\'e}marche consiste {\`a} en pr{\'e}ciser le contenu. Six questions permettent d'{\'e}laborer une m{\'e}thode permettant de passer des arguments {\`a} l'application. Mal pr{\'e}cis{\'e}e, la r{\`e}gle de l'{\'e}quilibre du budget permet aux communes d'adopter des comportements strat{\'e}giques. On conclut en formulant un th{\'e}or{\`e}me de l'{\'e}quilibre budg{\'e}taire en quatre points. Ce th{\'e}or{\`e}me veut d{\'e}monter que les r{\`e}gles juridiques sont l'expression de principes de gestion et non pas la marque d'une tutelle cantonale.},
	language = {fr},
	number = {3},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Dafflon, Bernard},
	year = {1995},
	pages = {141--165},
}

