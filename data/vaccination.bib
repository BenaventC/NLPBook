@article{lopez_produire_2003,
	title = {Produire la confiance verticale par la r{\'e}putation : le secteur public profite-t-il des outils du secteur marchand ?},
	volume = {21},
	issn = {0758-1726},
	shorttitle = {Produire la confiance verticale par la r{\'e}putation},
