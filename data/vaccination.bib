@article{palard_les_1995,
	title = {Les effets institutionnels de la politique r{\'e}gionale de l'{Union} {Europ{\'e}enne}},
	volume = {13},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_3_2060},
	doi = {10.3406/pomap.1995.2060},
	abstract = {Au cours du dernier quart de si{\`e}cle, la politique r{\'e}gionale de l'Union europ{\'e}enne ne s'est pas seulement traduite par une forte augmentation d'un engagement financier ordonn{\'e} {\`a} la r{\'e}duction des disparit{\'e}s interr{\'e}gionales ; on observe {\'e}galement, et peut- {\^e}tre surtout, un processus d'autonomisation progressive des autorit{\'e}s europ{\'e}ennes dans la d{\'e}finition et la conduite de ce champ d'action. Corr{\'e}lativement, la r{\'e}gion s'impose progressivement, pour ces autorit{\'e}s, comme un partenaire majeur, ce qui ne saurait {\^e}tre institutionnellement neutre au regard de l'architecture en devenir des diff{\'e}rents niveaux territoriaux de pouvoir. Ind{\'e}niablement, la Commission europ{\'e}enne a trouv{\'e} dans l'instauration et le d{\'e}veloppement de la politique r{\'e}gionale le moyen de s'engager dans une action de politique territoriale et, ce faisant, de tenter de nouer des relations directes avec les institutions de niveau r{\'e}gional (Autonomies espagnoles, r{\'e}gions fran{\c c}aises, L{\"a}nder allemands...) ; les "r{\'e}gions" elles-m{\^e}mes y ont souvent vu le moyen d'initier des formes de politique ext{\'e}rieure, notamment interr{\'e}gionale. L'{\'e}mergence de cette nouvelle g{\'e}opolitique europ{\'e}enne permet d'avancer l'hypoth{\`e}se d'une ad{\'e}quation entre strat{\'e}gies r{\'e}gionales et strat{\'e}gies europ{\'e}ennes qui para{\^i}t partiellement command{\'e}e par une logique d' "encerclement" ou de "contoumement" du pouvoir d'Etat.},
	language = {fr},
	number = {3},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Palard, Jacques},
	year = {1995},
	pages = {65--81},
}

@article{thoenig_incertitude_1995,
	title = {De l'incertitude en gestion territoriale},
	volume = {13},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1995_num_13_3_2057},
	doi = {10.3406/pomap.1995.2057},
	abstract = {L'incertitude est abord{\'e}e sous deux angles compl{\'e}mentaires. Elle est caract{\'e}ris{\'e}e telle qu'utilis{\'e}e par les sciences sociales, notamment en sociologie, conceptuellement et m{\'e}thodologiquement. Elle est {\'e}tudi{\'e}e {\`a} partir d'un terrain concret, les villes fran{\c c}aises, en tant qu'acteurs et d{\'e}cideurs publics. Il est propos{\'e} de distinguer inqui{\'e}tude et incertitude, cette derni{\`e}re renvoyant {\`a} des param{\`e}tres cognitifs et comportementaux. Il est montr{\'e} que les responsables politiques locaux ne vivent pas un {\'e}tat d'inqui{\'e}tude qui soit marqu{\'e}. Les facteurs sont identifi{\'e}s qui expliquent comment et pourquoi les dirigeants territoriaux r{\'e}duisent l'incertitude et construisent les conditions d'un univers gestionnaire et politique non al{\'e}atoire. Il est enfin sugg{\'e}r{\'e} que, loin de s'{\^e}tre accrue avec la "crise" des derni{\`e}res ann{\'e}es, l'incertitude s'est trouv{\'e}e r{\'e}duite par les pratiques des entrepreneurs territoriaux.},
	language = {fr},
	number = {3},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Th{\oe}nig, Jean-Claude},
	year = {1995},
	pages = {1--27},
}

@article{chambron_les_1995,
