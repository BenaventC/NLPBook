	number = {1},
	urldate = {2020-06-29},
	journal = {Politiques et Management public},
	author = {Pesle, Manon},
	year = {2019},
	pages = {55--74},
	annote = {cadres interm{\'e}diaires, {\'e}ducation, gestion, instruments d{\textquoteright}action publique, m{\'e}tropole, politiques locales.},
}

@article{bensadon_union_2016,
