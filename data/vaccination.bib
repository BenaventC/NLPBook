	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Uhaldeborde, Jean-Michel},
	year = {1984},
	pages = {7--37},
}

@article{j_hill_quelques_1985,
	title = {Quelques aspects du contr{\^o}le central et de la mise en {\oe}uvre de politiques nationales au niveau local : l'exemple des politiques gouvernementales de garantie de ressources en {Grande}-{Bretagne}},
	volume = {3},
	issn = {0758-1726},
	shorttitle = {Quelques aspects du contr{\^o}le central et de la mise en {\oe}uvre de politiques nationales au niveau local},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_1_1822},
	doi = {10.3406/pomap.1985.1822},
	abstract = {En d{\'e}pit du d{\'e}veloppement d'un syst{\`e}me de garantie de ressources g{\'e}r{\'e} par le gouvernement central, les autorit{\'e}s locales conservent, en Grande-Bretagne, la responsabilit{\'e} de certains services qui ont un impact sur les revenus individuels et qui sont {\`a} l'origine d'un certain nombre de probl{\`e}mes entre le syst{\`e}me de garantie de ressources d{\'e}termin{\'e} et administr{\'e} {\`a} l'{\'e}chelon national et les contributions qu'apportent les autorit{\'e}s locales {\`a} ce syst{\`e}me. Bien que la tendance g{\'e}n{\'e}rale soit {\`a} un renforcement du contr{\^o}le central sur les autorit{\'e}s locales, ces services, sur la mise en oeuvre desquels elles ont un impact consid{\'e}rable, ont ind{\'e}niablement tendu {\`a} augmenter. L'objet de cette contribution est d'{\'e}tudier les probl{\`e}mes soulev{\'e}s par de tels d{\'e}veloppements en prenant plus particuli{\`e}rement appui sur deux domaines d'intervention particuliers : les avantages en mati{\`e}re de logement offerts par les autorit{\'e}s locales et les aides mat{\'e}rielles g{\'e}r{\'e}es par les d{\'e}partements charg{\'e}s des services sociaux locaux. Bien que le gouvernement central essaie de pallier quelques anomalies r{\'e}sultant de l'interaction entre le syst{\`e}me local et le syst{\`e}me national de prestations soumises {\`a} conditions de ressources, il ne se pr{\'e}occupe gu{\`e}re des probl{\`e}mes de justice territoriale ou d'incoh{\'e}rence de l'administration. Ses interventions peuvent {\^e}tre interpr{\'e}t{\'e}es comme autant de tentatives visant davantage {\`a} {\'e}viter des solutions qui auraient pour cons{\'e}quence d'alourdir le {\guillemotleft}fardeau{\guillemotright} du centre qu'{\`a} restaurer d{\'e}lib{\'e}r{\'e}ment un syst{\`e}me d{\'e}centralis{\'e} de garantie de ressources. Les d{\'e}veloppements des services locaux sont tol{\'e}r{\'e}s dans le contexte d'un contr{\^o}le central {\'e}largi portant sur les d{\'e}penses des collectivit{\'e}s territoriales. Il semble qu'actuellement le gouvernement central britannique soit en voie de mettre en place quelques instruments relatifs {\`a} la mise en oeuvre des politiques susceptibles de favoriser un contr{\^o}le global central alors que, dans le m{\^e}me temps, ces dispositions semblent mettre la responsabilit{\'e} de la couverture du d{\'e}ficit {\`a} la charge du niveau local.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {J. Hill, Micha{\"e}l},
	year = {1985},
	pages = {191--214},
}

