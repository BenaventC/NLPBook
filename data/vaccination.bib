}

@article{mattijs_gerer_2001,
	title = {G{\'e}rer la justice distributive dans les politiques allocatives budg{\'e}taires : entre fonction et dysfonctionnement},
	volume = {19},
	issn = {0758-1726},
	shorttitle = {G{\'e}rer la justice distributive dans les politiques allocatives budg{\'e}taires},
