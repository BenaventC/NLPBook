	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1988_num_6_1_1988},
	doi = {10.3406/pomap.1988.1988},
	abstract = {En tant qu'organisations de travail, les mairies pr{\'e}sentent une sp{\'e}cificit{\'e} importante, l'existence d'une culture organisationnelle duale et complexe, int{\'e}grant des logiques diff{\'e}rentes : celle des {\'e}lus et celle du personnel communal, qui doivent coop{\'e}rer pour le meilleur service de la population, ce dernier acteur regroupant trois logiques diff{\'e}rentes, celles du contribuable, de l'{\'e}lecteur et du * client{\guillemotright}. La dissociation du troisi{\`e}me acteur le met en position de faiblesse face aux deux autres qui r{\'e}ussissent {\`a} imposer leur logique {\`a} l'organisation, soit pour l'innovation, soit pour la r{\'e}sistance au changement. C'est dire la complexit{\'e} d'une telle culture organisationnelle, o{\`u} la s{\'e}dimentation culturelle et le poids de l'histoire introduisent des contradictions difficiles {\`a} g{\'e}rer, mais porteuses d'innovations. Devant la mont{\'e}e des r{\'e}sistances fiscales et l'accroissement des charges municipales, les innovations concernent l'{\'e}mergence d'une culture manag{\'e}riale essentiellement bas{\'e}e sur de nouvelles techniques de gestion, et plus timidement sur les aspects humains et mobilisateurs du management moderne. Les {\'e}volutions seront- elles activ{\'e}es par de nouvelles relations avec le citoyen-{\'e}lecteur, tclient{\guillemotright} des mairies ? Les cultures organisationnelles locales pr{\'e}sentent certaines phases typiques d'{\'e}volution, dont une esquisse est propos{\'e}e pour {\'e}clairer les modalit{\'e}s du management local.},
	language = {fr},
	number = {1},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Becquart-Leclercq, Jeanne},
	year = {1988},
	pages = {21--60},
}

