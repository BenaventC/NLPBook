	doi = {10.3406/pomap.1985.1842},
	abstract = {Ce texte dresse l'inventaire des types de nomination dans les soci{\'e}t{\'e}s d'Etat au Qu{\'e}bec et des types de statuts de leurs pr{\'e}sidents. Il montre l'{\'e}volution de ce statut selon trois {\'e}poques : avant 1960, la p{\'e}riode 1960-1975 et apr{\`e}s 1976. On y remarque une mutation du client{\'e}lisme vers un syst{\`e}me de gestion ax{\'e} sur les grandes orientations du gouvernement. Le probl{\`e}me reste pos{\'e} du respect des orientations gouvernementales et des moyens dont dispose le gouvernement pour s'assurer de la conformit{\'e} des politiques des soci{\'e}t{\'e}s d'Etat. Un des instruments pertinents est certes le contr{\^o}le des nominations des pr{\'e}sidents mais il suppose une m{\'e}thode et un processus d'{\'e}valuation des performances des pr{\'e}sidents et des soci{\'e}t{\'e}s. Cet article traitera des difficult{\'e}s de ce processus d'{\'e}valuation.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Bourgault, Jacques},
	year = {1985},
	pages = {179--196},
}

@article{brana_structure_1985,
	title = {Structure des entreprises publiques, recyclage de l'industrie et autonomie de gestion : le cas de l'{Espagne}},
	volume = {3},
	issn = {0758-1726},
	shorttitle = {Structure des entreprises publiques, recyclage de l'industrie et autonomie de gestion},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_2_1841},
	doi = {10.3406/pomap.1985.1841},
	abstract = {L'article se compose de quatre parties. Dans la premi{\`e}re, nous nous effor{\c c}ons d'esquisser le cadre d'analyse et les diff{\'e}rentes solutions qui visent {\`a} atteindre les objectifs fix{\'e}s par les pouvoirs publics, notamment en ce qui concerne les probl{\`e}mes de gestion. Nous insistons sur les diff{\'e}rents syst{\`e}mes d'incitation et sur les contrats de programme, sur leur possibilit{\'e}s et sur les conditions de leur application. Dans la seconde partie, nous cherchons {\`a} pr{\'e}senter les chiffres {\'e}conomiques les plus significatifs du secteur des entreprises publiques espagnoles (non financi{\`e}res), afin de situer sa v{\'e}ritable importance. Contrairement {\`a} la croyance g{\'e}n{\'e}rale, sa dimension est l'une des plus faibles d'EUROPE et il n'a connu qu'une croissance mod{\'e}r{\'e}e au cours des quinze derni{\`e}res ann{\'e}es. Une br{\`e}ve description de la structure institutionnelle et sectorielle des entreprises publiques compl{\`e}te le tableau. Comme nous essayons de l'expliquer dans la troisi{\`e}me partie, la crise a eu une incidence tr{\`e}s importante sur les entreprises publiques, notamment par ses effets sur le budget de l'Etat. Cr{\'e}ant par ce biais un lien avec la quatri{\`e}me partie, nous {\'e}tudions les principaux probl{\`e}mes des entreprises publiques, qu'ils tiennent {\`a} l'appartenance {\`a} certains secteurs sp{\'e}cialement touch{\'e}s par la crise, {\`a} la fourniture de services publics ou {\`a} la mise en oeuvre d'une {\guillemotleft}socialisation des pertes priv{\'e}es{\guillemotright}. Dans cette derni{\`e}re partie, nous pr{\'e}sentons quelques cas et les solutions essay{\'e}es actuellement.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Brana, Francisco J.},
	year = {1985},
	pages = {153--177},
}

@article{wickham_biais_1985,
	title = {Biais marketing international des entreprises publiques},
	volume = {3},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_2_1840},
	doi = {10.3406/pomap.1985.1840},
	abstract = {Les entreprises publiques sont g{\'e}n{\'e}ralement cr{\'e}{\'e}es pour des raisons d'{\'e}conomie int{\'e}rieure. Les op{\'e}rations internationales, faiblement prises en compte dans les d{\'e}cisions l{\'e}gislatives de cr{\'e}ation, apparaissent plut{\^o}t l'ultime refuge de leur autonomie constamment menac{\'e}e. D'o{\`u} quelques sp{\'e}cificit{\'e}s de leurs strat{\'e}gies internationales largement explicables par leurs contraintes structurelles : {\textbullet} d{\'e}marches empiriques et peu syst{\'e}matiques {\textbullet} politiques de prix sur les march{\'e}s occidentaux souvent agressives {\textbullet} excellence dans les grands contrats et coop{\'e}rations technologiques souvent g{\'e}n{\'e}reuses sur le Tiers- Monde. Les managers publics sur la sc{\`e}ne internationale tiennent g{\'e}n{\'e}ralement {\`a} minimiser ces sp{\'e}cificit{\'e}s et appara{\^i}tre strictement similaires aux autres hommes d'affaires. Les th{\'e}ories du management public ne leur semblent pas un bon article d'exportation.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Wickham, Sylvain},
	year = {1985},
	pages = {141--151},
}

@article{lafay_les_1985,
