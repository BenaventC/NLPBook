}

@article{mouret_flexibilite_1994,
	title = {Flexibilit{\'e} et changement {\`a} {La} {Poste} : de la modernisation au d{\'e}veloppement social},
	volume = {12},
	issn = {0758-1726},
	shorttitle = {Flexibilit{\'e} et changement {\`a} {La} {Poste}},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1994_num_12_3_3179},
	doi = {10.3406/pomap.1994.3179},
	abstract = {Passer de pratiques de modernisation rationnelles/gestionnaires {\`a} des pratiques de d{\'e}veloppement social, c'est prendre en compte le syst{\`e}me social, comprendre ses r{\'e}gulations, s'appuyer sur les identit{\'e}s et les cultures. C'est cr{\'e}er de l'acteur, et de l'acteur conscient Le regard sociologique peut {\^e}tre d'un grand apport dans toutes les pratiques de changement, {\`a} deux niveaux : il permet de comprendre le syst{\`e}me social et ses fonctionnements cach{\'e}s ; {\`a} partir de l{\`a}, il permet de penser de mani{\`e}re pertinente et en coh{\'e}rence avec le syst{\`e}me social de base les formes de mise en {\oe}uvre du changement ou, dans le cas de La Poste, les formes de flexibilit{\'e} possibles. A la lecture sociologique, les {\'e}l{\'e}ments de r{\'e}gulation de La Poste sont structur{\'e}s essentiellement sur les dimensions culturelles et relationnelles : service public, conscience professionnelle, relation d'identification {\`a} la client{\`e}le ; mais aussi, lorsque le travail est moins qualifi{\'e} et plus routinier, les horaires et la convivialit{\'e}. S'appuyer sur ces r{\'e}gulations am{\`e}ne {\`a} penser la flexibilit{\'e} sur le mode du d{\'e}veloppement, en essayant d'allier d{\'e}veloppement local et d{\'e}veloppement social interne et externe, afin de pr{\'e}server la dimension {\'e}thique fortement ancr{\'e}e dans les esprits. En ce qui concerne la mise en {\oe}uvre de ces changements et compte tenu des diversit{\'e}s locales possibles, il est important de laisser des marges de man{\oe}uvre au niveau des acteurs locaux. En effet, dans le cas o{\`u} les changements touchent {\`a} des r{\'e}gulations sociales, il est important de recr{\'e}er des espaces d'autonomie au sein des syst{\`e}mes sociaux pour que de nouvelles r{\'e}gulations puissent se mettre en place.},
	language = {fr},
	number = {3},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Mouret, Brigitte},
	year = {1994},
	pages = {143--158},
}

@article{brisset-sillion_impact_1994,
	title = {Impact de la recherche sur la gestion et le pouvoir dans des universit{\'e}s publiques am{\'e}ricaines : le cas de la {State} {University} of {New} {York}},
	volume = {12},
	issn = {0758-1726},
	shorttitle = {Impact de la recherche sur la gestion et le pouvoir dans des universit{\'e}s publiques am{\'e}ricaines},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1994_num_12_3_3176},
	doi = {10.3406/pomap.1994.3176},
	abstract = {Trois mod{\`e}les, issus de l'{\'e}tude empirique de six campus, sont pr{\'e}sent{\'e}s dans lesquels la place occup{\'e}e par la recherche est plus ou moins importante. L'impact de ces variations est montr{\'e} au niveau des recrutements effectu{\'e}s et des normes d'{\'e}valuation de l'activit{\'e} utilis{\'e}es mais il correspond surtout {\`a} des fonctionnements internes diff{\'e}rents. Dans les universit{\'e}s (I) o{\`u} la recherche occupe une place centrale, l'autorit{\'e} est du c{\^o}t{\'e} des universitaires et les d{\'e}cisions sont n{\'e}goci{\'e}es, ce qui permet de proc{\'e}der aux r{\'e}allocations de ressources n{\'e}cessaires pour financer le d{\'e}veloppement scientifique. Dans les deux autres mod{\`e}les, en revanche, c'est plut{\^o}t l'administration qui domine. Dans l'universit{\'e} de type (II), recherche et enseignement sont plus int{\'e}gr{\'e}s. Le processus budg{\'e}taire est formalis{\'e} et repose sur une planification contr{\^o}l{\'e}e par l'administration, mais les acad{\'e}miques ont voix au chapitre. La croissance du campus semble {\^e}tre plus ma{\^i}tris{\'e}e. Enfin, dans les coll{\`e}ges de type (III), la recherche sponsoris{\'e}e est pratiquement absente et l'activit{\'e} tr{\`e}s statique : la plupart des d{\'e}cisions sont prises unilat{\'e}ralement, de fa{\c c}on informelle, par une administration per{\c c}ue comme "autoritaire11. Une r{\'e}flexion plus g{\'e}n{\'e}rale sur le lien entre march{\'e} et structure organisationnelle et les co{\^u}ts d'un fonctionnement centr{\'e} autour de la recherche est amorc{\'e}e en guise de conclusion.},
	language = {fr},
	number = {3},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Brisset-Sillion, C{\'e}cile},
	year = {1994},
	pages = {117--140},
}

