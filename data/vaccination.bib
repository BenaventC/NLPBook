@article{sacco_les_2019,
	title = {Les technologies de production automatis{\'e}e de donn{\'e}es comme instruments d{\textquoteright}action publique : de la production des donn{\'e}es aux effets sur les fonctionnaires interm{\'e}diaires},
	volume = {36},
	issn = {0758-1726},
	shorttitle = {Les technologies de production automatis{\'e}e de donn{\'e}es comme instruments d{\textquoteright}action publique},
	url = {https://orbi.uliege.be/handle/2268/234399},
	abstract = {Dans une perspective th{\'e}orique et m{\'e}ta-analytique, cet article discute les transformations de l{\textquoteright}action publique engendr{\'e}es par la multiplication des types de donn{\'e}es disponibles {\`a} l{\textquoteright}{\`e}re des big data (statistiques, probantes, homog{\'e}n{\'e}is{\'e}es, etc.). L{\textquoteright}article soutient que les outils de production de ces donn{\'e}es constituent des instruments d{\textquoteright}action publique {\`a} part enti{\`e}re, qui v{\'e}hiculent une repr{\'e}sentation particuli{\`e}re des probl{\`e}mes {\`a} traiter et de la relation qui se noue entre gouvernants et gouvern{\'e}s. Interrogeant les transformations concr{\`e}tes que l'impl{\'e}mentation et l'usage des donn{\'e}es et technologies impliquent pour les acteurs administratifs interm{\'e}diaires, l{\textquoteright}article met en {\'e}vidence plusieurs effets de ces technologies et des donn{\'e}es qu{\textquoteright}elles produisent sur l{\textquoteright}action publique, tels que l{\textquoteright}automatisation de la production de donn{\'e}es et du travail administratif, qui transforme la nature de l{\textquoteright}intervention humaine. Dans une perspective critique, il questionne le devenir du pouvoir discr{\'e}tionnaire de ces fonctionnaires, le sens et le contenu de leur travail, ainsi que les mutations cognitives {\`a} l{\textquoteright}{\oe}uvre. Cet article trace le cadre de recherches futures {\`a} mener sur la digitalisation de l{\textquoteright}action publique.},
	language = {fr},
	number = {1},
	urldate = {2020-06-29},
	journal = {Politiques et Management Public},
	author = {Sacco, Muriel and Schiffino, Nathalie and Piron, Damien and Perin, Emmanuelle},
	year = {2019},
	annote = {donn{\'e}es, big data, statistiques, automatisation, acteurs administratifs, instruments d{\textquoteright}action publique.},
}

