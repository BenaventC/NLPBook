	journal = {Politiques et management public},
	author = {Brami, Laurent and Damart, S{\'e}bastien and Kletz, Fr{\'e}d{\'e}ric},
	month = mar,
	year = {2012},
	pages = {541--561},
	annote = {H{\^o}pital, r{\'e}formes hospitali{\`e}res, absent{\'e}isme, organisation du travail.},
}

@article{mignot-gerard_gouvernement_2012,
	title = {Le gouvernement d{\textquoteright}une universit{\'e} face aux {\guillemotleft} {Initiatives} d{\textquoteright}{Excellence} {\guillemotright} : r{\'e}activit{\'e} et micro-r{\'e}sistances},
