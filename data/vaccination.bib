@article{geourjon_analyse_2004,
	title = {L'analyse de risque pour cibler les contr{\^o}les douaniers dans les pays en d{\'e}veloppement : une aventure risqu{\'e}e pour les recettes ?},
	volume = {22},
	issn = {0758-1726},
	shorttitle = {L'analyse de risque pour cibler les contr{\^o}les douaniers dans les pays en d{\'e}veloppement},
