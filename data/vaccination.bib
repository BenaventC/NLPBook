@article{decrop_usage_1993,
	title = {L'usage des politiques de transition : le cas des risques majeurs},
	volume = {11},
	issn = {0758-1726},
	shorttitle = {L'usage des politiques de transition},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1993_num_11_2_3112},
	doi = {10.3406/pomap.1993.3112},
	abstract = {Au d{\'e}but des ann{\'e}es quatre-vingts une nouvelle notion -les risques majeurs- a servi de r{\'e}f{\`e}rent {\`a} une action publique dont on a pu croire qu'elle {\'e}tait le fondement d'une politique globale dans le champ des risques. Une d{\'e}cennie apr{\`e}s, il semble qu'il n'en soit rien. Il convient pourtant de s'interroger sur le sens de telles actions publiques ayant un r{\'e}f{\`e}rent extr{\^e}me. En tant que "politiques de transition", elles ont effectivement de multiples usages. Elles sont {\`a} l'origine d'une dynamique dans le champ politico-administratif se traduisant par des modifications et des transferts de comp{\'e}tence, une circulation accrue des responsables politiques, administratifs et scientifiques. Elles permettent aussi l'engagement et l'entretien d'un d{\'e}bat fondamental sur la Cit{\'e}. Faut-il voir l{\`a} une simple fa{\c c}on d'introduire du "jeu" dans les grandes politiques publiques structurelles, de leur donner un suppl{\'e}ment d'{\^a}me aussi ou de maintenir {\`a} la marge le politique dans son {\'e}tat primitif ?},
	language = {fr},
	number = {2},
	urldate = {2020-06-25},
	journal = {Politiques et management public},
	author = {Decrop, Genevi{\`e}ve and Gilbert, Claude},
	year = {1993},
	pages = {143--157},
}

@article{cox_amelioration_1993,
	title = {L'am{\'e}lioration du management dans l'{Etat} : effets de l'initiative dite des "{\'e}tapes suivantes" sur l'efficience de l'administration au {Royaume}-{Uni}},
