	volume = {36},
	doi = {10.3166/pmp.36. 2019.0023},
	abstract = {Cet article est une {\'e}tude exploratoire visant {\`a} comprendre comment les pilotes d{\textquoteright}orga- nisations publiques relevant du domaine r{\'e}galien, donc marqu{\'e}es par l{\textquoteright}esprit de corps, interpr{\`e}tent et conduisent les changements attendus sur le sujet de la pr{\'e}vention des risques professionnels. Au travers une {\'e}tude de cas sur la toxicit{\'e} des fum{\'e}es dans le m{\'e}- tier des sapeurs-pompiers, nous montrons que le sujet de la pr{\'e}vention des risques est particulier du fait de l{\textquoteright}incertitude portant sur les dangers r{\'e}els et l{\textquoteright}efficacit{\'e} des mesures. Ceci implique une posture de la part des directions qui rel{\`e}ve davantage de la m{\'e}diation que de la conduite d{\textquoteright}un changement actif. Dans ce cadre, l{\textquoteright}esprit de corps les poussera {\`a} r{\'e}duire les dissonances relatives {\`a} la perception du risque dans leurs {\'e}quipes et {\`a} recher- cher un statu quo sur la conduite {\`a} tenir et les mesures {\`a} mettre en place. En outre l{\textquoteright}esprit de corps ne prot{\`e}ge pas le domaine du r{\'e}galien des logiques {\'e}conomiques et de l{\textquoteright}{\'e}vo- lution de la soci{\'e}t{\'e} qui mod{\`e}leront au moins en partie la perception des risques et les mesures qui doivent {\^e}tre prises.},
	number = {3},
	author = {Saint Jonsson, Anais and Agopian, Philippe and Turc, Emil},
	month = nov,
	year = {2019},
	pages = {389--412},
	annote = {esprit de corps\_culture organisationnelle\_changement culturel\_gestion du changement\_sapeurs-pompiers.},
}

@article{favreau_transnational_2019,
	series = {{PMP}},
	title = {Le transnational : du sympt{\^o}me {\`a} la cause de la crise de l{\textquoteright}{\'E}tat r{\'e}galien},
	volume = {36},
	doi = {10.3166/pmp.36. 2019.0021},
	abstract = {La notion de droits r{\'e}galiens peut {\^e}tre d{\'e}finie selon deux points de vue. En premier lieu, selon une conception large, les droits r{\'e}galiens sont les droits exerc{\'e}s par le souverain. En second lieu, selon une conception sp{\'e}cifique au droit international classique, les droits r{\'e}galiens doivent {\^e}tre con{\c c}us comme les droits de l{\textquoteright}{\'E}tat. La critique de l{\textquoteright}{\'E}tat, r{\'e}currente depuis le XIXe si{\`e}cle, s{\textquoteright}accompagne du retour {\`a} une conception large du r{\'e}galien. {\`A} par- tir des ann{\'e}es 1990, le d{\'e}veloppement de r{\'e}gimes transnationaux peut {\^e}tre consid{\'e}r{\'e} comme l{\textquoteright}un des sympt{\^o}mes de ce retour {\`a} une conception large du r{\'e}galien. Aujourd{\textquoteright}hui, ces r{\'e}gimes transnationaux, occupant d{\'e}sormais une place importante dans de nom- breux domaines, sont aussi la cause d{\textquoteright}un approfondissement de la crise de l{\textquoteright}{\'E}tat r{\'e}galien. C{\textquoteright}est la nature m{\^e}me de l{\textquoteright}{\'E}tat r{\'e}galien et de la distinction entre sph{\`e}res publique et pri- v{\'e}e qui est ici en mutation.},
	number = {4},
	author = {Favreau, Florian},
	month = dec,
	year = {2019},
	pages = {371--387},
	annote = {droit international\_d{\'e}veloppement durable\_transnational.},
}

