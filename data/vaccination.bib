@article{delpeuch_innovation_2019,
	title = {Innovation instrumentale et pluralisme cognitif la r{\'e}ception du diagnostic de s{\'e}curit{\'e} dans les organisations polici{\`e}res},
	volume = {36},
	abstract = {L{\textquoteright}article {\'e}tudie la relation entre, d{\textquoteright}une part, l{\textquoteright}innovation en mati{\`e}re d{\textquoteright}instruments d{\textquoteright}action publique et, d{\textquoteright}autre part, les matrices cognitives qui sous-tendent les mani{\`e}res de penser et d{\textquoteright}agir dans un secteur d{\textquoteright}action publique, ici celui de l{\textquoteright}action polici{\`e}re (policing). En nous basant sur le cas des usages policiers du diagnostic de s{\'e}curit{\'e}, nous montrons que la carri{\`e}re d{\textquoteright}un nouvel outil d{\textquoteright}intelligence d{\'e}pend, pour une bonne part, de la r{\'e}ception diff{\'e}rentielle dont il fait l{\textquoteright}objet de la part des diff{\'e}rentes communaut{\'e}s cognitives qui composent l{\textquoteright}univers professionnel du secteur. Cela signifie que l{\textquoteright}innovation p{\'e}n{\`e}tre le secteur en empruntant les chemins de moindre r{\'e}sistance, c{\textquoteright}est-{\`a}-dire en s{\textquoteright}implantant d{\textquoteright}abord dans les communaut{\'e}s de m{\'e}tier dont la matrice de cognitive de r{\'e}f{\'e}rence pr{\'e}sente le plus d{\textquoteright}affinit{\'e}s et le moins d{\textquoteright}incompatibilit{\'e}s avec le nouvel instrument. Par la suite, l{\textquoteright}innovation peut circuler vers des registres professionnels qui offrent un terreau moins favorable {\`a} son appropriation, mais c{\textquoteright}est au prix de transformations substantielles de l{\textquoteright}instrument et de ses usages.},
	number = {1},
	journal = {Politiques et Management public},
	author = {Delpeuch, Thierry},
	year = {2019},
	pages = {9--32},
	annote = {instruments d{\textquoteright}action publique, approches cognitives, innovation, cadres cognitifs, police.},
}

@article{fallon_les_2018,
	title = {Les instruments de mesure de performance au service de l{\textquoteright}action publique : une analyse compar{\'e}e des dispositifs de r{\'e}gulation de la recherche universitaire dans les communaut{\'e}s belges},
	shorttitle = {Les instruments de mesure de performance au service de l{\textquoteright}action publique},
	url = {https://orbi.uliege.be/handle/2268/228013},
	abstract = {Les discours politiques contribuent {\`a} d{\'e}finir les conceptions l{\'e}gitimes et mobilisables pour justifier un certain exercice du pouvoir : c{\textquoteright}est au niveau de leurs traductions pragmatiques au sein des dispositifs de gouvernement que se donne {\`a} lire une certaine {\guillemotleft} th{\'e}orie de l{\textquoteright}action {\guillemotright} qui propose et l{\'e}gitime le probl{\`e}me politique {\`a} r{\'e}soudre et les modalit{\'e}s d{\textquoteright}intervention pu- blique (Rose et Miller, 1992). Cet article mobilise une perspective constructiviste qui int{\`e}gre les transformations de {\guillemotleft} l{\textquoteright}{\^e}tre de l{\textquoteright}{\'E}tat {\guillemotright} et le {\guillemotleft} faire de l{\textquoteright}{\'E}tat {\guillemotright} dans le secteur de la politique universitaire : depuis 1988, les universit{\'e}s ont suivi des trajectoires tr{\`e}s diff{\'e}rentes dans les deux communaut{\'e}s, flamande et francophone, au point que le territoire belge pr{\'e}sente aujourd{\textquoteright}hui deux r{\'e}gimes politiques d{\textquoteright}enseignement sup{\'e}rieur et de recherche. En analysant au plus pr{\`e}s des acteurs quelques dispositifs r{\'e}cents en mati{\`e}re de gestion de la qualit{\'e} universitaire et de r{\'e}gulation du financement public de la recherche, cette approche compar{\'e}e contribue {\`a} mettre au jour les dynamiques sp{\'e}cifiques de normativit{\'e} au c{\oe}ur des dispositifs d{\textquoteright}{\'e}valuation qui constituent des formes de gouvernementalit{\'e} (Foucault, 1978) prenant appui sur l{\textquoteright}engagement des acteurs concern{\'e}s.},
	language = {fr},
	urldate = {2020-06-29},
	journal = {Politique et Management public},
	author = {Fallon, Catherine and Charlier, Nathan},
	year = {2018},
	annote = {instruments d{\textquoteright}action publique, politique scientifique, indicateurs.},
}

@article{baiz_pour_2018,
	title = {Pour une approche algorithmique de la nature prot{\'e}iforme et fractale des instruments de l{\textquoteright}action collective},
	volume = {35},
	issn = {0758-1726},
	url = {https://pmp.revuesonline.com/article.jsp?articleId=39932},
	doi = {10.3166/pmp.35.2018.0010},
	abstract = {{\`A} l{\textquoteright}aune de la rationalisation de l{\textquoteright}action publique et de l{\textquoteright}exigence de transparence de la d{\'e}cision politique, l{\textquoteright}analyse d{\textquoteright}impact de la r{\'e}glementation (A.I.R.) rev{\^e}t une importance plus cruciale que jamais. Or la r{\'e}glementation s{\textquoteright}appuie sur des instruments de politique publique de plus en plus divers, intriqu{\'e}s et complexes: au-del{\`a} d{\textquoteright}instruments standards tels que l{\textquoteright}obligation, l{\textquoteright}interdiction, la sensibilisation ou encore la taxe, {\'e}mergent en effet de nombreux instruments qui mettent en interaction un nombre croissant d{\textquoteright}acteurs selon des modalit{\'e}s d{\textquoteright}influence variables et sur des {\'e}chelles diverses. Il est indispensable d{\textquoteright}expliciter les contours des instruments de politique publique, et de lever la confusion s{\'e}mantique, conceptuelle et cognitive qui les entoure, et dont nous ferons {\'e}tat. {\`A} cet effet, nous proposons d{\textquoteright}abord de d{\'e}finir les instruments comme autant de cha{\^i}nes de causalit{\'e} mettant en interaction des acteurs, des actions, des op{\'e}rateurs logiques et des vecteurs d{\textquoteright}impact. Autour d{\textquoteright}un langage algorithmique ad hoc, nous d{\'e}veloppons ensuite trois leviers de (r{\'e}tro-)conception afin de rendre compte de la nature prot{\'e}iforme et fractale des instruments, en prenant l{\textquoteright}exemple de l{\textquoteright}instrument {\guillemotleft} taxe {\guillemotright}. Enfin, nous exposons succinctement les applications pratiques de cette approche algorithmique, en mati{\`e}re d{\textquoteright}{\'e}valuation de la mise en {\oe}uvre d{\textquoteright}un instrument et en mati{\`e}re d{\textquoteright}innovation.},
	number = {3-4},
	urldate = {2020-06-29},
	journal = {Politiques et management public},
	author = {Ba{\"i}z, Adam and Nakhla, Michel},
	month = dec,
	year = {2018},
	pages = {153--172},
}

@article{berkowitz_combler_2017,
	title = {Combler un vide organisationnel dans la fabrique d{\textquoteright}une politique publique : l{\textquoteright}{\'e}mergence d{\textquoteright}une m{\'e}ta-organisation},
	volume = {34},
	shorttitle = {Combler un vide organisationnel dans la fabrique d{\textquoteright}une politique publique},
	doi = {10.3166/pmp.34.2017.0003},
	abstract = {Comment assurer la r{\'e}ussite d'une fabrique plurielle d'une politique publique dans un secteur {\'e}mergent comme le financement participatif ou crowdfunding ? A travers une {\'e}tude de cas approfondie, cet article revient sur la place des plateformes de crowdfunding dans le r{\'e}seau d'acteurs cherchant {\`a} influencer la r{\'e}gulation, c'est-{\`a}-dire le policy network du crowdfunding. La m{\'e}thodologie s'appuie sur des documents juridiques et trente-et-un entretiens pour reconstituer les s{\'e}quences strat{\'e}giques de cette co-construction et les dialogues implicites entre acteurs h{\'e}t{\'e}rog{\`e}nes. Nous montrons le passage d'un mouvement collectif informel de plateformes {\`a} une m{\'e}ta-organisation sectorielle forte, l{\'e}gitime et cr{\'e}dible qui contribue {\`a} la gouvernance sectorielle en g{\'e}rant la pluralit{\'e} des acteurs et en servant d'interm{\'e}diaire de n{\'e}gociation. En entrepreneur institutionnel, la m{\'e}ta-organisation vient compl{\'e}ter le policy network {\'e}mergent, et conditionne la r{\'e}ussite de la co-construction des politiques publiques dans un secteur nouveau. Cet article contribue {\`a} d{\'e}velopper la notion de {\guillemotleft} vide organisationnel {\guillemotright} dans un policy network {\'e}mergent, i.e. l'absence d'un interm{\'e}diaire organis{\'e}, et {\`a} montrer comment peut se structurer en r{\'e}ponse, une m{\'e}ta-organisation.},
	journal = {Politiques et Management Public},
	author = {Berkowitz, Heloise and Souchaud, Antoine},
	month = oct,
	year = {2017},
	annote = {gouvernance sectorielle, co-construction de la r{\'e}gulation, m{\'e}ta-organisation, Crowdfunding, inno- vation, r{\'e}gulation financi{\`e}re.},
}

@article{fasshauer_crowdsourcing_2017,
	title = {Crowdsourcing public et innovation territoriale : le cas d{\textquoteright}un living lab rural},
	volume = {34},
	issn = {0758-1726},
	shorttitle = {Crowdsourcing public et innovation territoriale},
	url = {https://pmp.revuesonline.com/article.jsp?articleId=38663},
	doi = {10.3166/pmp.34.2017.0004},
	abstract = {Dans notre soci{\'e}t{\'e} reposant sur l{\textquoteright}{\'e}conomie de la connaissance, la comp{\'e}titivit{\'e} d{\textquoteright}une orga- nisation se d{\'e}finit par sa capacit{\'e} {\`a} innover. Cela repose sur ses capacit{\'e}s propres incluant le passage par l'intrapreneuriat, des hubs d{\textquoteright}innovation, des espaces ouverts d'innovations, des challenges internes... La direction {\guillemotleft} Innovation {\guillemotright} tend {\`a} s{\textquoteright}imposer dans les entreprises, pr{\'e}- sageant d{\'e}j{\`a} de sa transversalit{\'e} et de son importance dans la p{\'e}rennit{\'e} de l'entreprise. Des perm{\'e}abilit{\'e}s entre public et priv{\'e} sont recherch{\'e}es afin de capter l{\textquoteright}innovation d{\`e}s sa nais- sance. L'organisation priv{\'e}e, l'entreprise, a bien compris que son existence et sa comp{\'e}titivit{\'e} sont d{\'e}pendantes de sa capacit{\'e} {\`a} cr{\'e}er un r{\'e}seau o{\`u} tous les acteurs sont interd{\'e}pendants. De plus en plus d{\textquoteright}entreprises font appel {\`a} {\guillemotleft} la foule {\guillemotright} pour innover {\`a} travers le crowdsourcing (Howe, 2008). Fond{\'e}e sur le principe que chacun est expert en quelque chose, l{\textquoteright}appel {\`a} un grand nombre d{\textquoteright}acteurs permet de capter de nouvelles id{\'e}es et d{\textquoteright}innover. C{\^o}t{\'e} public, les appels {\`a} projets, les agences d'accompagnement et de sourcing sur des domaines particuliers mettent en place des r{\'e}seaux et des relations avec tous les porteurs de projets m{\^e}me quand ils ne sont pas laur{\'e}ats, afin de saisir les nouveaux changements, mouvements au niveau le plus micro : le territoire. De leur c{\^o}t{\'e} les acteurs territoriaux s{\textquoteright}organisent eux aussi pour faire valoir leurs id{\'e}es et influencer les politiques publiques. Il est donc envisageable, pour les organisa- tions publiques, comme pour les organisations priv{\'e}es de faire appel {\`a} la foule sous forme de {\guillemotleft} citizensourcing {\guillemotright} (Lukensmeyer et Torres, 2008). Notre question principale porte sur le ph{\'e}no- m{\`e}ne de crowdsourcing g{\'e}n{\'e}r{\'e} par les organisations publiques et la mani{\`e}re dont il contribue {\`a} l'innovation territoriale. Quelle forme prend-t-il ? Est-il porteur d{\textquoteright}une relation {\'e}quilibr{\'e}e entre tous les acteurs? Nous reviendrons sur l'innovation dans la sph{\`e}re publique, les acteurs et les r{\'e}seaux en pr{\'e}sence puis sur l{\textquoteright}{\'e}quilibre de la relation partenariale, ses d{\'e}terminants, pour enfin mettre en lumi{\`e}re une structure innovante le Living Lab (LL).},
	number = {1-2},
	urldate = {2020-06-29},
	journal = {Politiques et management public},
	author = {Fasshauer, Ingrid and Veil-Zadra, Cathy},
	month = jun,
	year = {2017},
	pages = {61--81},
	annote = {crowdsourcing, appels {\`a} projet, politiques publiques, innovation territoriale, Living Lab.},
}

@article{fenker_les_2017,
	title = {Les politiques nationales de d{\'e}veloppement urbain durable en {France} {\`a} l{\textquoteright}{\'e}preuve des exp{\'e}rimentations locales : le cas des {\'e}coquartiers},
	volume = {34},
	shorttitle = {Les politiques nationales de d{\'e}veloppement urbain durable en {France} {\`a} l{\textquoteright}{\'e}preuve des exp{\'e}rimentations locales},
	doi = {10.3166/pmp.34.2017.0005},
	abstract = {Cette contribution propose un {\'e}clairage sur {\guillemotleft} la fabrique des politiques publiques {\guillemotright} dans le domaine de l{\textquoteright}action urbaine sous deux angles distincts, mais indissociables. L'un concerne les modalit{\'e}s d{\textquoteright}{\'e}laboration et de diffusion d'une politique nationale en faveur de la {\guillemotleft} ville durable {\guillemotright}, en pr{\'e}cisant la fa{\c c}on dont l'{\'E}tat a mis en place, {\`a} travers l'incitation {\`a} la r{\'e}alisation {\guillemotleft} d{\textquoteright}{\'e}coquartiers {\guillemotright}, un nouveau {\guillemotleft} dispositif technique et social {\guillemotright} pour encourager et {\'e}valuer une transformation des pratiques dans l'am{\'e}nagement urbain en France. L'autre porte sur la r{\'e}alisation de ces op{\'e}rations par les collectivit{\'e}s locales, en s'int{\'e}ressant {\`a} la fa{\c c}on dont celles-ci con{\c c}oivent et r{\'e}alisent leurs projets, dans un processus d{\textquoteright}ajustement entre les ambitions de la politique nationale et les enjeux politiques et sociaux propres {\`a} leur territoire. Il appara{\^i}t ainsi que la d{\'e}marche de labellisation {\guillemotleft} {\'E}coQuartier {\guillemotright} qui cristallise un axe majeur de la mise en {\oe}uvre de la politique nationale {\guillemotleft} ville durable {\guillemotright}, s'est trouv{\'e}e au c{\oe}ur de dynamiques d{\textquoteright}articulation entre des mouvements descendants et ascendants, de coop{\'e}rations transversales entre organismes minist{\'e}riels et collectivit{\'e}s, de concertations impliquant une diversit{\'e} d'acteurs, y compris ceux de la soci{\'e}t{\'e} civile. L{\textquoteright}analyse d{\textquoteright}un corpus de 187 op{\'e}rations d{\textquoteright}{\'e}coquartiers montre que les collectivit{\'e}s locales disposent de capacit{\'e}s in{\'e}gales pour d{\'e}finir et concilier les diff{\'e}rents objectifs {\`a} poursuivre, et plus largement, pour mettre en place de nouvelles modalit{\'e}s de gouvernance et de pilotage permettant d'impliquer les citoyens dans la conception des projets, de composer avec les politiques nationales et de peser sur celles-ci. La contribution montre {\'e}galement comment la d{\'e}marche partenariale originale mise en place {\`a} l'occasion du programme {\guillemotleft} {\'E}coQuartier {\guillemotright} met {\`a} l'{\'e}preuve un fonctionnement des administrations centrales encore tr{\`e}s hi{\'e}rarchis{\'e} et sectoris{\'e} en France.},
	journal = {Politiques et management public},
	author = {Fenker, Michael and Zetlaoui-L{\'e}ger, Jodelle},
	month = jun,
	year = {2017},
	annote = {{\'e}coquartiers, d{\'e}veloppement urbain durable, pilotage et gouvernance des projets urbains, participa- tion citoyenne, politiques de d{\'e}veloppement durable, Grenelle de l{\textquoteright}Environnement.},
}

@article{fraisse_co-construire_2017,
	title = {Co-construire l{\textquoteright}action publique : {Apports} et limites des politiques locales de l{\textquoteright}{\'E}conomie sociale et solidaire en {France}},
	volume = {34},
	shorttitle = {Co-construire l{\textquoteright}action publique},
	doi = {10.3166/pmp.34.2017.0006},
	abstract = {Les politiques locales de l'{\'e}conomie sociale et solidaire (ESS) constituent un nouveau domaine de l'action publique en France. Les {\'e}lus comme les repr{\'e}sentants de l'ESS revendiquent publiquement le terme de co-construction pour qualifier leur processus d'{\'e}laboration, voire de leur mise en {\oe}uvre. Cette participation ouverte et organis{\'e}e d{\textquoteright}une pluralit{\'e} d{\textquoteright}acteurs de la soci{\'e}t{\'e} civile {\`a} la conception, au suivi et {\`a} l{\textquoteright}{\'e}valuation des politiques publiques s'explique autant par la volont{\'e} de faire de la politique autrement que par des consid{\'e}rations pragmatiques. Cet article analyse les diff{\'e}rentes configurations de cette co-construction des politiques locales de l'ESS en soulignant les potentialit{\'e}s et les limites de processus situ{\'e}s au croisement de d{\'e}mocraties repr{\'e}sentative et participative.},
	journal = {Politiques et management public},
	author = {Fraisse, Laurent},
	month = jun,
	year = {2017},
	pages = {105--120},
	annote = {{\'e}conomie sociale, {\'e}conomie solidaire, co-construction, co-production, politiques publiques, d{\'e}mocratie participative, collectivit{\'e}s locales.},
}

@article{kustosz_vers_2017,
	title = {Vers une gouvernance territorialis{\'e}e de l'innovation : la participation de la sph{\`e}re entrepreneuriale dans le cas de la sp{\'e}cialisation intelligente des r{\'e}gions},
	shorttitle = {Vers une gouvernance territorialis{\'e}e de l'innovation},
	doi = {10.3166/pmp.34},
	abstract = {La gouvernance des syst{\`e}mes de Recherche, D{\'e}veloppement et Innovation (RDI) conna{\^i}t une mutation continue depuis les ann{\'e}es 90. Une succession de r{\'e}formes et de dispositifs l{\'e}gislatifs dans les contextes communautaire, national et r{\'e}gional a contribu{\'e} {\`a} ent{\'e}riner l'id{\'e}e d'une participation accrue de la soci{\'e}t{\'e} civile {\`a} la construction des politiques publiques de recherche, de d{\'e}veloppement et d'innovation. Cette tendance traduit une {\'e}volution du rapport science et soci{\'e}t{\'e} dont les implications sont nombreuses tant sur l'organisation de la recherche publique, que sur les programmations th{\'e}matiques, les designs de recherche et les sch{\'e}mas de financement. Si la participation de la sph{\`e}re entrepreneuriale {\`a} la recherche et {\`a} l'innovation n'est {\'e}videmment pas r{\'e}cente, c'est la l{\'e}gitimation des processus de s{\'e}lection th{\'e}matique et d'articulation des financements sur la base de l'intervention et de l'expertise entrepreneuriale qui l'est davantage. Cette contribution se penche pr{\'e}cis{\'e}ment sur le cas des strat{\'e}gies r{\'e}gionales d'innovation en vue d'une sp{\'e}cialisation intelligente (SRI-SI) pour lesquelles un exercice de {\guillemotleft} d{\'e}couverte entrepreneuriale {\guillemotright} a {\'e}t{\'e} mis en oeuvre au sein des r{\'e}gions fran{\c c}aises dans le cadre de la programmation des fonds communautaires 2014-2020. Abstract Towards a territorialized Innovation Governance : the participation of the entrepreneurial sphere in the case of the smart specialization. The governance of the RDI system is altering since three decades. A succession of reforms within European, national or regional contexts contributed to confirm the idea of an active participation of firms and civil society in the construction of the public policies of research, development and innovation. This trend translates an evolution of the links between science and society impacting topics programming, designs of research and funding schemes. The participation of the entrepreneurial sphere in research and innovation activities is not obviously recent, nevertheless the legitimization of the processes of thematic choices and joint funding based on entrepreneurial expertise is increasing. This contribution presents the case of the regional innovation strategies focusing on smart specialization for which an "entrepreneurial discovery process" (EDP) has been implemented by french regions during the programming of European cohesion funds 2014-2020.},
	author = {Kustosz, Isabelle},
	month = jun,
	year = {2017},
	annote = {Sp{\'e}cialisation intelligente, D{\'e}couverte entrepreneuriale, Gouvernance, Politiques r{\'e}gionales, Syst{\`e}me de RDI.},
}

@article{meriade_les_2017,
	title = {Les effets de la {Proximit{\'e}} sur l{\textquoteright}ex{\'e}cution des politiques publiques : {L}'exemple d'un {Centre} de {Lutte} contre le {Cancer} ({CLCC})},
	volume = {34},
	shorttitle = {Les effets de la {Proximit{\'e}} sur l{\textquoteright}ex{\'e}cution des politiques publiques},
	doi = {10.3166/pmp.34.2017.000},
	abstract = {Les principes du nouveau management public infusent progressivement le secteur hospitalier, un secteur marqu{\'e} par de profondes transformations. La mani{\`e}re dont s'expriment les politiques publiques dans ces {\'e}tablissements permet de mettre en {\'e}vidence des injonctions paradoxales que les acteurs {\oe}uvrant en leur sein tentent de concilier. La mobilisation de la grille de la proximit{\'e} apporte une lecture originale des mani{\`e}res dont les politiques publiques sont ex{\'e}cut{\'e}es pour notamment faire face {\`a} ces nouveaux d{\'e}fis. Les dimensions de la proximit{\'e} sont utilis{\'e}es ici pour {\'e}tudier la confiance entre les acteurs, le contr{\^o}le organisationnel et la l{\'e}gitimit{\'e} des politiques publiques. La recherche porte sur l'{\'e}tude du cas d'un Centre de Lutte Contre le Cancer (CLCC). Cette {\'e}tude croise des donn{\'e}es secondaires (comptes rendus de r{\'e}union, pr{\'e}sentation du plan strat{\'e}gique du Centre, organigramme, rapports de gestion) et des entretiens semi-directifs aupr{\`e}s de personnels des quatre cat{\'e}gories les plus repr{\'e}sentatives du personnel hospitalier (dirigeants, m{\'e}decins, personnels de soin, personnel des fonctions support).},
	author = {Meriade, Laurent and Nautr{\'e}, Benoit and Rochette, Corinne and Talbot, Damien},
	month = jan,
	year = {2017},
	pages = {147--166},
	annote = {Politiques publiques, proximit{\'e}s, confiance, contr{\^o}le, l{\'e}gitimit{\'e}.},
}

