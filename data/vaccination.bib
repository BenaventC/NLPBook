@article{coleman_les_1991,
	title = {Les choix collectifs portant sur des questions d'int{\'e}r{\^e}t public},
	volume = {9},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1991_num_9_2_2988},
