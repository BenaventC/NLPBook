	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_2_1839},
	doi = {10.3406/pomap.1985.1839},
	abstract = {L'{\'e}conomie politique traditionnelle s'int{\'e}resse peu aux co{\^u}ts d'intervention de l'Etat et adopte le plus souvent une optique normative. Elle a donc beaucoup de mal {\`a} expliquer les raisons positives de l'appropriation publique et {\`a} analyser le fonctionnement effectif des entreprises publiques. On examine ici dans quelle mesure les analyses regroup{\'e}es sous le vocable de {\guillemotleft}th{\'e}orie {\'e}conomique de la politique{\guillemotright} sont susceptibles de combler cette lacune. La th{\'e}orie {\'e}conomique de la politique permet tout d'abord un examen syst{\'e}matique des avantages et co{\^u}ts politiques de la production publique et ceci met en relief le r{\^o}le d{\'e}terminant Jou{\'e} par les motifs de revenu, de distribution et de stabilisation, alors que la th{\'e}orie traditionnelle insiste {\`a} tort sur les motifs d'affectation des ressources. La th{\'e}orie {\'e}conomique de la politique permet aussi de bien poser le probl{\`e}me du contr{\^o}le de l'entreprise publique par le gouvernement, soit en le fondant sur des n relations de confiance{\guillemotright} avec la bureaucratie, soit en maintenant l'entreprise publique dans un environnement concurrentiel. En concentrant l'attention sur le comportement des groupes qui sont partie prenante dans l'entreprise publique, la th{\'e}orie {\'e}conomique de la politique permet enfin d'analyser de fa{\c c}on plus int{\'e}ressante le probl{\`e}me de son fonctionnement effectif.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Lafay, Jean-Dominique},
	year = {1985},
	pages = {115--140},
}

@article{ruffat_entreprise_1985,
	title = {L'entreprise publique dans la dialectique plan/march{\'e}},
	volume = {3},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_2_1838},
	doi = {10.3406/pomap.1985.1838},
	abstract = {Cet article vise {\`a} structurer la probl{\'e}matique {\'e}conomique et politique de l'interface entre l'entreprise publique et le march{\'e}, qui s'est consid{\'e}rablement d{\'e}velopp{\'e}e {\`a} la suite des nationalisations r{\'e}centes. Il d{\'e}bute par une analyse des difficult{\'e}s croissantes que rencontrent la quasi totalit{\'e} des peuples de la terre pour cr{\'e}er et pour stocker de la richesse face {\`a} la dynamique implacable et globale {\`a} laquelle est aujourd'hui soumise la valeur marchande. Il analyse ensuite le mod{\`e}le hybride que la France exp{\'e}rimente et qui tente de combiner efficacement le dynamisme esp{\'e}r{\'e} du march{\'e} avec la rationalit{\'e} suppos{\'e}e d'une r{\'e}gulation syst{\'e}matique et volontariste. Dans ce cadre l'entreprise publique subit la dynamique du march{\'e} des biens et services pour ses flux (fournitures et ventes), mais elle y {\'e}chappe pour sa structure qui n'est vraiment soumise ni au march{\'e} financier, ni au march{\'e} du travail. En cons{\'e}quence l'auteur conclut que la logique profonde du secteur public se situe hors du march{\'e}. Pourtant, tant qu'il fournira le mod{\`e}le de r{\'e}f{\'e}rence, l'entreprise publique sera d'autant plus l{\'e}gitime qu'elle y sera immerg{\'e}e. De la m{\^e}me fa{\c c}on son {\guillemotleft}autonomie de gestion{\guillemotright} sera d'autant plus justifi{\'e}e qu'elle sera confront{\'e}e aux diff{\'e}rents march{\'e}s.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Ruffat, Jean},
	year = {1985},
	pages = {99--114},
}

@article{anastassopoulos_les_1985,
	title = {Les entreprises publiques entre l'autonomie et la d{\'e}pendance : une analyse des divers instruments de r{\'e}gulation des entreprises publiques par l'{Etat}},
	volume = {3},
	issn = {0758-1726},
	shorttitle = {Les entreprises publiques entre l'autonomie et la d{\'e}pendance},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_2_1837},
	doi = {10.3406/pomap.1985.1837},
	abstract = {Les relations entre l'entreprise publique et l'Etat sont marqu{\'e}es, dans quelque pays ou contexte que ce soit, par une DIALECTIQUE opposant, d'une part, les objectifs et les strat{\'e}gies poursuivies par la premi{\`e}re, et de l'autre les politiques nationales poursuivies par le second, pour lequel l'entreprise n'est qu'un instrument. Cette dialectique se manifeste {\`a} TROIS NIVEAUX : le niveau rationnel-{\'e}conomique d'abord, o{\`u} s'expriment les conflits de fond entre ce qui est bon pour l'entreprise et ce qui est bon pour l'Etat ; le niveau organisationnel ensuite, o{\`u} ces conflits se doublent de toutes les guerres de proc{\'e}dures et de pouvoir qui se d{\'e}veloppent entre l'entreprise et les multiples tutelles, incoordonn{\'e}es autant qu'envahissantes, repr{\'e}sentant concr{\`e}tement l'Etat ; le niveau politique enfin, o{\`u} l'arbitrage des oppositions pr{\'e}c{\'e}dentes est fait en derni{\`e}re instance par des individus, des ministres ou pr{\'e}sidents, mus par des motivations personnelles fort {\'e}loign{\'e}es de la rationalit{\'e} {\'e}conomique. Pour conjuguer, dans l'entreprise, efficience et service de l'int{\'e}r{\^e}t g{\'e}n{\'e}ral, il faut am{\'e}liorer la relation avec l'Etat {\`a} ces trois niveaux. Au premier d'entre eux, l'Etat doit fixer les objectifs g{\'e}n{\'e}raux de l'entreprise et approuver le plan strat{\'e}gique qu'elle lui soumet, sans intervenir ensuite dans sa gestion. Au niveau organisationnel, la tutelle devrait {\^e}tre unique et le contr{\^o}le a posteriori rev{\^e}tir un caract{\`e}re strat{\'e}gique. Au niveau politique, l'interaction devrait se limiter {\`a} des engagements publics r{\'e}ciproques {\`a} moyen terme du ministre et du chef d'entreprise, {\`a} l'exclusion de toute manifestation d'autorit{\'e} partisane et/ou trop ponctuelle. Dans cette perspective, les {\guillemotleft}contrats de plan{\guillemotright} sont un instrument utile, {\`a} condition qu'ils soient op{\'e}rationnels aux trois niveaux.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Anastassopoulos, Jean-Pierre},
	year = {1985},
	pages = {73--98},
}

