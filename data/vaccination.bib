	journal = {Politiques et Management public},
	author = {Audikana, Ander and Messer, Marc and Kaufmann, Vincent},
	month = dec,
	year = {2016},
	pages = {197--213},
	annote = {Gouvernance, New Regionalism, M{\'e}tropole, Aire urbaine, Lib{\'e}ralisme, Coop{\'e}ration, Transport, Mobilit{\'e}.},
}

