	doi = {10.3406/pomap.1986.1915},
	abstract = {L 'objectif principal de la planification est de pr{\'e}parer {\`a} l'action future. La planification est donc bas{\'e}e sur les pr{\'e}visions et sur les hypoth{\`e}ses concernant l'environnement futur que l'on cherche {\`a} planifier. En cons{\'e}quence, il d{\'e}pend de notre habilet{\'e} {\`a} faire des pr{\'e}visions correctes que les plans soient ou ne soient pas une aide efficace {\`a} la mise en {\oe}uvre de l'activit{\'e} planifi{\'e}e. A partir de la litt{\'e}rature actuelle sur la planification strat{\'e}gique et la planification {\`a} long terme, cet article donne un descriptif rapide du processus de planification dans une universit{\'e} su{\'e}doise et pr{\'e}sente quelques exp{\'e}riences de r{\'e}alisation lorsque les plans ont {\'e}t{\'e} mis en {\oe}uvre. On a effectu{\'e} quelques recherches pour savoir dans quelle mesure les plans, {\'e}labor{\'e}s {\`a} un certain moment ont eu une influence sur la r{\'e}alit{\'e} quelques ann{\'e}es plus tard ou pour savoir si ces plans ont constitu{\'e} une aide dans la prise de d{\'e}cision pour s'adapter {\`a} cette r{\'e}alit{\'e}.},
	language = {fr},
	number = {4},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Nilsson, Thaly},
	year = {1986},
	pages = {69--113},
}

