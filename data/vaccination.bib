@article{palier_reference_1998,
	title = {La r{\'e}f{\'e}rence au territoire dans les nouvelles politiques sociales},
	volume = {16},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1998_num_16_3_2196},
	doi = {10.3406/pomap.1998.2196},
	abstract = {Dans les nouvelles politiques sociales d'insertion mises en place au cours des ann{\'e}es 1980 (politique de la ville, ZEP, politiques d'insertion professionnelle, RMI...), le territoire appara{\^i}t explicitement comme la r{\'e}f{\'e}rence centrale, depuis la conception jusqu'{\`a} la mise en {\oe}uvre de ces nouvelles formes d'intervention sociale. Ce texte analyse pr{\'e}cis{\'e}ment cette r{\'e}f{\'e}rence au territoire pour savoir si cette re-territorialisation des politiques sociales correspond {\`a} un retour {\`a} l'assistance ou bien {\`a} l'{\'e}mergence d'une nouvelle forme de citoyennet{\'e} sociale. Apr{\`e}s avoir rappel{\'e} les oppositions traditionnelles entre l'assistance et l'assurance sociale, ce texte {\'e}tudie les conditions d'{\'e}mergence des nouvelles politiques sociales. Il montre qu'elles ont {\'e}t{\'e} {\'e}labor{\'e}es en opposition au caract{\`e}re cat{\'e}goriel et sectoriel des politiques de protection sociale traditionnelles (S{\'e}curit{\'e} sociale) et qu'elles font appel au nouveau r{\'e}pertoire d'action publique, non sp{\'e}cifique au domaine social, qui s'{\'e}labore dans les ann{\'e}es 1980. L'examen des nombreuses {\'e}valuations de la mise en {\oe}uvre de ces nouvelles politiques sociales, et notamment de leur efficacit{\'e} en mati{\`e}re d'insertion, permet de conclure que si ces politiques ne sont pas un retour pur et simple aux pratiques assistancielles, elles ne parviennent pas {\`a} {\'e}laborer de nouvelles normes universelles de citoyennet{\'e} sociale, mais contribuent plut{\^o}t {\`a} une dualisation sociale en rapprochant progressivement le syst{\`e}me fran{\c c}ais de protection sociale du r{\'e}gime propre aux syst{\`e}mes anglo-saxons.},
	language = {fr},
	number = {3},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Palier, Bruno},
	year = {1998},
	pages = {13--41},
}

@article{hassenteufel_introduction_1998,
	title = {Introduction au num{\'e}ro sp{\'e}cial : {Think} {Social}, {Act} {Local} - {La} territorialisation comme r{\'e}ponse {\`a} la "crise de l'{Etat}-{Providence}" ?},
	volume = {16},
	issn = {0758-1726},
