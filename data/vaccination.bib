	abstract = {D{\'e}fini par la Statistique g{\'e}n{\'e}rale de la France en r{\'e}action aux pouss{\'e}es inflationnistes de la veille de la Premi{\`e}re Guerre mondiale l{\textquoteright}indice des prix de d{\'e}tail des 13 produits dits de consommation courante sert d{\textquoteright}abord {\`a} indexer quelques contrats. Il s{\textquoteright}{\'e}toffe et ses usages se diversifient entre les deux guerres, mais il faut attendre la cr{\'e}ation de l{\textquoteright}INSEE en 1946 pour qu{\textquoteright}il devienne une pi{\`e}ce essentielle de la r{\'e}gulation salariale. Objet de mani- pulations gouvernementales {\`a} la fin de la IVe R{\'e}publique, l{\textquoteright}indice est fortement critiqu{\'e} par les syndicats de salari{\'e}s dans les ann{\'e}es 1970 et la CGT publie un indice alternatif. Instrument d{\textquoteright}une politique fond{\'e}e sur l{\textquoteright}usage de statistiques publiques (la politique des grands nombres), l{\textquoteright}indice subit de plein fouet le virage ultralib{\'e}ral de la fin du XXe si{\`e}cle. De puissants int{\'e}r{\^e}ts priv{\'e}s, men{\'e}s par les syndicats de la grande distribution, cherchent {\`a} intervenir dans sa fabrication et les gouvernants en qu{\^e}te d{\textquoteright}{\'e}conomies budg{\'e}taires ne s{\textquoteright}y opposent pas.},
	number = {4},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Touchelay, B{\'e}atrice},
	month = dec,
	year = {2014},
	pages = {393--403},
	annote = {histoire de la France contemporaine, statistique publique, indice des prix, syndicats, n{\'e}o lib{\'e}ralisme.},
}

@article{alcaras_quand_2014,
	title = {Quand les directeurs des services sociaux d{\'e}partementaux se saisissent du management : des repr{\'e}sentations et des pratiques singuli{\`e}res et {\'e}quivoques},
	volume = {31},
	issn = {0758-1726},
	shorttitle = {Quand les directeurs des services sociaux d{\'e}partementaux se saisissent du management},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=19759},
	doi = {10.3166/pmp.31.337-356},
	abstract = {Les Conseils g{\'e}n{\'e}raux sont aujourd{\textquoteright}hui les {\guillemotleft} chefs de file {\guillemotright} de l{\textquoteright}aide et de l{\textquoteright}action sociales en France. {\`A} l{\textquoteright}heure d{\textquoteright}une rationalisation accrue des d{\'e}penses publiques et d{\textquoteright}une large diffusion du New Public Management, les hauts-fonctionnaires territoriaux en charge de ces politiques publiques ont-ils d{\'e}velopp{\'e} une culture manag{\'e}riale dans leurs services? {\`A} partir d{\textquoteright}une recherche engag{\'e}e depuis trois ans sur les repr{\'e}senta- tions et les pratiques professionnelles des directeurs d{\'e}partementaux de l{\textquoteright}aide et de l{\textquoteright}action sociales, nous montrerons que ces acteurs publics se sont aujourd{\textquoteright}hui large- ment convertis {\`a} l{\textquoteright}usage d{\textquoteright}un langage manag{\'e}rial et d{\textquoteright}un r{\'e}f{\'e}rentiel gestionnaire. Mais ce constat doit n{\'e}anmoins {\^e}tre nuanc{\'e} : ces acteurs ne donnent pas tous le m{\^e}me sens au management, et le nouveau vocabulaire qu{\textquoteright}ils emploient comme la nouvelle pos- ture qu{\textquoteright}ils adoptent ne doivent pas masquer un usage singulier et {\'e}quivoque des outils de gestion et des r{\'e}formes manag{\'e}riales qui, in fine, peuvent servir des logiques non- exclusivement gestionnaires.},
	number = {3},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Alcaras, Jean-Robert and Marchand, Christ{\`e}le and Marrel, Guillaume and Nonjon, Magali},
	month = sep,
	year = {2014},
	pages = {337--356},
	annote = {management public, management des collectivit{\'e}s territoriales, management du social, manag{\'e}rialisation de l{\textquoteright}action publique, sociologie de l{\textquoteright}action publique.},
}

@article{yatim_lexercice_2014,
	title = {L{\textquoteright}exercice de la responsabilit{\'e} manag{\'e}riale dans la sph{\`e}re publique. {Le} cas des responsables de programme dans la {LOLF}},
	volume = {31},
	issn = {0758-1726},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=19758},
	doi = {10.3166/pmp.31.319-336},
	abstract = {En France, la loi Organique relative aux Lois de Finances (LOLF) fait {\'e}voluer la logique de l{\textquoteright}action publique pour imposer une culture de gestion par les r{\'e}sultats. Ce faisant, elle implique de nombreux changements notamment en termes d{\textquoteright}{\'e}volution du r{\^o}le des hauts fonctionnaires. Dans cet article, nous nous int{\'e}ressons plus particuli{\`e}rement aux responsables de programmes de la LOLF et nous nous interrogeons sur les marges de man{\oe}uvre dont ils disposent pour exercer la responsabilit{\'e} manag{\'e}riale que la r{\'e}- forme leur attribue. Nous nous appuyons sur une recherche documentaire bas{\'e}e sur des rapports publics et en particulier le rapport bilan de la LOLF r{\'e}alis{\'e} par la Cour des Comptes fin 2011. Nous mettons en {\'e}vidence et expliquons l{\textquoteright}insuffisance des marges de man{\oe}uvre des responsables de programme ainsi que la tension existante entre leur responsabilit{\'e} manag{\'e}riale, celle des politiques et celles d{\textquoteright}autres acteurs administra- tifs. Nous en discutons les implications en mati{\`e}re de partage des responsabilit{\'e}s et de reddition des comptes.},
	number = {3},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Yatim, Fatima},
	month = sep,
	year = {2014},
	pages = {319--336},
	annote = {LOLF, responsables de programme, responsabilit{\'e} manag{\'e}riale, gestion par les r{\'e}sultats.},
}

@article{meuret_normes_2014,
