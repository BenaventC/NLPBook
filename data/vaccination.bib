@article{letort_comptabilite_2015,
	title = {La comptabilit{\'e} de gestion : un outil au service de la r{\'e}duction des d{\'e}penses publiques ? {Une} {\'e}tude de cas sur une communaut{\'e} d'agglom{\'e}ration},
