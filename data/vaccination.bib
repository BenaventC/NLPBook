	doi = {10.3166/pmp.29.519-539},
	abstract = {Les th{\'e}ories n{\'e}o-institutionnalistes ont montr{\'e} que les organisations r{\'e}pondaient fr{\'e}quemment aux changements institutionnels en adaptant leurs structures formelles sans modifier leurs pratiques r{\'e}elles. Les universit{\'e}s, du fait de leurs caract{\'e}ristiques organisationnelles, ont une capacit{\'e} particuli{\`e}re {\`a} r{\'e}aliser ces d{\'e}couplages. L{\textquoteright}{\'e}tude de cas pr{\'e}sent{\'e}e porte sur la r{\'e}ponse d{\textquoteright}une universit{\'e} fran{\c c}aise au dispositif des {\guillemotleft} initiatives d{\textquoteright}excellence {\guillemotright} lanc{\'e} dans le cadre du Grand Emprunt (ou {\guillemotleft} investissements d{\textquoteright}avenir {\guillemotright}). Contrairement {\`a} l{\textquoteright}hypoth{\`e}se ins- titutionnaliste, les dirigeants de l{\textquoteright}{\'e}tablissement font preuve d{\textquoteright}une grande r{\'e}activit{\'e} face aux attentes des autorit{\'e}s de tutelle, ce qui les am{\`e}ne {\`a} modifier sensiblement leurs valeurs et pratiques habituelles. L{\textquoteright}article montre que les nouveaux instruments d{\textquoteright}action publique mis en place dans le secteur de l{\textquoteright}enseignement sup{\'e}rieur et de la recherche en France produisent de l{\textquoteright}autodiscipline dans l{\textquoteright}{\'e}tablissement {\'e}tudi{\'e}. Cette autodiscipline s{\textquoteright}explique par l{\textquoteright}anxi{\'e}t{\'e} des dirigeants {\'e}lus de voir leur {\'e}tablissement stigmatis{\'e} en cas d{\textquoteright}{\'e}chec {\`a} l{\textquoteright}appel {\`a} projets ; elle est amplifi{\'e}e par la s{\'e}duction qu{\textquoteright}exercent sur les acteurs les mesures quantifi{\'e}es de la qualit{\'e} mobilis{\'e}es dans le pilotage des universit{\'e}s.},
	number = {3},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Mignot-G{\'e}rard, St{\'e}phanie},
	month = mar,
	year = {2012},
	pages = {519--539},
	annote = {Universit{\'e}s, Instruments d{\textquoteright}Action publique, Initiatives d{\textquoteright}excellence, Auto-discipline, N{\'e}o-institutionnalisme},
}

