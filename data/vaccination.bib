@article{nilsson_planification_1986,
	title = {Planification d'aujourd'hui et r{\'e}alit{\'e} de demain : comment les concilier ?},
	volume = {4},
	issn = {0758-1726},
	shorttitle = {Planification d'aujourd'hui et r{\'e}alit{\'e} de demain},
