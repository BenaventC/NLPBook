	doi = {10.3406/pomap.1996.2097},
	abstract = {Les programmes am{\'e}ricains d'int{\'e}gration {\`a} l'emploi con{\c c}us pour les allocataires de l'assistance sociale ont fait l'objet de nombreuses {\'e}valuations. Celles-ci {\'e}taient souvent bas{\'e}es sur la m{\'e}thode "exp{\'e}rimentale", dont les particularit{\'e}s sont pr{\'e}sent{\'e}es. Les r{\'e}sultats obtenus gr{\^a}ce {\`a} ce type d'{\'e}valuations, souvent consid{\'e}r{\'e} aux Etats-Unis comme des plus fiables, ont favoris{\'e} la formation d'un consensus en faveur du d{\'e}veloppement de "mesures actives" dans le domaine de l'assistance sociale. Celles-ci semblaient en effet se traduire par des augmentations de gains pour les participants et par des {\'e}conomies budg{\'e}taires au chapitre de l'aide sociale. Cependant, elles ne permettaient pas de r{\'e}duire la pauvret{\'e} de fa{\c c}on notable. L'utilisation de la m{\'e}thode exp{\'e}rimentale soul{\`e}ve de nombreuses critiques, tant sur le plan technique que du point de vue {\'e}thique. Des efforts nouveaux doivent {\^e}tre d{\'e}ploy{\'e}s en direction d'autres m{\'e}thodes d'{\'e}valuation, comme les {\'e}tudes de processus, afin d'enrichir le champ des pratiques {\'e}valuatives am{\'e}ricaines.},
	language = {fr},
	number = {2},
	urldate = {2020-06-26},
	journal = {Politiques et management public},
	author = {Morel, Sylvie},
	year = {1996},
	pages = {33--58},
}

