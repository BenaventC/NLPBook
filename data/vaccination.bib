}

@article{e_ashford_les_1985,
	title = {Les aspects politiques des politiques de solidarit{\'e} sociale},
	volume = {3},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_1_1819},
	doi = {10.3406/pomap.1985.1819},
	abstract = {Il est impossible de saisir par des mod{\`e}les {\'e}conomiques et sociaux toute la complexit{\'e} de l'{\'e}laboration d'une politique dans le cadre actuel de l'Etat, marqu{\'e} par la solidarit{\'e} sociale. On a n{\'e}glig{\'e} les probl{\`e}mes politiques pos{\'e}s par la solidarit{\'e} sociale, en grande partie parce qu'on s'est refus{\'e} {\`a} consid{\'e}rer, sous un angle comparatif, les dilemmes et probl{\`e}mes particuliers que soul{\`e}ve l'{\'e}laboration d'une politique dans le domaine de la solidarit{\'e} sociale. Si on situe le d{\'e}veloppement de cette solidarit{\'e} dans une perspective historique, on constate que la fa{\c c}on dont les premi{\`e}res mesures et les premiers programmes ont {\'e}t{\'e} justifi{\'e}s, con{\c c}us et mis en {\oe}uvre, a eu des effets durables sur la mani{\`e}re dont les services de solidarit{\'e} sociale sont assur{\'e}s. Afin d'illustrer les tr{\`e}s grandes diff{\'e}rences que l'on constate dans l'{\'e}volution des diverses nations, on compare la mani{\`e}re dont certains services sociaux locaux sont fournis dans trois pays (services sociaux personnels en Grande-Bretagne ; aide sociale en France ; centres de soins communautaires aux Etats-Unis). Dans chaque cas, nous avons trouv{\'e} que la fa{\c c}on dont les mesures ont {\'e}t{\'e} adopt{\'e}es s'est conform{\'e}e aux donn{\'e}es territoriales et {\`a} la politique budg{\'e}taire qui caract{\'e}risent chaque syst{\`e}me. Malgr{\'e} leur immense importance dans la vie politique de toutes les d{\'e}mocraties modernes, les aspects politiques de la solidarit{\'e} sociale sont li{\'e}s aux circonstances historiques, constitutionnelles et politiques plus g{\'e}n{\'e}rales qui ont model{\'e} chaque d{\'e}mocratie.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {E. Ashford, Douglas},
	year = {1985},
	pages = {107--133},
}

@article{hayward_instruments_1985,
	title = {Instruments de mise en {\oe}uvre des politiques gouvernementales et relations centre-p{\'e}riph{\'e}rie en {Grande}-{Bretagne} et en {France}},
	volume = {3},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_1_1818},
	doi = {10.3406/pomap.1985.1818},
	abstract = {Dans cet article relatif {\`a} la phase ultime du processus politique, les auteurs {\'e}tudient les changements intervenus dans le type d'instruments utilis{\'e}s par les gouvernements centraux pour la mise en oeuvre de leurs politiques ; sont examin{\'e}es les raisons pour lesquelles ces gouvernements tendent {\`a} privil{\'e}gier des instruments de plus ou moins forte contrainte ainsi que les cons{\'e}quences de l'utilisation de combinaisons diff{\'e}rentes de tels moyens. L'emploi de plus en plus fr{\'e}quent de proc{\'e}dures de type contractuel en France est ainsi oppos{\'e} au d{\'e}veloppement d'interventions de nature r{\'e}glementaire en Grande-Bretagne ; les m{\'e}thodes employ{\'e}es en mati{\`e}re de contr{\^o}le des d{\'e}penses locales entra{\^i}nent certaines difficult{\'e}s lors de la mise en oeuvre de la politique du gouvernement britannique, alors que l'utilisation d'instruments de moindre contrainte semble, en France, devoir favoriser une meilleure coordination des priorit{\'e}s d{\'e}finies aux niveaux local et central. L'hypoth{\`e}se selon laquelle existerait une tendance g{\'e}n{\'e}rale {\`a} l'utilisation d'instruments plus contraignants dans les soci{\'e}t{\'e}s industrielles ne peut {\^e}tre confirm{\'e}e. Pour conclure, les auteurs soulignent que si les instruments h{\'e}rit{\'e}s du pass{\'e} sont parfois adapt{\'e}s et utilis{\'e}s avec plus de vigueur qu'auparavant pour l'application de mesures nouvelles, aucun instrument nouveau de contrainte n'est mis en place dans le cadre de politiques traditionnelles.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Hayward, Jack and Page, Edward},
	year = {1985},
	pages = {81--105},
}

@article{rose_responsabilite_1985,
	title = {La responsabilit{\'e} devant les {\'e}lecteurs et devant le march{\'e} : les choix offerts aux organisations publiques},
	volume = {3},
	issn = {0758-1726},
	shorttitle = {La responsabilit{\'e} devant les {\'e}lecteurs et devant le march{\'e}},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_1_1816},
	doi = {10.3406/pomap.1985.1816},
	abstract = {Comment les organisations publiques peuvent-elles doser de fa{\c c}on convenable leur responsabilit{\'e} devant les {\'e}lecteurs et leur responsabilit{\'e} devant le march{\'e} ? La premi{\`e}re partie examine les diff{\'e}rences dans la responsabilit{\'e} {\`a} l'{\'e}gard du march{\'e} et {\`a} l'{\'e}gard des {\'e}lecteurs que pr{\'e}sentent, au sein du secteur public, des organisations g{\'e}rant divers types de programme. En second lieu, on {\'e}tudie les implications politiques d'une responsabilit{\'e} imparfaite dans les deux sph{\`e}res. Si on teste les effets des diff{\'e}rences dans la responsabilit{\'e} des organisations sur la croissance des programmes publics, on d{\'e}couvre que les organisations responsables devant le march{\'e} et devant les {\'e}lecteurs se d{\'e}veloppent beaucoup moins vite que les organisations qui ne subissent que les pressions des producteurs et des sp{\'e}cialistes. La derni{\`e}re partie expose les principes d'une affectation marginale des ressources entre des organisations ayant des responsabilit{\'e}s de type diff{\'e}rent, dans le cadre des contraintes budg{\'e}taires tr{\`e}s strictes qui caract{\'e}risent les ann{\'e}es quatre-vingts. La combinaison des d{\'e}marches fond{\'e}es sur les {\'e}lecteurs et sur le march{\'e} peut am{\'e}liorer la r{\'e}partition budg{\'e}taire et la justice sociale.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Rose, Richard},
	year = {1985},
	pages = {49--77},
}

@article{laufer_gouvernabilite_1985,
	title = {Gouvernabilit{\'e} et management des syst{\`e}mes administratifs complexes},
	volume = {3},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_1_1815},
	doi = {10.3406/pomap.1985.1815},
	abstract = {Gouverner, c'est diriger {\`a} la fa{\c c}on dont on dirige une embarcation {\`a} l'aide d'un gouvernail. Pour gouverner, il faut disposer d'un savoir relatif {\`a} la carte de l'oc{\'e}an et au fonctionnement du bateau. Il faut {\'e}galement disposer d'une autorit{\'e} reconnue. La possibilit{\'e} du gouvernement d'un syst{\`e}me administratif d{\'e}pend donc de la question de la l{\'e}gitimit{\'e} du savoir, de la question de la l{\'e}gitimit{\'e} de l'autorit{\'e} et de la fa{\c c}on dont est r{\'e}alis{\'e}e l'articulation entre savoir et autorit{\'e} au sein de ce syst{\`e}me. Une d{\'e}finition pr{\'e}cise de la notion de l{\'e}gitimit{\'e} permet de retracer l'histoire de la l{\'e}gitimit{\'e} du lib{\'e}ralisme. Elle permet de constater que la crise du fondement de l'autorit{\'e} l{\'e}gitime (crise du droit) et que la crise du savoir l{\'e}gitime (crise de la science) correspondent {\`a} l'{\'e}mergence de syst{\`e}mes administratifs complexes. Ces syst{\`e}mes sont caract{\'e}ris{\'e}s par le fait qu'ils sont directement confront{\'e}s au probl{\`e}me de l'articulation entre le local et le global. Il est montr{\'e} que le management ne porte pas {\`a} lui seul la solution aux probl{\`e}mes des syst{\`e}mes administratifs complexes. Toutefois, du fait que les syst{\`e}mes administratifs deviennent complexes, ils ne peuvent plus ignorer le management dans leur fonctionnement quel que soit leur statut juridique.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Laufer, Romain},
	year = {1985},
	pages = {25--48},
}

@article{crozier_les_1985,
	title = {Les probl{\`e}mes du management public face {\`a} la transformation de l'environnement},
	volume = {3},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_1_1814},
	doi = {10.3406/pomap.1985.1814},
	abstract = {L'Administration publique a davantage de difficult{\'e}s {\`a} s'adapter aux transformations acc{\'e}l{\'e}r{\'e}es de son environnement que les entreprises priv{\'e}es. Apr{\`e}s avoir analys{\'e} les trois grands types de r{\'e}actions de l'Administration fran{\c c}aise face {\`a} la pression croissante de son environnement, fauteur met l'accent sur les contradictions qu'impose au management public l'opposition entre la volont{\'e} de maintenir des principes traditionnels d'unification et de standardisation et une r{\'e}alit{\'e} d'activit{\'e}s de plus en plus diversifi{\'e}es. L 'auteur distingue ensuite quatre types de m{\'e}tiers dans la fonction publique auxquels il n'est pas possible d'appliquer les m{\^e}mes r{\`e}gles et plaide pour une mutation intellectuelle rendant possible une gestion diversifi{\'e}e de la fonction publique.},
	language = {fr},
	number = {1},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Crozier, Michel},
	year = {1985},
	pages = {9--23},
}

@article{gerbaux_politique_1985,
	title = {La politique fran{\c c}aise de la montagne : les difficult{\'e}s d'un compromis social},
	volume = {3},
	issn = {0758-1726},
	shorttitle = {La politique fran{\c c}aise de la montagne},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_2_1846},
	doi = {10.3406/pomap.1985.1846},
	abstract = {En 1972, le th{\`e}me dominant de la politique de la montagne est la n{\'e}cessit{\'e} d'une coh{\'e}rence de l'am{\'e}nagement, puis en 1981 l'urgence d'une politique globale s'appuyant sur les initiatives locales. La recherche d'un compromis social autour de cette probl{\'e}matique est, selon nous, la source des difficult{\'e}s rencontr{\'e}es dans sa mise en oeuvre parce qu'elle renvoie {\`a} la difficile, mais n{\'e}cessaire, coordination des strat{\'e}gies des organisations publiques. En permanence, l'intervention de la D{\'e}l{\'e}gation {\`a} l'Am{\'e}nagement du Territoire et {\`a} l'Action R{\'e}gionale a {\'e}t{\'e} centrale puisqu'elle a jou{\'e} le r{\^o}le d'un catalyseur, s'efforcent de mobiliser les forces politiques et sociales ainsi que les administrations sectorielles centrales et les services ext{\'e}rieurs autour de la question montagnarde. Les pratiques nouvelles instaur{\'e}es entre le centre et la p{\'e}riph{\'e}rie {\`a} travers l'{\'e}laboration d'une politique globale {\`a} des niveaux infranationaux comme {\guillemotleft}le massif de montagne{\guillemotright} ne r{\'e}v{\`e}lent- elles pas une tentative de {\guillemotleft}d{\'e}sectoriser{\guillemotright} l'action de l'Etat ?},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Gerbaux, Fran{\c c}oise},
	year = {1985},
	pages = {227--238},
}

