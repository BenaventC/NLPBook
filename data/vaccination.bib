	journal = {Politiques et management public},
	author = {B{\'e}har, Laurie and Desmoulins, Vincent},
	month = jun,
	year = {2014},
	pages = {151--167},
	annote = {bien public, environnement, expertise, participation politique, syst{\`e}me politique local.},
}

