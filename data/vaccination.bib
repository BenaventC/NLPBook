	doi = {10.3166/pmp.33.27-48},
	abstract = {La coordination des soins est un enjeu de politiques publiques avec des implications sani- taires, sociales et {\'e}conomiques majeures. En France comme dans d{\textquoteright}autres pays, de nom- breuses r{\'e}formes structurelles ont {\'e}t{\'e} mises en place. L{\textquoteright}objectif premier de ces r{\'e}formes est d{\textquoteright}assurer pour les patients des parcours mieux coordonn{\'e}s, voire int{\'e}gr{\'e}s. Or, les diff{\'e}- rentes exp{\'e}rimentations men{\'e}es montrent que le levier structurel est insuffisant et qu{\textquoteright}une meilleure prise en compte des dynamiques locales est indispensable. Partant de ce constat, le but de cet article est de mieux comprendre ces dynamiques locales et les besoins qui s{\textquoteright}en d{\'e}duisent en mati{\`e}re d{\textquoteright}action publique. Pour ce faire, nous nous basons sur l{\textquoteright}{\'e}tude d{\textquoteright}une initiative locale, le dispositif {\guillemotleft} Parcours de Soins coordonn{\'e} {\textendash} PSC {\guillemotright} mis en place dans un {\'e}tablissement d{\textquoteright}oncologie. Cette {\'e}tude de cas nous permet de montrer les diff{\'e}rents enjeux li{\'e}s {\`a} la mise en {\oe}uvre d{\textquoteright}un dispositif de coordination lorsque l{\textquoteright}on se situe {\`a} l{\textquoteright}{\'e}che- lon local. Nous en d{\'e}gageons les implications en termes d{\textquoteright}action publique, en proposant un mod{\`e}le adapt{\'e} {\`a} la coordination des soins : si la dynamique des acteurs de terrain est fondamentale, elle ne peut se concr{\'e}tiser qu{\textquoteright}en lien avec une action publique accompagna- trice, dans un cadre structurel int{\'e}grateur et avec un objectif d{\textquoteright}{\'e}valuation r{\'e}fl{\'e}chi en amont.},
	number = {1},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Yatim, Fatima and Minvielle, Etienne},
	month = mar,
	year = {2016},
	pages = {27--48},
	annote = {coordination des soins, action publique, dynamiques locales.},
}

@article{guerin-schneider_y_2016,
