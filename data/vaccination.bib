@article{boudet_gestion_2011-1,
	title = {La gestion publique au prisme du d{\'e}veloppement durable : l{\textquoteright}{\guillemotleft} administration exemplaire {\guillemotright}},
	volume = {28},
	issn = {0758-1726},
	shorttitle = {La gestion publique au prisme du d{\'e}veloppement durable},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=17054},
	doi = {10.3166/pmp.28.533-545},
	abstract = {{\'E}tat {\guillemotleft} interventionniste {\guillemotright}, {\'E}tat {\guillemotleft} strat{\`e}ge {\guillemotright}, {\'E}tat {\guillemotleft} performant {\guillemotright}, {\'E}tat {\guillemotleft} propri{\'e}taire {\guillemotright}, les ad- jectifs ne manquent plus aujourd{\textquoteright}hui pour qualifier l{\textquoteright}{\'E}tat. {\`A} ces vocables convient-il d{\'e}sor- mais d{\textquoteright}y ajouter celui d{\textquoteright}{\guillemotleft} exemplaire {\guillemotright}. La Strat{\'e}gie nationale de d{\'e}veloppement durable (SNDD) traduit en 2003 cette n{\'e}cessit{\'e} pour l{\textquoteright}{\'E}tat fran{\c c}ais de devenir exemplaire et fixe en particulier des objectifs et des indicateurs chiffr{\'e}s (r{\'e}duction de 10 \% de la consom- mation d{\textquoteright}{\'e}nergie ; r{\'e}duction de 10 \% de la consommation d{\textquoteright}eau ; r{\'e}alisation de 50 \% des constructions de l{\textquoteright}{\'E}tat avec des techniques {\'e}prouv{\'e}es pour pr{\'e}server l{\textquoteright}environnement (HQE par exemple) ; achat de 20 \% de v{\'e}hicules moins polluants lors de renouvellements des flottes de v{\'e}hicules existants). Ces quelques chiffres appliqu{\'e}s {\`a} l{\textquoteright}{\'E}tat et {\`a} son adminis- tration montrent que ces derniers doivent avoir les m{\^e}mes probl{\'e}matiques que les autres personnes morales ou physiques, si ce n{\textquoteright}est plus. La Charte de l{\textquoteright}environnement (2004) et le Grenelle de l{\textquoteright}environnement (2007) n{\textquoteright}{\'e}chappent pas {\`a} l{\textquoteright}administration; l{\textquoteright}{\'E}tat s{\textquoteright}impose les normes optimales en mati{\`e}re d{\textquoteright}environnement et de d{\'e}veloppement durable. Cette d{\'e}marche le conduit {\`a} mesurer le co{\^u}t de fonctionnement de son administration et {\`a} rationaliser ses d{\'e}penses en consid{\'e}rant les {\'e}l{\'e}ments du d{\'e}veloppement durable : r{\'e}duc- tion du co{\^u}t des loyers priv{\'e}s pay{\'e}s par l{\textquoteright}{\'E}tat ; r{\'e}duction des surfaces utilis{\'e}es par les agents publics; regroupement des administrations centrales minist{\`e}re par minist{\`e}re; mutualisa- tion ou externalisation des moyens affect{\'e}s {\`a} la repr{\'e}sentation de l{\textquoteright}{\'E}tat dans les pr{\'e}fec- tures (entretien des espaces verts, nettoyages, garages, cuisine). {\guillemotleft} Exemplaires {\guillemotright}, l{\textquoteright}{\'E}tat et son administration doivent en cons{\'e}quence calculer leurs charges fixes pour les r{\'e}duire au mieux dans une d{\'e}marche {\'e}coresponsable.},
	number = {4},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Boudet, Jean-Fran{\c c}ois},
	month = dec,
	year = {2011},
	pages = {533--545},
	annote = {Gestion et finances publiques, administration exemplaire, d{\'e}veloppement durable, r{\'e}formes de l{\textquoteright}{\'E}tat},
}

@article{hautbois_sport_2011,
	title = {Le sport dans la strat{\'e}gie de communication des collectivit{\'e}s territoriales : le cas de la {Seine}-{Saint}-{Denis}},
	volume = {28},
	issn = {0758-1726},
	shorttitle = {Le sport dans la strat{\'e}gie de communication des collectivit{\'e}s territoriales},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=17053},
	doi = {10.3166/pmp.28.509-530},
	abstract = {Depuis les ann{\'e}es 1990, le marketing territorial appara{\^i}t comme un domaine de recherche original, f{\'e}cond bien que peu explor{\'e}. Les travaux de Sperling (1991 ; 1995), Girard (1999) mais aussi de Cohen-Bacrie (2003) et Hatem (2007) ont progressivement structur{\'e} le cadre de cette r{\'e}flexion. Le marketing territorial consiste, pour reprendre la d{\'e}finition de Hatem (2007), {\`a} {\guillemotleft} am{\'e}liorer la part de march{\'e} d{\textquoteright}un territoire donn{\'e} dans les flux nationaux et inter- nationaux de diff{\'e}rentes natures {\guillemotright}. Pour atteindre un tel objectif, les {\'e}lus locaux doivent concevoir une strat{\'e}gie de communication adapt{\'e}e. Diff{\'e}rents leviers peuvent d{\`e}s lors {\^e}tre mobilis{\'e}s par les manageurs publics. Le sport en g{\'e}n{\'e}ral appara{\^i}t {\`a} leurs yeux comme un vecteur pertinent de communication territoriale. Cet article vise {\`a} interroger le r{\^o}le strat{\'e}gique des acteurs publics dans la construction d{\textquoteright}une communication territoriale. Il s{\textquoteright}appuie sur une {\'e}tude longitudinale (10 ans) portant sur le d{\'e}partement de Seine-Saint-Denis. Des entretiens semi-directifs ont {\'e}t{\'e} r{\'e}alis{\'e}s aupr{\`e}s des responsables publics ayant {\'e}t{\'e} au c{\oe}ur de la politique de communication durant la d{\'e}cen- nie 1998-2008. Leur analyse fait appara{\^i}tre quatre r{\^o}les principaux : l{\textquoteright}acteur public {\guillemotleft} specta- teur {\guillemotright}, l{\textquoteright}acteur public {\guillemotleft} opportuniste {\guillemotright}, l{\textquoteright}acteur public {\guillemotleft} accompagnateur {\guillemotright} et l{\textquoteright}acteur public {\guillemotleft} moteur {\guillemotright}.},
	number = {4},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Hautbois, Christopher and Desbordes, Michel},
	month = dec,
	year = {2011},
	pages = {509--530},
	annote = {management public, communication, sport, image, strat{\'e}gie, collectivit{\'e}s locales, Seine-Saint-Denis},
}

@article{biondi_les_2013,
