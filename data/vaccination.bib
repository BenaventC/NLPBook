	year = {1989},
	pages = {217--235},
}

@article{proulx_les_1989,
	title = {Les effets pervers d'une strat{\'e}gie de r{\'e}duction des d{\'e}penses publiques : le cas des tribunaux d'instance fran{\c c}ais},
