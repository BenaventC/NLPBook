	abstract = {Les universit{\'e}s, au m{\^e}me titre que l{\textquoteright}ensemble du secteur public, sont actuellement soumises {\`a} des transformations majeures et la conduite du changement devient une pr{\'e}occupation de premier plan. Le pr{\'e}sent article s{\textquoteright}int{\'e}resse {\`a} la mise en place d{\textquoteright}une mission d{\textquoteright}interm{\'e}diation, {\`a} cr{\'e}er de toutes pi{\`e}ces et con{\c c}ue au d{\'e}part comme un outil d{\textquoteright}am{\'e}lioration de l{\textquoteright}organisation, tout en revendiquant un r{\^o}le de m{\'e}diation interne. Le travail du m{\'e}diateur est analys{\'e} pour montrer comment le contexte am{\`e}ne le tiers {\`a} venir en appui {\`a} l{\textquoteright}accompagnement au changement. La tension entre le projet politique {\`a} l{\textquoteright}origine de la mission d{\textquoteright}une part, et l{\textquoteright}exigence de neutralit{\'e} du m{\'e}diateur d{\textquoteright}autre part, se trouve {\^e}tre un {\'e}l{\'e}ment d{\'e}terminant de l{\textquoteright}{\'e}quilibre {\`a} trouver.},
	journal = {Politiques et Management Public},
	author = {Dubernet, Catherine},
	month = jan,
	year = {2016},
	pages = {285--305},
	annote = {M{\'e}diation, risques psychosociaux, changement, dysfonctionnement, d{\'e}lib{\'e}ration.},
}

@article{brest_lolf_2016,
	title = {La {Lolf} entre dans les {\'e}tablissements scolaires en {France} : institutionnalisation d{\textquoteright}une r{\'e}forme manag{\'e}riale ou bureaucratique ?},
	volume = {33},
	shorttitle = {La {Lolf} entre dans les {\'e}tablissements scolaires en {France}},
	doi = {10.3166/pmp.33.2.97-117},
	abstract = {L{\textquoteright}article porte sur une r{\'e}forme modifiant, dans la perspective de la Lolf, le cadre budg{\'e}taire d{\textquoteright}organisations publiques : les {\'e}tablissements publics locaux d'enseignement. Rattach{\'e} au courant th{\'e}orique de l{\textquoteright}institutional work, ce travail met en {\'e}vidence un processus combinant, {\`a} diff{\'e}rents niveaux organisationnels, des activit{\'e}s de cr{\'e}ation, de soutien et de perturbation institutionnelle. L{\textquoteright}analyse sociotechnique r{\'e}alis{\'e}e indique qu{\textquoteright}en raison de la latitude d{\'e}cision- nelle dont disposent les parties prenantes, des interpr{\'e}tations locales de la r{\'e}forme peuvent orienter sa mise en {\oe}uvre. En cons{\'e}quence, le processus budg{\'e}taire d{\'e}cid{\'e} au sommet hi{\'e}- rarchique peut contribuer {\`a} dynamiser le management des organisations vis{\'e}es ou se r{\'e}duire {\`a} une proc{\'e}dure bureaucratique, pour une large part, en fonction de la confrontation sur le terrain des points de vue des parties prenantes.},
	journal = {Politiques et management public},
	author = {Brest, Philippe},
