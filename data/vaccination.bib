	doi = {10.3166/pmp.32.185-202},
	abstract = {L{\textquoteright}article vise {\`a} apporter une r{\'e}flexion sur les apprentissages issus de plusieurs ann{\'e}es d{\textquoteright}im- plantation d{\textquoteright}une politique publique transversale touchant l{\textquoteright}ensemble des minist{\`e}res, ici les politiques publiques favorables {\`a} la sant{\'e} (PPFS). {\`A} partir d{\textquoteright}une analyse des menaces pass{\'e}es, actuelles et {\`a} court terme pour la mise en {\oe}uvre des PPFS au Qu{\'e}bec, nous d{\'e}ga- geons des pistes d{\textquoteright}action pour le futur. Les menaces ont {\'e}t{\'e} identifi{\'e}es {\`a} partir d{\textquoteright}entrevues avec les acteurs {\oe}uvrant {\`a} la prise en compte de la sant{\'e} dans les politiques publiques. Notre analyse est centr{\'e}e sur les menaces relatives aux aspects communicationnels. La d{\'e}marche d{\textquoteright}identification des pistes d{\textquoteright}action proc{\`e}de en quatre {\'e}tapes : la docu- mentation des menaces, l{\textquoteright}identification d{\textquoteright}options r{\'e}pondant aux menaces, la docu- mentation de r{\'e}sultats probants, la possibilit{\'e} d{\textquoteright}implantation dans le contexte local. Cette d{\'e}marche nous para{\^i}t {\^e}tre une alternative relativement rapide et syst{\'e}matique pour documenter une situation et en d{\'e}duire logiquement des axes d{\textquoteright}intervention. Centrer l{\textquoteright}analyse sur les aspects communicationnels appara{\^i}t pertinent pour approfon- dir les menaces et d{\'e}gager des pistes d{\textquoteright}action sp{\'e}cifiques aux politiques transversales. D{\textquoteright}autres initiatives transversales, comme les strat{\'e}gies de d{\'e}veloppement durable, de lutte contre la pauvret{\'e}, de lutte contre le changement clinique, pourraient b{\'e}n{\'e}ficier d{\textquoteright}une telle d{\'e}marche.},
	number = {2},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Beguinet, Sophie},
	month = jun,
	year = {2015},
	pages = {185--202},
	annote = {politiques publiques favorables {\`a} la sant{\'e}, analyse des menaces, action horizontale, {\'e}valuation d{\textquoteright}impacts en sant{\'e}, communication.},
}

@article{chassy_critique_2015,
