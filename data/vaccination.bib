@book{boutin_conflits_nodate,
	title = {Conflits environnementaux, milieux naturels et d{\'e}cisions publiques : {\'E}tude de deux cas industriels {\textbackslash}textbar {Request} {PDF}},
	shorttitle = {Conflits environnementaux, milieux naturels et d{\'e}cisions publiques},
	url = {https://www.researchgate.net/publication/337809735_Conflits_environnementaux_milieux_naturels_et_decisions_publiques_Etude_de_deux_cas_industriels},
	abstract = {Au travers d{\textquoteright}{\'e}tudes de cas, on explore ici la dynamique de deux conflits environnementaux portant sur des projets industriels affectant de mani{\`e}re significative des milieux naturels et on consid{\`e}re l{\textquoteright}interaction des d{\'e}cisions publiques et priv{\'e}es. Le premier cas met en sc{\`e}ne l{\textquoteright}usine d{\textquoteright}alumine ALTEO {\`a} Gardanne qui a rejet{\'e} des boues rouges dans la M{\'e}diterran{\'e}e de 1967 {\`a} 2015 et qui a {\'e}t{\'e} autoris{\'e}e ensuite {\`a} poursuivre un rejet d{\textquoteright}eau sans {\'e}l{\'e}ments solides mais en d{\'e}rogation {\`a} certaines normes. Le second cas concerne le projet d{\textquoteright}une unit{\'e} {\guillemotleft} biomasse {\guillemotright} par le groupe E.ON pour alimenter une centrale {\'e}lectrique dans la m{\^e}me ville. Cette tr{\`e}s grosse unit{\'e} repr{\'e}sente un enjeu pour les milieux terrestres car la ressource sera pr{\'e}lev{\'e}e en partie dans un environnement subissant de multiples pressions. On d{\'e}crit la dynamique des conflits, en identifiant les parties-prenantes et en mettant en exergue l{\textquoteright}encha{\^i}nement de leurs d{\'e}cisions et en s{\textquoteright}attachant particuli{\`e}rement {\`a} celles des d{\'e}cisionnaires publics. On constate l{\textquoteright}imbrication de niveaux d{\'e}cisionnels publics face {\`a} des enjeux li{\'e}s aux milieux naturels car ceux-ci transcendent les d{\'e}coupages politiques et administratifs. On observe aussi que les attitudes des parties-prenantes, au-del{\`a} des int{\'e}r{\^e}ts av{\'e}r{\'e}s, expriment des {\guillemotleft} valeurs {\guillemotright} accord{\'e}es {\`a} la nature, dont les divergences sont amplifi{\'e}es par la dispersion et la fragmentation des savoirs relatifs au fonctionnement des milieux naturels. Le recueil de 1 184 prises de position permet d{\textquoteright}associer les attitudes envers les projets aux cat{\'e}gories de parties-prenantes.},
	language = {en},
	urldate = {2020-06-29},
	author = {Boutin, Nathalie and Batteau, Pierre and Gachet, Sophie and Raynal, Jean-Claude},
	note = {Publication Title: ResearchGate},
	annote = {d{\'e}cision publique, conflits environnementaux, pollution industrielle, parties prenantes.},
}

@book{emil_dimensions_nodate,
