@article{douillet_centralisation_2014,
	title = {Une centralisation renforc{\'e}e par le chiffre ? {Les} effets contradictoires des indicateurs chiffr{\'e}s dans la police nationale en {France}},
	volume = {31},
	issn = {0758-1726},
	shorttitle = {Une centralisation renforc{\'e}e par le chiffre ?},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=20145},
	doi = {10.3166/pmp.31.421-442},
	abstract = {Les indicateurs chiffr{\'e}s ont pris une importance croissante au sein de la Police Nationale dans un contexte d{\textquoteright}introduction du nouveau management public et de recours croissant aux nouvelles technologies. Au-del{\`a} de l{\textquoteright}usage politique de ces chiffres, l{\textquoteright}article s{\textquoteright}inter- roge sur leurs effets concrets sur les pratiques et des priorit{\'e}s de la police, en particulier sur un {\'e}ventuel renforcement du caract{\`e}re centralis{\'e} de l{\textquoteright}organisation polici{\`e}re. Le poids grandissant des chiffres s{\textquoteright}est traduit par une utilisation accrue de diff{\'e}rentes bases de donn{\'e}es comme la Main Courante Informatis{\'e}e et autres tableaux de bord, par l{\textquoteright}intro- duction de primes {\`a} la performance ainsi que par un renforcement des {\'e}quipes char- g{\'e}es de collecter et de transmettre les donn{\'e}es. La n{\'e}cessit{\'e} de rendre des comptes et de faire remonter l{\textquoteright}information {\`a} tous les {\'e}chelons de la hi{\'e}rarchie, du commissariat au minist{\`e}re, a contribu{\'e} {\`a} un renforcement de la pyramide hi{\'e}rarchique tout en r{\'e}duisant la marge d{\textquoteright}autonomie des agents. Ceux-ci ressentent une forme de frustration face {\`a} des demandes de plus en plus pressantes d{\textquoteright}information et de r{\'e}sultats {\'e}manant de la hi{\'e}- rarchie. En d{\'e}coulent un certain nombre de strat{\'e}gies d{\textquoteright}{\'e}vitement, d{\textquoteright}adaptation, voire de tricherie, afin de fournir de bons chiffres en r{\'e}ponse aux objectifs assign{\'e}s par le niveau sup{\'e}rieur, souvent suspect{\'e} de se soucier uniquement des statistiques au d{\'e}triment de l{\textquoteright}efficacit{\'e} du {\guillemotleft} vrai {\guillemotright} travail policier. Les effets centralisateurs sont ainsi contrebalanc{\'e}s par des pratiques centrifuges.},
	number = {4},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Douillet, Anne-C{\'e}cile and de Maillard, Jacques and Zagrodski, Mathieu},
	month = dec,
	year = {2014},
	pages = {421--442},
	annote = {police, management, centralisation, indicateurs, chiffres, statistiques.},
}

@article{boulet_roquantification_2014,
	title = {Le r{\^o}le de la quantification dans le processus de marchandisation de l'universit{\'e}. {\'e}tude de cas d'une universit{\'e} anglaise},
	volume = {31},
	issn = {0758-1726},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=20144},
	doi = {10.3166/pmp.31.405-420},
	abstract = {Dans le processus de marchandisation du secteur universitaire, {\`a} l{\textquoteright}{\oe}uvre en Europe depuis le d{\'e}but des ann{\'e}es 1980, la quantification joue un r{\^o}le central, en {\'e}tablissant des {\'e}qui- valences et des hi{\'e}rarchies (classements) : elle permet de traiter des universit{\'e}s, des for- mations ou encore des chercheurs(euses) comme des {\'e}l{\'e}ments interchangeables dont la valeur n'est pas intrins{\`e}que mais est d{\'e}termin{\'e}e par le rapport de l'offre et de la demande. En nous appuyant sur l'{\'e}tude de cas d'une universit{\'e} anglaise, nous voulons montrer ici comment marchandisation et quantification vont de pair. Nous montrerons comment les chiffres sont mobilis{\'e}s par l{\textquoteright}{\'E}tat et par la direction de l'universit{\'e} {\'e}tudi{\'e}e pour transformer l'enseignement et la recherche en {\'e}l{\'e}ments interchangeables, r{\'e}ductibles {\`a} un seul ou un tr{\`e}s petit nombre de crit{\`e}res pertinents; et pour donner corps {\`a} cette r{\'e}duction en orien- tant les pratiques des universitaires. Le caract{\`e}re performatif de la quantification est ainsi utilis{\'e} comme un outil de transformation de la r{\'e}alit{\'e} sociale, avec pour finalit{\'e} la mise en march{\'e} du secteur universitaire.},
	number = {4},
	urldate = {2020-06-28},
	journal = {Politiques et management public},
	author = {Boulet, Elsa and Harari-Kermadec, Hugo},
	month = dec,
	year = {2014},
	pages = {405--420},
	annote = {quantification, marchandisation, n{\'e}olib{\'e}ralisme, enseignement sup{\'e}rieur, classements, new public management.},
}

