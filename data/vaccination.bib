	journal = {Politiques et management public},
	author = {Cappelletti, Laurent and Khenniche, Samia},
	month = jun,
	year = {2017},
	pages = {19--41},
	annote = {Performance socialement responsable, analyse socio-{\'e}conomique, diagnostic, groupe de projet, comit{\'e} de pilotage, management public coproduit, co{\^u}ts performances cach{\'e}s.},
}

@article{marty_les_2016,
	title = {Les contrats {\`a} impact social : une nouvelle g{\'e}n{\'e}ration de {PPP} pour les politiques sociales},
	volume = {33},
	issn = {0758-1726},
	shorttitle = {Les contrats {\`a} impact social},
	url = {https://pmp.revuesonline.com/article.jsp?articleId=38149},
	doi = {10.3166/pmp.33.3-4.259-282},
	abstract = {Exp{\'e}riment{\'e}s depuis quelques ann{\'e}es au Royaume-Uni et aux {\'E}tats-Unis, les Social Impact Bonds, ou contrats {\`a} impact social (CIS), en France visent {\`a} faire financer par des investisseurs priv{\'e}s des programmes sociaux en conditionnant leur remboursement et leur niveau de r{\'e}mun{\'e}ration financi{\`e}re {\`a} la r{\'e}alisation d{\textquoteright}objectifs de performance. Les CIS, qui ont {\'e}t{\'e} par exemple utilis{\'e}s outre-Manche dans des programmes de pr{\'e}vention de la r{\'e}cidive pour de jeunes d{\'e}linquants, ont fait l{\textquoteright}objet d{\textquoteright}un appel {\`a} projets lanc{\'e} par le minist{\`e}re en charge de l{\textquoteright}{\'e}conomie sociale et solidaire en mars 2016. Les CIS, qui financi{\`e}rement correspondent plus souvent {\`a} des contrats {\`a} terme sur des r{\'e}sultats sociaux qu{\textquoteright}{\`a} des titres obligataires, visent {\`a} faire pr{\'e}financer des programmes sociaux par des investisseurs (banques, fondations...) et {\`a} b{\'e}n{\'e}ficier des capacit{\'e}s du secteur priv{\'e} en termes d{\textquoteright}innovation et d{\textquoteright}efficacit{\'e}. Le recours aux CIS pose n{\'e}anmoins un ensemble de questions tenant aux co{\^u}ts et aux risques li{\'e}s (notamment en regard de la d{\'e}finition des objectifs contractuels) mais aussi {\`a} la capacit{\'e} du secteur priv{\'e} {\`a} s{\textquoteright}investir dans des politiques sociales. Il s{\textquoteright}agit dans cette contribution de pr{\'e}senter, notamment au travers des exp{\'e}riences {\'e}trang{\`e}res, les modalit{\'e}s de fonctionnement et les premiers bilans des CIS, de discuter des risques qu{\textquoteright}ils seraient susceptibles d{\textquoteright}induire en termes de d{\'e}finition et de gestion des politiques publiques et enfin de montrer comment ces derniers s{\textquoteright}int{\`e}grent dans un mouvement de contractualisation voire de mise en march{\'e} de l{\textquoteright}action publique, avec notamment la mont{\'e}e en puissance dans les pays anglo-saxons de sch{\'e}mas de paiements en fonction des r{\'e}sultats.},
	language = {fr},
	number = {3-4},
	urldate = {2020-07-09},
	journal = {Politiques et management public},
	author = {Marty, Fr{\'e}d{\'e}ric},
	month = dec,
	year = {2016},
	pages = {259--282},
	annote = {contrats {\`a} impact social, privatisation, contractualisation de l{\textquoteright}action publique, paiement {\`a} la performance.},
}

@article{dubernet_en_2016,
	title = {En quoi la m{\'e}diation interne peut-elle renouveler la question de l{\textquoteright}accompagnement au changement ? {Le} retour d{\textquoteright}exp{\'e}rience d{\textquoteright}une universit{\'e}},
