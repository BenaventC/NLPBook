@article{bonnier_deploiement_2014,
	title = {Le d{\'e}ploiement d{\textquoteright}outils financiers dans le cadre de la r{\'e}forme du syst{\`e}me de soins : quand les acteurs les utilisent parce que c{\textquoteright}est ce qu{\textquoteright}il convient de faire},
