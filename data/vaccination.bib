@article{chelimsky_levaluation_1985,
	title = {L'{\'e}valuation de programmes aux {Etats}-{Unis}},
	volume = {3},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_2_1844},
