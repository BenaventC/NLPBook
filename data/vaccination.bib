	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_2_1833},
	doi = {10.3406/pomap.1985.1833},
	abstract = {La politique de concertation entre l'Administration et les syndicats a {\'e}t{\'e} bor{\'e}e par {\`a}-coups, {\`a} la suite de mouvements sociaux ou politiques (1946, 1968). En 1982, l'une de ses caract{\'e}ristiques est de s'adresser particuli{\`e}rement aux {\'e}chelons d{\'e}centralis{\'e}s des administrations et des organisations professionnelles. C'est donc au travers des pratiques locales qu'elle prend vie, forme et signification. La mani{\`e}re dont la concertation est per{\c c}ue localement, les enjeux qui en d{\'e}coulent pour les partenaires sociaux, sa confrontation avec l'{\'e}tat des relations au sein des services nous permettent d'observer, au-del{\`a} de la diversit{\'e} des pratiques, des types de strat{\'e}gies locales. L 'un d'eux conduit {\`a} la constitution d'un syst{\`e}me social syndicalo-administratif, local et coh{\'e}rent, portant sa finalit{\'e} relationnelle en lui-m{\^e}me. A quelles conditions de tels syst{\`e}mes locaux peuvent-ils exister et fonctionner ? Quels sont les facteurs qui interviennent dans la r{\'e}ussite d'une politique de concertation ? Peut-on pr{\'e}dire ses chances de succ{\`e}s ? Les observations r{\'e}alis{\'e}es permettent d'identifier des {\'e}l{\'e}ments constitutifs d'un syst{\`e}me relationnel {\'e}quilibr{\'e} et ouvert. Mais aucun d'entre eux ne peut avoir une influence isol{\'e}ment. C'est en relation avec les autres qu'il prend son sens au regard de la mise en oeuvre de la politique de concertation.},
	language = {fr},
	number = {2},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Chambron, Nicole},
	year = {1985},
	pages = {9--32},
}

@article{deleeck_distribution_1985,
	title = {La distribution in{\'e}gale des d{\'e}penses sociales en {Belgique}},
	volume = {3},
	issn = {0758-1726},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1985_num_3_3_1856},
	doi = {10.3406/pomap.1985.1856},
	abstract = {La pr{\'e}sente {\'e}tude se fonde principalement sur les r{\'e}sultats de deux enqu{\^e}tes repr{\'e}sentatives effectu{\'e}es en 1976 et 1982 par le Centre de Politique Sociale de l'Universit{\'e} d'Anvers. La perception des prestations de la s{\'e}curit{\'e} sociale diminue d'abord {\`a} mesure que s'{\'e}l{\`e}ve la cat{\'e}gorie des revenus mais elle se stabilise {\`a} partir du cinqui{\`e}me d{\'e}cile avec m{\^e}me une l{\'e}g{\`e}re augmentation au niveau du dixi{\`e}me. L'efficacit{\'e} du syst{\`e}me de s{\'e}curit{\'e} sociale a {\'e}t{\'e} appr{\'e}ci{\'e}e {\`a} l'aide d'une norme, le revenu minimum social, {\'e}tablie inductivement pour diff{\'e}rents types de m{\'e}nages (seuil de pauvret{\'e}). Pour 1976 on a trouv{\'e} que 24 \% des m{\'e}nages se trouvaient dans une situation d'ins{\'e}curit{\'e} d'existence (pour 1982 : 22 \%). En ce qui concerne les biens et les services sociaux, notre {\'e}tude d{\'e}montre que les cat{\'e}gories sociales sup{\'e}rieures en profitent plus par m{\'e}nage que les cat{\'e}gories inf{\'e}rieures. Cette distribution in{\'e}gale se manifeste par rapport {\`a} l'usage de l'enseignement (y compris les allocations familiales). Elle se manifeste aussi, mais d'une fa{\c c}on moins {\'e}vidente, dans les secteurs du logement social, des soins de sant{\'e} et dans les services culturels et sportifs. Cette distribution in{\'e}gale des services sociaux doit {\^e}tre comprise dans le cadre d'une th{\'e}orie g{\'e}n{\'e}rale de la distribution des revenus dans l'Etat Providence. La r{\'e}partition des biens sociaux est la cons{\'e}quence d'un processus de d{\'e}cision par lequel les groupes qui sont politiquement les mieux organis{\'e}s parviennent {\`a} r{\'e}aliser des services sociaux orient{\'e}s vers les int{\'e}r{\^e}ts de leurs membres. Elle est aussi la cons{\'e}quence des positions de d{\'e}part socio-politiques diff{\'e}rentes qui encouragent ou emp{\^e}chent l'acc{\`e}s {\`a} et l'emploi de services universels ou quasi- universels. Nous avons d{\'e}sign{\'e} ce ph{\'e}nom{\`e}ne de la distribution in{\'e}gale des d{\'e}penses sociales de l'Etat par l'appellation d'effet Mathieu.},
	language = {fr},
	number = {3},
	urldate = {2020-06-24},
	journal = {Politiques et management public},
	author = {Deleeck, Herman},
	year = {1985},
	pages = {97--130},
}

