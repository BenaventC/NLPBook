@book{bourbilleres_quand_nodate,
	title = {Quand la ville d{\'e}cide de soutenir les {\'e}v{\'e}nements sportifs commerciaux et comp{\'e}titifs : les d{\'e}terminants de l{\textquoteright}action concert{\'e}e {\`a} {Paris}},
	shorttitle = {({PDF}) {Quand} la ville d{\'e}cide de soutenir les {\'e}v{\'e}nements sportifs commerciaux et comp{\'e}titifs},
	url = {https://www.researchgate.net/publication/336835990_Quand_la_ville_decide_de_soutenir_les_evenements_sportifs_commerciaux_et_competitifs_les_determinants_de_l'action_concertee_a_Paris},
	abstract = {La position centrale de l{\textquoteright}acteur public dans l{\textquoteright}organisation du sport en France est juridique- ment constitu{\'e}e et historiquement d{\'e}montr{\'e}e. Dans le m{\^e}me temps, l{\textquoteright}implication crois- sante des acteurs priv{\'e}s introduit une logique commerciale qui fait {\'e}voluer les contours de l{\textquoteright}action publique. Consid{\'e}rant la pluralit{\'e} des interventions et des acteurs dans le champ sportif, nous avons cherch{\'e} {\`a} identifier et comprendre quels sont le (ou les) d{\'e}terminant(s) de l{\textquoteright}implication de la ville de Paris dans l{\textquoteright}{\'e}v{\'e}nementiel sportif comp{\'e}titif et commercial. Au- del{\`a} de configurations locales singuli{\`e}res, c{\textquoteright}est la perception par un {\'e}lu local d{\textquoteright}une finalit{\'e} sup{\'e}rieure dans l{\textquoteright}{\'e}v{\'e}nement qui l{\'e}gitime l{\textquoteright}intervention publique. Cette perception de mul- tiples fonctions sociales adoss{\'e}es aux {\'e}v{\'e}nements sportifs aboutit {\`a} une forme de porosit{\'e} dans la gouvernance qui correspond bien {\`a} une action concert{\'e}e (Chalas, 2000); c{\textquoteright}est-{\`a}- dire {\`a} une situation de porosit{\'e} o{\`u} les deux principaux acteurs (porteur de projet et acteur public) r{\'e}ajustent leurs positions respectives pour maintenir l{\textquoteright}int{\'e}r{\^e}t commun par un pro- tocole de bonne volont{\'e}. En analysant l{\textquoteright}implication de la ville de Paris dans la gouvernance d{\textquoteright}{\'e}v{\'e}nements sportifs r{\'e}currents, la contribution propose un mod{\`e}le visant {\`a} comprendre les raisons qui motivent le soutien des acteurs publics aux {\'e}v{\`e}nements sportifs comp{\'e}titifs et commerciaux.},
	language = {en},
	urldate = {2020-06-29},
	author = {Bourbill{\`e}res, Hugo and Evrard, Barbara and Charrier, Dominique},
	note = {Publication Title: ResearchGate},
	annote = {action publique, {\'e}v{\'e}nement sportif, action concert{\'e}e, fonction sociale, Paris.},
}

@article{maurel_mutualisation_2019,
	title = {De la mutualisation de services au lean government pour rechercher des {\'e}conomies financi{\`e}res dans les collectivit{\'e}s territoriales : le cas d{\textquoteright}une communaut{\'e} urbaine},
	volume = {36},
	issn = {0758-1726},
	shorttitle = {De la mutualisation de services au lean government pour rechercher des {\'e}conomies financi{\`e}res dans les collectivit{\'e}s territoriales},
	url = {https://pmp.revuesonline.com/article.jsp?articleId=40935},
	doi = {10.3166/pmp.36.2019.0016},
	abstract = {De nombreuses recherches indiquent les difficult{\'e}s que rencontrent les collectivit{\'e}s {\`a} ob- tenir des {\'e}conomies financi{\`e}res en p{\'e}riode d{\textquoteright}aust{\'e}rit{\'e} (Guenoun et al., 2015), malgr{\'e} les recommandations du l{\'e}gislateur {\`a} mutualiser les services (rapport Lambert et al., 2010). Des collectivit{\'e}s se tournent alors vers des approches de type lean government (Janssens et Estevez, 2013), recourant {\`a} la coproduction de services avec les usagers. {\`A} l{\textquoteright}aide d{\textquoteright}un cas de collectivit{\'e} ayant initi{\'e} une d{\'e}marche de mutualisation de services puis de lean government pour obtenir des {\'e}conomies, nous illustrons les cons{\'e}quences financi{\`e}res et organisationnelles de ces approches. Le cas confirme que les r{\'e}sultats financiers de la mutualisation sont limit{\'e}s, et que la coproduction de services n{\'e}cessite d{\textquoteright}investir dans l{\textquoteright}accompagnement de l{\textquoteright}usager. Nous discutons alors les faibles effets financiers de ces d{\'e}marches avec les investissements n{\'e}cessaires et le travail sur les dispositifs d{\textquoteright}accompa- gnement de l{\textquoteright}usager.},
	number = {3},
	urldate = {2020-06-29},
	journal = {Politiques et management public},
	author = {Maurel, Christophe and Pujol, Laurent},
	month = sep,
	year = {2019},
	pages = {321--339},
	annote = {Aust{\'e}rit{\'e}, collectivit{\'e}s, mutualisation de services, lean government, coproduction.},
}

@article{gallois_developpement_2019,
	title = {Le d{\'e}veloppement de la t{\'e}l{\'e}m{\'e}decine au prisme des r{\'e}f{\'e}rentiels de politiques publiques. {Une} cartographie de trois nations europ{\'e}ennes},
	volume = {36},
	doi = {10.3166/pmp.36.2019.0008},
	abstract = {La t{\'e}l{\'e}m{\'e}decine consiste en la pratique de la m{\'e}decine {\`a} distance via l'utilisation des tech- nologies de l{\textquoteright}information et de la communication. {\`A} l{\textquoteright}{\'e}chelle de l{\textquoteright}Union europ{\'e}enne, des recommandations communes, {\`a} travers des directives europ{\'e}ennes, sont donn{\'e}es {\`a} l{\textquoteright}en- semble des pays membres en vue d{\textquoteright}un d{\'e}veloppement harmonis{\'e} de la pratique. Dans les faits, malgr{\'e} ces recommandations uniformes, des stades de d{\'e}veloppement diff{\'e}rents et des mod{\`e}les organisationnels h{\'e}t{\'e}rog{\`e}nes sont recens{\'e}s. En mobilisant l{\textquoteright}approche par les r{\'e}f{\'e}rentiels politiques, cet article a pour objectif de caract{\'e}riser le lien entre le niveau de d{\'e}- veloppement de la t{\'e}l{\'e}m{\'e}decine et la forme de l{\textquoteright}action publique nationale et europ{\'e}enne. Trois nations poss{\'e}dant des niveaux de d{\'e}veloppement de la pratique contrast{\'e}s sont {\'e}tu- di{\'e}es: le Danemark, la France et l{\textquoteright}{\'E}cosse. Cette analyse permet de discuter de l{\textquoteright}adaptabi- lit{\'e} des directives europ{\'e}ennes {\`a} chaque pays en fonction de ses sp{\'e}cificit{\'e}s nationales et sectorielles dans le champ de la sant{\'e}. L{\textquoteright}ad{\'e}quation ou inad{\'e}quation global/sectoriel des r{\'e}f{\'e}rentiels est alors mise en {\'e}vidence.},
	journal = {Politiques et management public},
	author = {Gallois, Florence and Rauly, Amandine},
	month = sep,
	year = {2019},
	pages = {275--295},
	annote = {T{\'e}l{\'e}m{\'e}decine, r{\'e}f{\'e}rentiel global, r{\'e}f{\'e}rentiel sectoriel, performance.},
}

