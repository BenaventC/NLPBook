	volume = {29},
	issn = {0758-1726},
	shorttitle = {Professionnalisation et identit{\'e} des associations du secteur social},
	url = {http://pmp.revuesonline.com/article.jsp?articleId=18110},
	doi = {10.3166/pmp.29.671-693},
	abstract = {La demande des acteurs publics vis-{\`a}-vis des associations s{\textquoteright}est orient{\'e}e vers plus de transpa- rence avec pour corollaire le d{\'e}veloppement de dispositifs de gestion sp{\'e}cifiques suppos{\'e}s renforcer la capacit{\'e} d{\textquoteright}{\'e}valuation des bailleurs de fonds. Dans le m{\^e}me temps, la part des financements globaux de fonctionnement a fortement diminu{\'e} au profit de financements accord{\'e}s sur la base d{\textquoteright}activit{\'e}s sp{\'e}cifiques conduites par les associations. Il en d{\'e}coule l{\textquoteright}obligation pour les associations de d{\'e}velopper une expertise plus grande dans le mon- tage de dossiers de financement dont le nombre et la technicit{\'e} augmentent. Les r{\'e}sultats de cette recherche montrent la non-pertinence du seul mod{\`e}le d{\textquoteright}entreprise pour penser la professionnalisation d{\textquoteright}une association du secteur social. L{\textquoteright}observation d{\textquoteright}une situation de crise dans une association soumise {\`a} une pression financi{\`e}re met {\`a} jour une cr{\'e}ation de sens qui s{\textquoteright}articule autour de deux postures : le simple habillage de la relation aux finan- ceurs (en jouant {\'e}ventuellement sur les contradictions entre les discours nationaux et les pratiques locales des responsables de l{\textquoteright}instruction des dossiers), et la recr{\'e}ation de marges de man{\oe}uvre par l{\textquoteright}exploration de nouvelles pistes de financement. Ces deux postures ren- voient {\`a} des visions diff{\'e}rentes de l{\textquoteright}identit{\'e} des associations et supposent des fondements diff{\'e}rents de l{\textquoteright}action collective.},
	number = {4},
	urldate = {2020-06-27},
	journal = {Politiques et management public},
	author = {Eynaud, Philippe and Mouray, Damien},
	month = mar,
	year = {2012},
	pages = {671--693},
	annote = {Financement, projet, identit{\'e}, cr{\'e}ation de sens, instrumentation de gestion, {\'e}valuation, association.},
}

@article{ngaha_rse_2012,
