@article{hocreitere_audit_1993,
	title = {L'audit juridique : l'exemple du code de l'urbanisme},
	volume = {11},
	issn = {0758-1726},
	shorttitle = {L'audit juridique},
	url = {http://www.persee.fr/web/revues/home/prescript/article/pomap_0758-1726_1993_num_11_4_3137},
