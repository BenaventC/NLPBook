	year = {2018},
	pages = {279--290},
	annote = {Processus l{\'e}gislatif, {\'e}tudes d{\textquoteright}impact, qualit{\'e} du droit, consultation publique, analyse co{\^u}ts avantages, politique publique culturelle.},
